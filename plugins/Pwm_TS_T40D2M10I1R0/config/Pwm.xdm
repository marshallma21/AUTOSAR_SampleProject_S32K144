                    <a:da name="INVALID" type="Range">
                       <a:tst expr="&lt;=4294967295"/>
                        <a:tst expr="&gt;=0"/>
                    </a:da>               
                </v:var>

        
                <v:var name="PwmFaultSupport" type="BOOLEAN">
