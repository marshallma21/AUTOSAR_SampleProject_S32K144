  <d:ctr type="AUTOSAR" factory="autosar"
         xmlns:ad="http://www.tresos.de/_projects/DataModel2/08/admindata.xsd"
         xmlns:icc="http://www.tresos.de/_projects/DataModel2/08/implconfigclass.xsd"
         xmlns:mt="http://www.tresos.de/_projects/DataModel2/11/multitest.xsd" >
    <d:lst type="TOP-LEVEL-PACKAGES">
        <d:ctr name="TS_T40D2M10I1R0" type="AR-PACKAGE">
            <a:a name="UUID" value="ECUC:93e6601e-da37-4f47-8685-8d3f54ae5df6"/>
            <d:lst type="ELEMENTS">
                <!-- @implements LIN_Object -->
                <d:chc name="Lin" type="AR-ELEMENT" value="MODULE-DEF">
                    <v:ctr type="MODULE-DEF">
                        <a:a name="RELEASE" value="asc:4.2"/>
                        <a:a name="ADMIN-DATA" type="ADMIN-DATA">
                            <ad:ADMIN-DATA>
                                <ad:DOC-REVISIONS>
                                    <ad:DOC-REVISION>
                                        <ad:REVISION-LABEL>4.1.0</ad:REVISION-LABEL>
                                        <ad:ISSUED-BY>AUTOSAR</ad:ISSUED-BY>
                                        <ad:DATE>2010-12-03</ad:DATE>
                                    </ad:DOC-REVISION>
                                </ad:DOC-REVISIONS>
                            </ad:ADMIN-DATA>
                        </a:a>
                        <a:a name="DESC">
                            <a:v>
                              <![CDATA[EN:
                                  <html><p> Configuration of the Lin (Local Interconnect Network) module. </p></html>
                              ]]>
                            </a:v>
                        </a:a>
                        <a:a name="POSTBUILDVARIANTSUPPORT" value="true"/>
                        <a:a name="UUID" value="ECUC:754b773c-6809-46a7-be78-94f24abb9262"/>
                        <!-- @implements IMPLEMENTATION_CONFIG_VARIANT_Object -->
                        <v:var name="IMPLEMENTATION_CONFIG_VARIANT" type="ENUMERATION">
                            <a:a name="LABEL" value="Configuration Variant"/>
                            <a:a name="UUID" value="ECUC:c3f60b73-7066-4b07-b81e-b9c63fea2016"/>
                            <a:a name="DESC">
                                <a:v>
                                    <![CDATA[EN:
                                        <html><p>
                                            VariantPreCompile: Only precompile time configuration parameters.<br>
                                            VariantPostBuild: Mix of precompile and postbuild time configuration parameters.<br>
                                            If Config Variant = VariantPreCompile, the files  Lin_Cfg.h and Lin_Cfg.c shall be used.<br>
                                            If Config Variant = VariantPostBuild, the files Lin_Cfg.h and Lin_PBcfg.c shall be used.<br>
                                        </p></html>
                                    ]]>
                                </a:v>
                            </a:a>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                <icc:v class="PreCompile">VariantPostBuild</icc:v>
                                <icc:v class="PreCompile">VariantPreCompile</icc:v>
                            </a:a>
                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                            <a:da name="DEFAULT" value="VariantPostBuild"/>
                            <a:da name="RANGE">
                                <a:v>VariantPostBuild</a:v>
                                <a:v>VariantPreCompile</a:v>
                            </a:da>
                        </v:var>
                        <v:ctr name="NonAutosar" type="IDENTIFIABLE">
                            <a:a name="UUID" value="ECUC:f9793957-805b-400b-921f-acc6d3289e14"/>
                            <a:a name="DESC">
                                <a:v>
                                    <![CDATA[EN:
                                        <html>
                                            <h1>NonAutosar</h1>
                                            <p>Autosar Requirements: </p>
                                            <p>This container contains the global configuration parameters of the Non-Autosar Lin driver.
                                               This container is a MultipleConfigurationContainer, i.e. this container and
                                               its sub-containers exist once per configuration set.</p>
                                        </html>
                                    ]]>
                                </a:v>
                            </a:a>
                            <v:var name="LinDisableDemReportErrorStatus" type="BOOLEAN">
                                <a:a name="LABEL" value="Lin Disable Production Error Reporting"/>
                                <a:a name="DESC">
                                <a:v>
                                    <![CDATA[EN:
                                        <html>
                                            <h1>LinDisableDemReportErrorStatus</h1>
                                            <p>Switches the Diagnostic Error Reporting and Notification OFF.</p>
                                        </html>
                                    ]]>
                                </a:v>
                                </a:a>
                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                </a:a>
                                <a:a name="ORIGIN" value="NXP"/>
                                <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                                <a:a name="SCOPE" value="LOCAL"/>
                                <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                <a:a name="UUID" value="ECUC:72a8a638-4009-4639-124a-bb4daf863a7b"/>
                                <a:da name="DEFAULT" value="false"/>
                            </v:var>
                            <!-- @implements LinDisableFrameTimeout_Object -->
                            <v:var name="LinDisableFrameTimeout" type="BOOLEAN">
                                <a:a name="LABEL" value="Lin Disable Frame Timeout"/>
                                <a:a name="DESC">
                                    <a:v>
                                        <![CDATA[EN:
                                            <html>
                                                <h1>LinDisableFrameTimeout</h1>
                                                <p>The master will accept the frame that is longer than TFrame_Maximum.</p>
                                            </html>
                                        ]]>
                                    </a:v>
                                </a:a>
                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                </a:a>
                                <a:a name="ORIGIN" value="NXP"/>
                                <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                                <a:a name="SCOPE" value="LOCAL"/>
                                <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                <a:a name="UUID" value="ECUC:72a8a638-4009-4639-124a-bb4daf863a7c"/>
                                <a:da name="DEFAULT" value="false"/>
                            </v:var>
                            <!-- @implements LinEnableUserModeSupport_Object -->
                            <v:var name="LinEnableUserModeSupport" type="BOOLEAN">
                                <a:a name="LABEL" value="Lin Enable User Mode Support"/>
                                <a:a name="DESC">
                                    <a:v>
                                        <![CDATA[EN:
                                            <html>
                                                <h1>LinEnableUserModeSupport</h1>
                                                <p>When this parameter is enabled, the LIN module will adapt to run from User Mode</p>
                                                <h1>Note:</h1>
                                                <p>Lin module does not include registers protection. So, It is accessible to all registered in any public mode. Lin module is not affected by this field</p>
                                            </html>
                                        ]]>
                                    </a:v>
                                </a:a>
                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                </a:a>
                                <a:a name="ORIGIN" value="NXP"/>
                                <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                                <a:a name="SCOPE" value="LOCAL"/>
                                <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                <a:a name="UUID" value="ECUC:72a8a638-4009-4639-124a-bb4daf863a77"/>
                                <a:da name="DEFAULT" value="false"/>
                                <a:a name="READONLY" value="true"/>
                            </v:var>
                        </v:ctr>
                        <!-- @implements LinGeneral_Object -->
                        <v:ctr name="LinGeneral" type="IDENTIFIABLE">
                            <a:a name="DESC">
                                <a:v>
                                    <![CDATA[EN:
                                        <html>
                                            <h1>LinGeneral</h1>
                                            <p>Autosar Requirements: LIN183_Conf</p>
                                            <p>This container contains the parameters related to each LIN Driver Unit.
                                                This container is a MultipleConfigurationContainer, i.e. this container and
                                                its sub-containers exit once per configuration set.</p>
                                        </html>
                                    ]]>
                                </a:v>
                            </a:a>
                            <a:a name="UUID" value="ECUC:386c52b6-2650-435a-a896-b6f81bccf83a"/>
                            <!-- @implements LinDevErrorDetect_Object -->
                            <v:var name="LinDevErrorDetect" type="BOOLEAN">
                                <a:a name="LABEL" value="Lin Development Error Detection"/>
                                <a:a name="DESC">
                                    <a:v>
                                        <![CDATA[EN:
                                            <html>
                                                <h1>LinDevErrorDetect</h1>
                                                <p>Autosar Requirements: LIN066_Conf</p>
                                                <p>Switches the Default Error Detection and Notification ON or OFF.</p>
                                            </html>
                                        ]]>
                                    </a:v>
                                </a:a>
                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                </a:a>
                                <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                                <a:a name="SCOPE" value="LOCAL"/>
                                <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                <a:a name="UUID" value="ECUC:7228a638-4009-4639-8e1a-bb3d9f762a7b"/>
                                <a:da name="DEFAULT" value="true"/>
                            </v:var>
                            <!-- @implements LinIndex_Object -->
                            <v:var name="LinIndex" type="INTEGER">
                                <a:a name="LABEL" value="InstanceId"/>
                                <a:a name="DESC">
                                  <a:v>
                                      <![CDATA[EN:
                                          <html>
                                              <p>Specifies the InstanceId of this module instance. If only one instance is present it shall have the Id 0.</p>
                                              <p>Note, this parameter is not used in the current implementation.</p>
                                          </html>
                                      ]]>
                                  </a:v>
                                </a:a>
                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                </a:a>
                                <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                                <a:a name="SCOPE" value="LOCAL"/>
                                <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                <a:a name="UUID" value="ECUC:8a1935ff-76b3-4a7e-aa67-e516f2704cfa"/>
                                <a:da name="DEFAULT" value="0"/>
                                <a:a name="READONLY" value="true"/>
                                <a:da name="INVALID" type="Range">
                                    <a:tst expr="&lt;=255"/>
                                    <a:tst expr="&gt;=0"/>
                                </a:da>
                            </v:var>
                            <!-- @implements LinTimeoutDuration_Object -->
                            <v:var name="LinTimeoutDuration" type="INTEGER">
                                <a:a name="LABEL" value="Lin Timeout Duration"/>
                                <a:a name="DESC">
                                    <a:v>
                                        <![CDATA[EN:
                                            <html>
                                            Specifies the maximum number of loops for blocking function until a timeout is raised in short term wait loops
                                            </html>
                                        ]]>
                                    </a:v>
                                </a:a>
                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                </a:a>
                                <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                                <a:a name="SCOPE" value="LOCAL"/>
                                <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                <a:a name="UUID" value="ECUC:ea0e01de-6a8a-4565-840a-76cb28e5c197"/>
                                <a:da name="DEFAULT" value="1000"/>
                                <a:da name="INVALID" type="Range">
                                    <a:tst expr="&lt;=65535"/>
                                    <a:tst expr="&gt;=0"/>
                                </a:da>
                            </v:var>
                            <!-- @implements LinVersionInfoApi_Object -->
                            <v:var name="LinVersionInfoApi" type="BOOLEAN">
                                <a:a name="LABEL" value="Provide Lin VersionInfo Api"/>
                                <a:a name="DESC">
                                    <a:v>
                                        <![CDATA[EN:
                                            <html>
                                                <h1>LinVersionInfoApi</h1>
                                                <p>Autosar Requirements: LIN067_Conf</p>
                                                <p>Switches the Lin_GetVersionInfo function ON or OFF.</p>
                                            </html>
                                        ]]>
                                    </a:v>
                                </a:a>
                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                    <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                </a:a>
                                <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                                <a:a name="SCOPE" value="LOCAL"/>
                                <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                <a:a name="UUID" value="ECUC:be230747-6176-4c0b-90e2-290362e2d3ef"/>
                                <a:da name="DEFAULT" value="true"/>
                            </v:var>
                        </v:ctr>
                        <!-- @implements LinDemEventParameterRefs_Object -->
                        <v:ctr name="LinDemEventParameterRefs" type="IDENTIFIABLE">
                            <a:a name="LABEL" value="Lin Dem Event Parameter Refs" />
                            <a:a name="DESC" value="EN: Container for the references to DemEventParameter elements which shall be invoked using the API Dem_ReportErrorStatus API in case the corresponding error occurs. The EventId is taken from the referenced DemEventParameter&apos;s DemEventId value. The standardized errors are provided in the container and can be extended by vendor specific error references."/>
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                <icc:v mclass="PreCompile">VariantPostBuild</icc:v>
                                <icc:v mclass="PreCompile">VariantPreCompile</icc:v>
                            </a:a>
                            <a:a name="OPTIONAL" value="true"/>
                            <a:a name="UUID" value="ECUC:4c0d3fc1-6d2c-4614-8f58-fa7632263cbb"/>
                            <!-- @implements LIN_E_TIMEOUT_Object -->
                            <v:ref name="LIN_E_TIMEOUT" type="SYMBOLIC-NAME-REFERENCE">
                            <a:a name="LABEL" value="Lin Timeout Dem Error" />
                                <a:a name="DESC" value="EN: Reference to the DemEventParameter which shall be issued when the error &quot;Timeout caused by hardware error&quot; has occured."/>
                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                <icc:v class="PreCompile">VariantPostBuild</icc:v>
                                <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                </a:a>
                                <a:a name="OPTIONAL" value="true"/>
                                <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                <a:a name="POSTBUILDVARIANTMULTIPLICITY" value="false"/>
                                <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                                <a:a name="SCOPE" value="LOCAL"/>
                                <a:a name="UUID" value="ECUC:111e0a82-ab9f-4c95-879f-b686bbba6c65"/>
                                <a:da name="REF" value="ASPathDataOfSchema:/AUTOSAR/EcucDefs/Dem/DemConfigSet/DemEventParameter"/>
                                <a:da name="INVALID" type="XPath"> 
                                    <a:tst expr="node:refvalid(.)" false="Invalid or empty reference."/> 
                                </a:da>
                            </v:ref>
                        </v:ctr>
                        <!-- @implements LinGlobalConfig_Object -->
                        <v:ctr name="LinGlobalConfig" type="IDENTIFIABLE">
                            <a:a name="DESC">
                                <a:v>
                                    <![CDATA[EN:
                                        <html>
                                        This container contains the global configuration parameter of the Lin driver. This container is a MultipleConfigurationContainer, i.e. this container and its sub-containers exit once per configuration set.
                                        </html>
                                    ]]>
                                </a:v>
                            </a:a>
                            <a:a name="UUID" value="ECUC:ea2b8cf8-0ae8-4a24-bd07-1759472cf680"/>
                            <!-- @implements LinChannel_Object -->
                            <v:lst name="LinChannel" type="MAP">
                                <a:da name="MIN" value="1"/>
                                <a:da name="INVALID" type="XPath">
                                    <a:tst expr="num:i(count(node:current()/*)) &gt; ecu:get(&apos;Lin.LinGlobalConfig.LinChannel&apos;)"
                                           true="Maximum available LIN hardware channels was exceeded."/>
                                </a:da>
                                <v:ctr name="LinChannel" type="IDENTIFIABLE">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:
                                                <html>
                                                This container contains the configuration (parameters) of the LIN Controller(s).
                                                <h1>Note:</h1>"User should use unique names for naming the LIN channels across different LinGlobalConfig Sets."
                                                </html>
                                            ]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v mclass="PreCompile">VariantPostBuild</icc:v>
                                        <icc:v mclass="PreCompile">VariantPreCompile</icc:v>
                                    </a:a>
                                    <a:a name="POSTBUILDVARIANTMULTIPLICITY" value="false"/>
                                    <a:a name="UUID" value="ECUC:fdac1ad9-1217-44a8-bc3f-341635a20162"/>
                                    <!-- @implements LinChannelId_Object -->
                                    <v:var name="LinChannelId" type="INTEGER">
                                        <a:a name="DESC">
                                            <a:v>
                                                <![CDATA[EN:
                                                    <html>
                                                    Identifies the LIN channel. Replaces LIN_CHANNEL_INDEX_NAME from the LIN SWS.
                                                    </html>
                                                ]]>
                                            </a:v>
                                        </a:a>
                                        <a:a name="LABEL" value="Lin Channel ID" />
                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                        </a:a>
                                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                        <a:a name="POSTBUILDVARIANTVALUE" value="false"/>
                                        <a:a name="SCOPE" value="LOCAL"/>
                                        <a:a name="SYMBOLICNAMEVALUE" value="true"/>
                                        <a:a name="UUID" value="ECUC:aac952ef-7cb7-4b94-acc5-b6c4ca835e7a"/>
                                        <a:da name="DEFAULT" type="XPath" expr="node:fallback(node:current()/../@index, num:i(0))"/>
                                        <a:da name="INVALID" type="Range">
                                            <a:tst expr="&gt;=0"/>
                                            <a:tst expr="&lt;=2"/>
                                        </a:da>
                                        <a:da name="RANGE" type="XPath">
                                            <a:tst expr="(. &gt;= 0) and (.&lt; num:i(count(node:current()/../../*)))"
                                                    false="Value out of range: must be in range 0 to N-1 (N is number of configured channels). Use the Calc button to calculate correct default value"/>
                                            <a:tst expr="text:uniq(../../*/LinChannelId, .)"
                                                    false="Duplicated value, LinChannelId must be unique across all Lin channels. Use the Calc button to calculate correct default value."/>
                                            <a:tst expr="(node:fallback( ., 0 ) &gt;= ecu:get(&apos;Lin.LinGlobalConfig.LinChannel&apos;))"
                                                    true="Maximum Lin channels supported by this plugin is 3 only, refer reference mannual to see this limit."/>
                                        </a:da>
                                    </v:var>
                                    <!-- @implements LinChannelBaudRate_Object -->
                                    <v:var name="LinChannelBaudRate" type="INTEGER">
                                        <a:a name="DESC">
                                            <a:v>
                                                <![CDATA[EN:
                                                    <html>
                                                        <h1>LinChannelBaudRate</h1>
                                                        <p>Autosar Requirements: LIN180_Conf</p>
                                                        <p>Specifies the baud rate of the LIN channel in 'bps'. Valid range: 1000..20000.</p>
                                                    </html>
                                                ]]>
                                            </a:v>
                                        </a:a>
                                        <a:a name="LABEL" value="Lin Channel BaudRate (bps)" />
                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                        </a:a>
                                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                        <a:a name="SCOPE" value="LOCAL"/>
                                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                        <a:a name="UUID" value="ECUC:087f3523-f919-4b2d-841a-d18846908390"/>
                                        <a:da name="DEFAULT" value="9600"/>
                                        <a:da name="INVALID" type="Range">
                                            <a:tst expr="&lt;=20000"/>
                                            <a:tst expr="&gt;=1000"/>
                                        </a:da>
                                    </v:var>
                                    <v:var name="BreakLength" type="ENUMERATION">
                                        <a:a name="DESC">
                                            <a:v>
                                                <![CDATA[EN:
                                                    <html>
                                                    Defines the break length in bits.
                                                    <h1>Note:</h1>
                                                    This Parameter is an Implementation Specific Parameter.
                                                    </html>
                                                ]]>
                                            </a:v>
                                        </a:a>
                                        <a:a name="LABEL" value="Break Length (bits)" />
                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                        </a:a>
                                        <a:a name="ORIGIN" value="NXP"/>
                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                    <a:a name="SCOPE" value="LOCAL"/>
                                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                        <a:a name="UUID" value="ECUC:dcc46433-9de4-4883-9bd4-84274be6e67f"/>
                                        <a:da name="DEFAULT" value="BL_13"/>
                                        <a:da name="RANGE">
                                            <a:v>BL_10</a:v>
                                            <a:v>BL_13</a:v>
                                        </a:da>
                                    </v:var>
                                    <v:var name="LinHwChannel" type="ENUMERATION">
                                        <a:a name="DESC">
                                            <a:v>
                                                <![CDATA[EN:
                                                    <html>
                                                    Selects the Lin Hardware Channel.
                                                    <h1>Note:</h1>
                                                    This Parameter is an Implementation Specific Parameter.
                                                    </html>
                                                ]]>
                                            </a:v>
                                        </a:a>
                                        <a:a name="LABEL" value="Lin hardware channel" />
                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
