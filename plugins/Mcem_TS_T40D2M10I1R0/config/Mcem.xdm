        <d:lst type="TOP-LEVEL-PACKAGES">
            <d:ctr name="TS_T40D2M10I1R0" type="AR-PACKAGE">
                <a:a name="UUID" value="ECUC:4af873de-a8cf-41ad-a857-11cca7dcd97a"/>
                <d:lst type="ELEMENTS">
                    <d:chc name="Mcem" type="AR-ELEMENT" value="MODULE-DEF">
                        <v:ctr type="MODULE-DEF">
                            <a:a name="RELEASE" value="asc:4.2"/>
                            <a:a name="ADMIN-DATA" type="ADMIN-DATA">
                                <ad:ADMIN-DATA>
                                    <ad:DOC-REVISIONS>
                                        <ad:DOC-REVISION>
                                            <ad:REVISION-LABEL>4.6.0</ad:REVISION-LABEL>
                                            <ad:ISSUED-BY>AUTOSAR</ad:ISSUED-BY>
                                            <ad:DATE>2014-10-31</ad:DATE>
                                        </ad:DOC-REVISION>
                                    </ad:DOC-REVISIONS>
                                </ad:ADMIN-DATA>
                            </a:a>
                            <a:a name="POSTBUILDVARIANTSUPPORT" value="true"/>
                            <a:a name="DESC">
                                <a:v><![CDATA[EN: <html>
                                 <h1>Mcem</h1><p>
                                 Configuration of the Mcem (Microcontroller error manager) module
                                 </p></html>]]>
                                </a:v>
                            </a:a>    
                            <a:a name="LOWER-MULTIPLICITY" value="1"/>
                            <a:a name="UPPER-MULTIPLICITY" value="1"/>
                            <a:a name="UUID" value="ECUC:a8a495cc-11e9-459b-807f-0179efa09e77"/>
                            
                            <v:var name="IMPLEMENTATION_CONFIG_VARIANT" type="ENUMERATION">
                                <a:a name="LABEL" value="Config Variant"/>
                                <a:a name="UUID" value="ECUC:8816958c-2892-40bf-8678-a95ffacab8eb"/>
                                <a:da name="DEFAULT" value="VariantPreCompile"/>
                                <a:da name="EDITABLE" value="true"/>
                                <a:da name="RANGE">
                                 <a:v>VariantPreCompile</a:v>                                 
                                 <a:v>VariantPostBuild</a:v> 
                                </a:da>
                            </v:var>

                            <!--/** @implements  MCEMGeneralConfiguration_Object */ -->
                            <v:ctr name="McemGeneral" type="IDENTIFIABLE">
                                <a:a name="DESC">
                                    <a:v> <![CDATA[EN: <html>
                                    <h1>McemGeneral</h1><p>
                                    All general parameters of the MCEM driver are collected here.
                                    </p></html>]]>
                                    </a:v> 
                                </a:a>

                                <a:a name="UUID" value="ECUC:17517e42-6da8-437e-b057-35cc7f943c0e"/>
                                
                                <!-- /** @implements   McemDevErrorDetect_Object */ -->
                                <v:var name="McemDevErrorDetect" type="BOOLEAN">
                                    <a:a name="LABEL" value="Development Error Detection"/>
                                    <a:a name="DESC">
                                        <a:v><![CDATA[EN: <html>
                                            <h1>Mcem Development Error Detect</h1><p>
                                            Compile switch to enable / disable development error detection for this module.
                                            <ul>
                                            <li>Unchecked: Development error detection disabled</li>
                                            <li>Checked  : Development error detection enabled </li>
                                            </ul>
                                            </p></html>
                                            ]]> 
                                        </a:v> 
                                    </a:a> 
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                            type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                    <a:a name="SCOPE" value="LOCAL"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:9e744655-7df7-4a93-b826-65e7bf3760ad"/>
                                    <a:da name="DEFAULT" value="true"/> 
                                </v:var>

                                <!-- /** @implements   McemVersionInfoApi_Object */ -->
                                <v:var name="McemVersionInfoApi" type="BOOLEAN">
                                    <a:a name="LABEL" value="Provide Version Info API"/>                
                                    <a:a name="DESC">
                                        <a:v> <![CDATA[EN: <html>
                                            <h1>MCEM VersionInfo Api</h1><p>
                                            Compile switch to enable / disable the version information API.
                                            <ul>
                                            <li>Checked  : API enabled </li>
                                            <li>Unchecked:API disabled </li>
                                            </ul>
                                            </p></html>]]>
                                        </a:v>
                                    </a:a>                                                                                                                 
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                            type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                    <a:a name="SCOPE" value="LOCAL"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:22b5db0e-c172-4efb-a08b-8ffe218aa92a"/>
                                    <a:da name="DEFAULT" value="true"/>
                                </v:var>  


                                <!--/** @implements  McemAlarmNotificationApi_Object */ -->
                                <v:var name="McemAlarmNotificationApi" type="BOOLEAN">
                                    <a:a name="LABEL" value="Provide Alarm Notification"/>                
                                    <a:a name="DESC">
                                        <a:v> <![CDATA[EN: <html>
                                            <h1>MCEM Alarm Notification</h1><p>
                                            Compile switch to enable / disable the alarm notification .
                                            <ul>
                                            <li>Checked  : Notification  enabled </li>
                                            <li>Unchecked: Notification disabled </li>
                                            </ul>
                                            </p></html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" 
                                            type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                    <a:a name="SCOPE" value="LOCAL"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:0e089a82-6516-4e09-a6b5-859781abc8d8"/>
                                    <a:da name="DEFAULT" value="false"/>
                                    <a:da name="READONLY" value="true"/>
                                </v:var>

                                <!-- /** @implements McemEnableUserModeSupport_Object */ -->
                                <v:var name="McemEnableUserModeSupport" type="BOOLEAN">
                                    <a:a name="LABEL" value="Mcem Enable User Mode Support"/>
                                    <a:a name="DESC">
                                    <a:v>
                                    <![CDATA[EN:
                                        <html>
                                            <p>When this parameter is enabled, the MCEM module will adapt to run from User Mode, with the following measures:</p>
                                            <p>(if applicable) a) configuring REG_PROT for the MCEM registers so that the registers under protection can be accessed from user mode by setting UAA bit in REG_PROT_GCR to 1</p>
                                            <p>(if applicable) b) using 'call trusted function' stubs for all internal function calls that access registers requiring supervisor mode.</p>
                                            <p>(if applicable) c) other module specific measures for more information, please see chapter 5.7 User Mode Support in IM</p>
                                        </html>
                                    ]]>
                                    </a:v>
                                    </a:a>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                    </a:a>
                                    <a:a name="EDITABLE" type="XPath" expr="ecu:get('Mcem.McemEnableUserModeSupport')='true'"/>
                                    <a:a name="ORIGIN" value="NXP"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:faa4800a-3714-4ef4-9a8b-457befc658cd"/>
                                    <a:da name="DEFAULT" value="false"/>
                                </v:var>
                                <v:var name="McemEnableIsrSingleErrorBit" type="BOOLEAN">
                                    <a:a name="LABEL" value="Mcem Enable Interrupt for Signle bit ECC"/>
                                    <a:a name="DESC">
                                    <a:v>
                                    <![CDATA[EN:
                                        <html>
                                            <p>When this parameter is enabled, the MCEM module will provide ISR handler to process Interrupt signal when single bit ECC is detected.</p>
                                            <p>Note: The processing of handler depend on configure reaction of each fault (enable/disable ISR processing of this fault).</p>
                                        </html>
                                    ]]>
                                    </a:v>
                                    </a:a>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="NXP"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:faa4800a-3714-4fe4-9a8b-457befc658cd"/>
                                    <a:da name="DEFAULT" value="false"/>
                                </v:var>
                                <v:var name="McemEnableIsrDoubleErrorBit" type="BOOLEAN">
                                    <a:a name="LABEL" value="Mcem Enable Interrupt for Double bits ECC"/>
                                    <a:a name="DESC">
                                    <a:v>
                                    <![CDATA[EN:
                                        <html>
                                            <p>When this parameter is enabled, the MCEM module will provide ISR handler to process Interrupt signal when single bit ECC is detected.</p>
                                            <p>Note: The processing of handler depend on configure reaction of each fault (enable/disable ISR processing of this fault).</p>
                                        </html>
                                    ]]>
                                    </a:v>
                                    </a:a>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="NXP"/>
                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                    <a:a name="UUID" value="ECUC:faa4800a-3714-4ee4-9a8b-457befc658cd"/>
                                    <a:da name="DEFAULT" value="false"/>
                                </v:var>
                            </v:ctr> 
                            
                            <!--/** @implements  McemModuleConfiguration_Object */ -->
                            <v:ctr name="McemConfigSet" type="IDENTIFIABLE"> 
                                <a:a name="DESC">
                                    <a:v><![CDATA[EN: <html>
                                    <p></p>This container is the base for a multiple configuration set.
                                        </html>]]>
                                    </a:v>
                                </a:a>                                
                                <a:a name="UUID" value="ECUC:3fda88af-5143-4276-b8b8-a58cf5b7d256"/>    
                                <!--/** @implements FaultTimeout_Object */ -->  
                                <v:var name="FaultTimeout" type="INTEGER">
                                    <a:a name="LABEL" value="Fault Recovery Timeout"/>
                                    <a:a name="UUID" value="ECUC:1f40c244-6e74-494e-877e-9a7288eaecdc"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:da name="DEFAULT" value="65535"/>
                                    <a:a name="DESC">
                                        <a:v><![CDATA[EN: <html>
                                          <p>Non-critical fault timeout. If the fault with enabled recovery timeout is not recovered 
                                          within the timeout period the FCCU moves from the ALARM state to the FAULT state.</p>
                                          <p>NCF timeout can be computed using the following equations:</p>
                                          <p style="margin-left: 5em;">Timeout&nbsp;=&nbsp;T<sub>RC16MHz</sub>&nbsp;&times;&nbsp;FaultTimeout</p>
                                          <p style="margin-left: 5em;">Timeout&nbsp;=&nbsp;0.0625&nbsp;&times;&nbsp;FaultTimeout&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;[&mu;s]</p>
                                          </html>]]> 
                                        </a:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="NXP"/>
                                    <a:a name="SCOPE" value="LOCAL"/>
                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                    <a:da name="INVALID" type="Range">
                                        <a:tst expr="&gt;=0"/>
                                        <a:tst expr="&lt;4294967296"/>
                                    </a:da>
                                    <a:da name="READONLY" value="true"/>
                                 </v:var> 
                                <!--/** @implements EOUT_FaultOutputMode_Object */ -->
                                <v:var name="EOUT_FaultOutputMode" type="ENUMERATION">
                                    <a:a name="LABEL" value="Fault Output Mode Selection"/>
                                    <a:a name="UUID" value="ECUC:9779877e-7535-411c-875d-f66f6df20e29"/>
                                    <a:a name="ORIGIN" value="NXP"/>
                                    <a:a name="SCOPE" value="LOCAL"/>
                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                    </a:a>
                                        <a:da name="DEFAULT" value="EOUT_DualRail"/>
                                    <a:a name="DESC">
                                        <a:v><![CDATA[EN: <html>
                                        Fault Output Mode Selection
                                        <ul>
                                        <li> Dual-Rail (default state) [fccu_eout[1:0]= outputs] </li>
                                        <li> Time Switching [fccu_eout[1:0]= output to be used] </li>
                                        <li> Bi-Stable </li>
                                        </ul>
                                        <p></p> Note: In the Test mode, a simple double-stage resynchronization stage is used to resynchronize the EOUT input/outputs on the system/safe clock. 
                                        </html>]]> 
                                        </a:v>
                                    </a:a>
                                    <a:da name="RANGE">
                                       <a:v>EOUT_DualRail</a:v>                                 
                                       <a:v>EOUT_TimeSwitching</a:v> 
                                       <a:v>EOUT_BiStable</a:v>                                 
                                    </a:da>
                                    <a:da name="READONLY" value="true"/>
                                </v:var>
                                <!--/** @implements  McemHardlockedConfiguration_Object */ -->
                                <v:var name="McemHardlockedConfiguration" type="BOOLEAN">
                                    <a:a name="LABEL" value="Hard-Locked Configuration Set"/>
                                    <a:a name="UUID" value="ECUC:bbe1e60f-0e7c-402d-8068-e89799970f43"/>
                                    <a:a name="ORIGIN" value="NXP"/>
                                    <a:a name="SCOPE" value="LOCAL"/>
                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:da name="DEFAULT" value="false"/>
                                    <a:a name="DESC">    
                                        <a:v><![CDATA[EN: <html>
                                        Hard-lock this configuration set: 
                                        <ul>
                                            <li>Unchecked: After Mcem_Init, the configuration is not locked. </li>
                                            <li>Checked: After Mcem_Init, the configuration is hardlocked. </li>
                                        </ul>
                                        </html>]]> 
                                        </a:v>
                                    </a:a>
                                    <a:da name="READONLY" value="true"/>
                                </v:var>
                                <!--/** @implements  McemSoftLockedConfiguration_Object */ -->
                                <v:var name="McemSoftLockedConfiguration" type="BOOLEAN">
                                    <a:a name="LABEL" value="Soft-Locked Configuration Set"/>
                                    <a:a name="UUID" value="ECUC:bbe1e60f-0e7c-402d-8086-e89799970f43"/>
                                    <a:a name="ORIGIN" value="NXP"/>
                                    <a:a name="SCOPE" value="LOCAL"/>
                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:da name="DEFAULT" value="false"/>
                                    <a:a name="DESC">    
                                        <a:v><![CDATA[EN: <html>
                                        Soft-lock this configuration set: 
                                        <ul>
                                            <li>Unchecked: After Mcem_Init, the configuration is not locked. </li>
                                            <li>Checked: After Mcem_Init, the configuration is soft-locked. </li>
                                        </ul>
                                        </html>]]> 
                                        </a:v>
                                    </a:a>
                                </v:var>
                                <!--/** @implements EOUT_SwitchingMode_Object */ -->                                 
                                <v:var name="EOUT_SwitchingMode" type="BOOLEAN">
                                    <a:a name="LABEL" value="EOUT Switching Mode"/>
                                    <a:a name="UUID" value="ECUC:f8c348ee-b9ff-41c6-87ee-c182e9b87e25"/>
                                    <a:a name="ORIGIN" value="NXP"/>
                                    <a:a name="SCOPE" value="LOCAL"/>
                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:da name="DEFAULT" value="false"/>
                                    <a:da name="EDITABLE" type="XPath">
                                        <a:tst expr="not(contains(node:value(../EOUT_FaultOutputMode),'BiStable'))"/>
                                    </a:da>
                                    <a:a name="DESC">    
                                        <a:v><![CDATA[EN: <html>
                                        EOUT Switching mode
                                        <ul>
                                        <li>Unchecked: EOUT protocol (dual-rail, time-switching) slow switching mode. </li>
                                        <li>Checked  : EOUT protocol (dual-rail, time-switching) fast switching mode. </li>
                                        </ul>
                                        Switching Mode has no effect on the bi-stable protocol.
                                        </html>]]> 
                                        </a:v>
                                    </a:a>
                                    <a:da name="READONLY" value="true"/>
                                </v:var>
                                <!--/** @implements EOUT_PolaritySelection_Object */ -->
                                <v:var name="EOUT_PolaritySelection" type="BOOLEAN">
                                    <a:a name="LABEL" value="EOUT Polarity Selection"/>
                                    <a:a name="UUID" value="ECUC:50970b86-6dfc-44d2-a0d8-3b533828790c"/>
                                    <a:a name="ORIGIN" value="NXP"/>
                                    <a:a name="SCOPE" value="LOCAL"/>
                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:da name="DEFAULT" value="false"/>
                                    <a:da name="EDITABLE" type="XPath">
                                        <a:tst expr="(contains(node:value(../EOUT_FaultOutputMode),'BiStable'))"/>
                                    </a:da>
                                    <a:a name="DESC">    
                                        <a:v><![CDATA[EN: <html>
                                        EOUT Polarity Selection
                                        <ul>
                                        <li>Unchecked: fcc_eout[1] active high, fcc_eout[0] active low. </li>
                                        <li>Checked: fcc_eout[1] active low, fcc_eout[0] active high. </li>
                                        </ul>
                                        This bit can be read and written by the software. Active state means a state indicating FAULT and 
                                        is applicable for non-toggling protocols only.
                                        </html>]]> 
                                        </a:v>
                                    </a:a>
                                    <a:da name="READONLY" value="true"/>
                                </v:var>

                                <v:var name="McemErrorNotificationWithAddressAPI" type="FUNCTION-NAME">
                                    <a:a name="LABEL" value="Mcem Error Notification with Error Address API"/>
                                    <a:a name="UUID" value="ECUC:58b5e6c6-4207-499b-9b26-f473d37fdff6"/>
                                    <a:a name="ORIGIN" value="NXP"/>
                                    <a:a name="SCOPE" value="LOCAL"/>
                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:da name="DEFAULT" value="NULL_PTR"/>
                                    <a:a name="DESC">
                                        <a:v><![CDATA[EN: <html>
                                          <p>In case of a failure event, if interrupt is enabled, the fault shall notify error address to 
                                            application using this callback. The prototype of the notification function is</p>
                                          <p style="margin-left: 5em;">void Mcem_NotificationCallbackFunction(Mcem_FaultType FaultID, uint32 u32ErrorAddr );</p>
                                        </html>]]> 
                                        </a:v>
                                    </a:a>
                                    <a:da name="INVALID" type="XPath">
                                        <a:tst expr="(text:toupper(.)='NULL') or (text:toupper(.)='FALSE') or ((text:toupper(.)='NULL_PTR') and (. != 'NULL_PTR'))" true="Invalid name of the McemAlarmNotification. Must be valid C function name or NULL_PTR."/>
                                        <a:tst expr="normalize-space(.) = 'NULL_PTR' or text:match(normalize-space(.),'^[_a-zA-Z]+[_0-9a-zA-Z]*$')" false="Invalid name of the McemAlarmNotification. Must be valid C function name or NULL_PTR."/>
                                    </a:da>
                                </v:var>
                                <v:var name="McemTcmLowerAddr" type="INTEGER">
                                    <a:a name="DESC">
                                        <a:v>
                                            <![CDATA[EN:<html>
                                                The address use to inject error in lower TCM space
                                            </html>]]>
                                        </a:v>
                                    </a:a>
                                    <a:a name="LABEL" value="Mcem Lower TCM address (for Injected)"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                        <icc:v class="PreCompile">VariantPostBuild</icc:v>
                                    </a:a>
                                    <a:a name="ORIGIN" value="NXP"/>
                                    <a:a name="SCOPE" value="LOCAL"/>
