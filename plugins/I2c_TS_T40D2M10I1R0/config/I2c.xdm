                                      <a:da name="DEFAULT" value="1"/>
                                      <a:a name="EDITABLE" type="XPath">
                                        <a:tst expr="(
                                                        (node:fallback(../../I2cMasterSlaveConfiguration, 'MASTER_MODE') = 'MASTER_MODE') and
