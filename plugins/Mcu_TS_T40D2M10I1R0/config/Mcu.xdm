                                                      <![CDATA[EN:
                                                        <html>
                                                          <p> Configures the SCG_HCCR[DIVBUS] bitfield
                                                            This parameter represents the bus clock divider.
                                                            Note: implementation specific parameter.
                                                          </p>
                                                        </html>
                                                      ]]>
                                                    </a:v>
                                                  </a:a>
                                                  <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                    <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                  </a:a>
                                                  <a:a name="ORIGIN" value="NXP"/>
                                                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                  <a:a name="SCOPE" value="LOCAL"/>
                                                  <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                  <a:a name="UUID" value="ECUC:32681e6f-3916-4d25-819f-89ed2f727f7c"/>
                                                  <a:a name="LABEL" value="HSRUN Bus Clock Divider"/>
                                                    <a:a name="EDITABLE" type="XPath">
                                                        <a:tst expr="ecu:get('MCU.HsrunSystemClkSource.Exist') = 'true'"/>
                                                    </a:a>
                                                  <a:da name="DEFAULT" value="1"/>
                                                  <a:da name="INVALID" type="Range">
                                                    <a:tst expr="&lt;=16"/>
                                                    <a:tst expr="&gt;=1"/>
                                                  </a:da>
                                                </v:var>

                                                <v:var name="McuSlowClockDivider" type="INTEGER">
                                                  <a:a name="DESC">
                                                    <a:v>
                                                      <![CDATA[EN:
                                                        <html>
                                                          <p> Configures the SCG_HCCR[DIVSLOW] bitfield
                                                            This parameter represents the flash clock divider.
                                                            Note: implementation specific parameter.
                                                          </p>
                                                        </html>
                                                      ]]>
                                                    </a:v>
                                                  </a:a>
                                                  <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                    <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                  </a:a>
                                                  <a:a name="EDITABLE" type="XPath">
                                                      <a:tst expr="ecu:get('MCU.HsrunSystemClkSource.Exist') = 'true'"/>
                                                  </a:a>
                                                  <a:a name="ORIGIN" value="NXP"/>
                                                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                  <a:a name="SCOPE" value="LOCAL"/>
                                                  <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                  <a:a name="UUID" value="ECUC:ade8fe56-0a8b-4ca9-9dc7-e31c3b369057"/>
                                                  <a:a name="LABEL" value="HSRUN Slow Clock Divider"/>
                                                  <a:da name="DEFAULT" value="2"/>
                                                  <a:da name="INVALID" type="Range">
                                                    <a:tst expr="&lt;=8"/>
                                                    <a:tst expr="&gt;=1"/>
                                                  </a:da>
                                                </v:var>
                                                
                                                <v:var name="McuScgClkOutFrequency" type="FLOAT">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                This is frequency of SCG clockout. It is given in Hz. <p/>
                                                                Note: Implementation Specific Parameter.
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="EDITABLE" type="XPath">
                                                        <a:tst expr="ecu:get('MCU.HsrunSystemClkSource.Exist') = 'true'"/>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="LABEL" value="HSRUN SCG ClockOut Frequency (Hz)"/>
                                                    <a:a name="UUID" value="ECUC:3ce46395-a36f-43ba-bd7b-075104b7180c"/>
                                                    <a:da name="DEFAULT" type="XPath">
                                                        <a:tst expr="num:i( num:i( number( node:fallback(../../McuScgClkOutSelect,'SCG_SLOW_CLK')='SCG_SLOW_CLK')) *
                                                                                    num:i( node:fallback(../McuFlashClockFrequency, num:i(1000000))) +
                                                                                    num:i( number( node:fallback(../../McuScgClkOutSelect,'SCG_SLOW_CLK')='SIRC')) *
                                                                                    num:i( node:fallback(../../McuSIRCClockConfig/McuSIRCFrequency, num:i(8000000))) +
                                                                                    num:i( number( node:fallback(../../McuScgClkOutSelect,'SCG_SLOW_CLK')='FIRC')) *
                                                                                    num:i( node:fallback(../../McuFIRCClockConfig/McuFIRCFrequency, num:i(48000000))) +
                                                                                    num:i( number( node:fallback(../../McuScgClkOutSelect,'SCG_SLOW_CLK')='SOSC')) *
                                                                                    num:i( node:fallback(../../McuSystemOSCClockConfig/McuSOSCFrequency, num:i(40000000))) +
                                                                                    num:i( number( node:fallback(../../McuScgClkOutSelect,'SCG_SLOW_CLK')='SPLL')) *
                                                                                    num:i( node:fallback(../../McuSystemPll/McuSPLLFrequency, num:i(40000000)))
                                                                                  )"/>
                                                    </a:da>
                                                    <a:a name="RANGE" type="XPath">
                                                        <a:tst expr="( ( not( node:empty( node:fallback(., 100000000) ) )
                                                                         and
                                                                         ( node:fallback(., 100000000) !=
                                                                           ( num:i( num:i( number( node:fallback(../../McuScgClkOutSelect,'SCG_SLOW_CLK')='SCG_SLOW_CLK')) *
                                                                                    num:i( node:fallback(../McuFlashClockFrequency, num:i(1000000))) +
                                                                                    num:i( number( node:fallback(../../McuScgClkOutSelect,'SCG_SLOW_CLK')='SIRC')) *
                                                                                    num:i( node:fallback(../../McuSIRCClockConfig/McuSIRCFrequency, num:i(8000000))) +
                                                                                    num:i( number( node:fallback(../../McuScgClkOutSelect,'SCG_SLOW_CLK')='FIRC')) *
                                                                                    num:i( node:fallback(../../McuFIRCClockConfig/McuFIRCFrequency, num:i(48000000))) +
                                                                                    num:i( number( node:fallback(../../McuScgClkOutSelect,'SCG_SLOW_CLK')='SOSC')) *
                                                                                    num:i( node:fallback(../../McuSystemOSCClockConfig/McuSOSCFrequency, num:i(40000000))) +
                                                                                    num:i( number( node:fallback(../../McuScgClkOutSelect,'SCG_SLOW_CLK')='SPLL')) *
                                                                                    num:i( node:fallback(../../McuSystemPll/McuSPLLFrequency, num:i(40000000)))
                                                                                  )
                                                                           )
                                                                         )
                                                                       )
                                                                     )
                                                                     or
                                                                     ( node:fallback(., num:i(100000000)) &gt; num:i(180000000) )
                                                                     or
                                                                     ( node:fallback(., num:i(100000000)) &lt; num:i(1000) ) "
                                                        true="Please press the button to calculate the value automatically."/>
                                                    </a:a>
                                                    <a:a name="INVALID" type="Range">
                                                        <a:tst expr="&lt;=180000000"/>
                                                        <a:tst expr="&gt;=1000"/>
                                                    </a:a>
                                                </v:var>

                                            </v:ctr>

                                            <v:ctr name="McuSystemOSCClockConfig" type="IDENTIFIABLE">
                                                <a:a name="DESC">
                                                  <a:v>
                                                    <![CDATA[EN:
                                                      <html>
                                                          Configures System OSC registers.<p/>
                                                          Note: Implementation specific Container.
                                                      </html>
                                                    ]]>
                                                  </a:v>
                                                </a:a>
                                                <a:a name="TAB" value="McuSOSCClockConfig"/>
                                                <a:a name="UUID" value="ECUC:e0b759b3-4c7d-4320-b7ea-977fa7f120ea"/>
                                                <v:var name="McuSOSCUnderMcuControl" type="BOOLEAN">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                0 - System OSC is NOT under mcu control.<p/>
                                                                1 - System OSC is under mcu control.<p/>
                                                                If this is set to false, the MCU code will not configure the SOSC registers when Mcu_InitClock is called<p/>
                                                                Note: Implementation Specific Parameter.<p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="SOSC under MCU control"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="UUID" value="ECUC:b0a57506-2fb4-4973-bd2a-e9d45d9b376b"/>
                                                    <a:da name="DEFAULT" value="true"/>
                                                </v:var>

                                                <v:var name="McuSOSCFrequency" type="FLOAT">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:
                                                                <html>
                                                                    This is the SOSC frequency for the specific instance of the
                                                                    McuClockReferencePoint container. It is expressed in Hz. <p/>
                                                                    If PLL is used, then oscillator needs to be in high range only, SCG_SOSCCFG[RANGE] on 11 as used in reference clock. <p/>
                                                                    Note: Implementation Specific Parameter.
                                                                </html>
                                                          ]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="SOSC Frequency"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="UUID" value="ECUC:85886c75-ae23-49a8-87ca-b76b3110bad2"/>
                                                    <a:da name="DEFAULT" value="8000000"/>
                                                    
                                                    <a:a name="INVALID" type="Range">
                                                        <a:tst expr="&lt;=40000000"/>
                                                        <a:tst expr="&gt;=4000000"/>
                                                    </a:a>
                                                </v:var>

                                                <v:var name="McuSOSCDiv2Frequency" type="FLOAT">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:
                                                                <html>
                                                                    This is the SOSC Divider 2 frequency for the specific instance of the
                                                                    McuClockReferencePoint container. It is expressed in Hz.<p/>
                                                                    This should be configured to 40 MHz or less in RUN/HSRUN mode. <p/>
                                                                    Note: Implementation Specific Parameter.
                                                                </html>
                                                          ]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="SOSC Div2 Frequency"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="UUID" value="ECUC:af6121d0-e74f-4427-9b8e-9603f120da75"/>
                                                    <a:da name="DEFAULT" type="XPath">
                                                        <a:tst expr="(num:i(
                                                                            (node:fallback(../McuSOSCFrequency, 8000000)) 
                                                                             div num:i(node:fallback(../McuSOSCDiv2, num:i(1)))
                                                                          )*num:i(number(node:fallback(../McuSOSCDiv2, num:i(1)) != num:i(0)))
                                                                      +
                                                                      num:i(4000000)*num:i(number(node:fallback(../McuSOSCDiv2, num:i(1)) = num:i(0))))"/>
                                                    </a:da>
                                                    <a:da name="EDITABLE" type="XPath">
                                                        <a:tst expr="(node:fallback(../McuSOSCDiv2, num:i(1)) != num:i(0))"/>
                                                    </a:da>
                                                    <a:a name="RANGE" type="XPath">
                                                        <a:tst expr="( ( not( node:empty( node:fallback(., 8000000) ) )
                                                                         and
                                                                         ( node:fallback(., 8000000) != 
                                                                           num:i
                                                                           (
                                                                             (node:fallback(../McuSOSCFrequency, 8000000)) 
                                                                              div num:i(node:fallback(../McuSOSCDiv2, num:i(1)))
                                                                           )
                                                                         )
                                                                         and
                                                                         ( node:fallback(../McuSOSCDiv2, num:i(1)) != num:i(0))
                                                                       )
                                                                     )
                                                                     or
                                                                     ( node:fallback(., num:i(8000000)) &gt; num:i(40000000) )
                                                                     or
                                                                     ( node:fallback(., num:i(8000000)) &lt; num:i(4000000) ) "
                                                        true="Please press the button to calculate the value automatically."/>
                                                    </a:a>
                                                    <a:a name="INVALID" type="Range">
                                                        <a:tst expr="&lt;=40000000"/>
                                                        <a:tst expr="&gt;=4000000"/>
                                                    </a:a>
                                                </v:var>

                                                <v:var name="McuSOSCDiv1Frequency" type="FLOAT">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:
                                                                <html>
                                                                    This is the SOSC Divider 1 frequency for the specific instance of the
                                                                    McuClockReferencePoint container. It is expressed in Hz.<p/>
                                                                    This should be configured to 40 MHz or less in RUN/HSRUN mode.<p/>
                                                                    Note: Implementation Specific Parameter.
                                                                </html>
                                                          ]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="SOSC Div1 Frequency"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="UUID" value="ECUC:7b3ca528-ac23-4117-a9c8-19bcad269dbb"/>
                                                    <a:da name="DEFAULT" type="XPath">
                                                        <a:tst expr="(num:i(
                                                                            (node:fallback(../McuSOSCFrequency, 8000000)) 
                                                                             div num:i(node:fallback(../McuSOSCDiv1, num:i(1)))
                                                                          )*num:i(number(node:fallback(../McuSOSCDiv1, num:i(1)) != num:i(0)))
                                                                      +
                                                                      num:i(4000000)*num:i(number(node:fallback(../McuSOSCDiv1, num:i(1)) = num:i(0))))"/>
                                                    </a:da>
                                                    <a:da name="EDITABLE" type="XPath">
                                                        <a:tst expr="(node:fallback(../McuSOSCDiv1, num:i(1)) != num:i(0))"/>
                                                    </a:da>
                                                    <a:a name="RANGE" type="XPath">
                                                        <a:tst expr="( ( not( node:empty( node:fallback(., 8000000) ) )
                                                                         and
                                                                         ( node:fallback(., 8000000) != 
                                                                           num:i
                                                                           (
                                                                             (node:fallback(../McuSOSCFrequency, 8000000)) 
                                                                              div num:i(node:fallback(../McuSOSCDiv1, num:i(1)))
                                                                           )
                                                                         )
                                                                         and
                                                                         ( node:fallback(../McuSOSCDiv1, num:i(1)) != num:i(0))
                                                                       )
                                                                     )
                                                                     or
                                                                     ( node:fallback(., num:i(8000000)) &gt; num:i(40000000) )
                                                                     or
                                                                     ( node:fallback(., num:i(8000000)) &lt; num:i(4000000) ) "
                                                        true="Please press the button to calculate the value automatically."/>
                                                    </a:a>
                                                    <a:a name="INVALID" type="Range">
                                                        <a:tst expr="&lt;=40000000"/>
                                                        <a:tst expr="&gt;=4000000"/>
                                                    </a:a>
                                                </v:var>

                                                <v:var name="McuSOSCEnable" type="BOOLEAN">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                SCG_SOSCCSR[SOSCEN] - System OSC Enable <p/>
                                                                0 - System OSC is disabled. <p/>
                                                                1 - System OSC is enabled.<p/>
                                                                Note: Implementation Specific Parameter.<p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="SOSC Enable"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="UUID" value="ECUC:b2780807-a4fc-42ba-a9c1-2646b8f6d4d5"/>
                                                    <a:da name="DEFAULT" value="true"/>
                                                </v:var>

                                                <v:var name="McuSOSCClockMonitorResetEnable" type="BOOLEAN">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                SCG_SOSCCSR[SOSCCMRE] - System OSC Clock Monitor Reset Enable <p/>
                                                                0 - Clock Monitor generates interrupt when error detected.<p/>
                                                                1 - Clock Monitor generates reset when error detected.<p/>
                                                                Note: Implementation Specific Parameter.<p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="SOSC clock monitor reset enable"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="UUID" value="ECUC:63daa71d-58e5-45ea-8551-43a918c2c766"/>
                                                    <a:da name="DEFAULT" value="false"/>
                                                </v:var>

                                                <v:var name="McuSOSCClockMonitorEnable" type="BOOLEAN">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                SCG_SOSCCSR[SOSCCM] - System OSC Clock Monitor <p/>
                                                                Enables the clock monitor, if the clock source is disabled in a low power mode then the clock monitor is
                                                                also disabled in the low power mode. When the clock monitor is disabled in a low power mode, it remains
                                                                disabled until the clock valid flag is set following exit from the low power mode. <p/>
                                                                0 - System OSC Clock Monitor is disabled.<p/>
                                                                1 - System OSC Clock Monitor is enabled.<p/>
                                                                Note: Implementation Specific Parameter.<p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="SOSC clock monitor enable"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="UUID" value="ECUC:e55a04ab-792f-48af-93cd-e419f0132f78"/>
                                                    <a:da name="DEFAULT" value="false"/>
                                                </v:var>

                                                <v:var name="McuSOSCDiv2" type="INTEGER">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                    Configures SCG_SOSCDIV[SOSCDIV2].<p/>
                                                                    System OSC Clock Divide 2.<p/>
                                                                    Clock divider 2 for System OSC. Used by bus clock modules that need an asynchronous clock source. <p/>
                                                                    0 - Output disabled. <p/>
                                                                    Note: Implementation specific Parameter.
                                                            </html>
                                                            ]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="SOSC Divider 2"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="UUID" value="ECUC:903610a0-7a3a-40d3-8db9-bc06ef4fade7"/>
                                                    <a:da name="DEFAULT" value="1"/>
                                                    <a:da name="INVALID" type="Range">
                                                        <a:tst expr="&lt;=64"/>
                                                        <a:tst expr="&gt;=0"/>
                                                    </a:da>
                                                    <a:da name="INVALID" type="XPath">
                                                        <a:tst expr="((. != 0) and (. != 1) and (. != 2) and (. != 4) and (. != 8) and (. != 16) and (. != 32) and (. != 64))" true="Only the values 0, 1, 2, 4, 8, 16, 32 and 64 are allowed" />
                                                    </a:da>
                                                </v:var>
                                                
                                                <v:var name="McuSOSCDiv1" type="INTEGER">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                    Configures SCG_SOSCDIV[SOSCDIV1]<p/>
                                                                    System OSC Clock Divide 1.<p/>
                                                                    Clock divider 1 for System OSC. Used to generate the system clock source and by platform clock modules that need an asynchronous clock source. <p/>
                                                                    0 - Output disabled. <p/>
                                                                    Note: Implementation specific Parameter.
                                                            </html>
                                                            ]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="SOSC Divider 1"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="UUID" value="ECUC:f6d587b6-8a6e-4af0-b708-8c04ceb70061"/>
                                                    <a:da name="DEFAULT" value="1"/>
                                                    <a:da name="INVALID" type="Range">
                                                        <a:tst expr="&lt;=64"/>
                                                        <a:tst expr="&gt;=0"/>
                                                    </a:da>
                                                    <a:da name="INVALID" type="XPath">
                                                        <a:tst expr="((. != 0) and (. != 1) and (. != 2) and (. != 4) and (. != 8) and (. != 16) and (. != 32) and (. != 64))" true="Only the values 0, 1, 2, 4, 8, 16, 32 and 64 are allowed" />
                                                    </a:da>
                                                </v:var>
                                                
                                                <v:var name="McuSOSCRangeSelect" type="ENUMERATION">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                SCG_SOSCCFG[RANGE] - System OSC Range Select <p/>
                                                                Selects the frequency range for the system crystal oscillator (OSC) <p/>
                                                                <b>NOTE</b> The following constraints are not checked by the xdm schema: <p/>
                                                                If PLL is used, then oscillator needs to be in high range only, SCG_SOSCCFG[RANGE] on 11 as used in reference clock. <p/>
                                                                    - Medium frequency range selected for the crytstal oscillator of 4 MHz to 8 MHz.<p/>
                                                                    - High frequency range selected for the crystal oscillator of 8 MHz to 32 MHz.<p/>
                                                                
                                                                Note: Implementation Specific Parameter.
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="LABEL" value="SOSC Range Select"/>
                                                    <a:a name="UUID" value="ECUC:829e0049-1dd8-4488-a891-455fdf7d729b"/>
                                                    <a:da name="DEFAULT" value="HIGH_FREQ_RANGE"/>
                                                    <a:da name="RANGE">
                                                        <a:v>MEDIUM_FREQ_RANGE</a:v>
                                                        <a:v>HIGH_FREQ_RANGE</a:v>
                                                    </a:da>
                                                </v:var>
                                                
                                                <v:var name="McuSOSCHighGainOscillatorSelect" type="BOOLEAN">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                SCG_SOSCCFG[HGO] - High Gain Oscillator Select <p/>
                                                                Controls the crystal oscillator power mode of operations. <p/>
                                                                    unchecked - Configure crystal oscillator for low-power operation <p/>
                                                                    checked - Configure crystal oscillator for high-gain operation <p/>
                                                                Note: Implementation Specific Parameter.<p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="High Gain Oscillator Select"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="UUID" value="ECUC:7f4d664f-3ce5-4e0e-8c43-e8eb4ed0e5b8"/>
                                                    <a:da name="DEFAULT" value="false"/>
                                                </v:var>
                                                
                                                <v:var name="McuSOSCExternalReferenceSelect" type="BOOLEAN">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                SCG_SOSCCFG[EREFS] - External Reference Select <p/>
                                                                Selects the source for the external reference clock. <p/>
                                                                    unchecked - Internal oscillator of OSC requested. <p/>
                                                                    checked - External reference clock from PAD pin selected <p/>
                                                                Note: Implementation Specific Parameter.<p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="Select external reference clock"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="UUID" value="ECUC:d1847dfc-3867-4977-9e13-a0ba58cf600d"/>
                                                    <a:da name="DEFAULT" value="false"/>
                                                </v:var>
                                                                      
                                            </v:ctr>                                            
                                                                                                
                                            <v:ctr name="McuSIRCClockConfig" type="IDENTIFIABLE">
                                                <a:a name="DESC">
                                                  <a:v>
                                                    <![CDATA[EN:
                                                      <html>
                                                          Configures Slow IRC (SIRC) registers.<p/>
                                                          Note: Implementation specific Container.
                                                      </html>
                                                    ]]>
                                                  </a:v>
                                                </a:a>
                                                <a:a name="TAB" value="McuSIRCClockConfig"/>
                                                <a:a name="UUID" value="ECUC:24b8eaea-53c1-45b7-a10d-6326ef098e92"/>
                                                <v:var name="McuSIRCUnderMcuControl" type="BOOLEAN">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                0 - Slow IRC is NOT under mcu control.<p/>
                                                                1 - Slow IRC is under mcu control.<p/>
                                                                If this is set to false, the MCU code will not configure the SIRC registers when Mcu_InitClock is called<p/>
                                                                Note: Implementation Specific Parameter.<p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="SIRC under MCU control"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="UUID" value="ECUC:b464923e-8c5f-4f06-ab18-0c1b848e5a04"/>
                                                    <a:da name="DEFAULT" value="true"/>
                                                </v:var>

                                                <v:var name="McuSIRCFrequency" type="FLOAT">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:
                                                                <html>
                                                                    This is the SIRC frequency for the specific instance of the
                                                                    McuClockReferencePoint container. It is expressed in Hz.<p/>
                                                                    Note: Implementation Specific Parameter.<p/>
                                                                </html>
                                                          ]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="SIRC Frequency"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="UUID" value="ECUC:16d78457-9e58-40ea-8207-b0e936640855"/>
                                                    <a:da name="DEFAULT" type="XPath">
                                                    <a:tst expr="num:i( num:i( number( node:fallback(../McuSIRCRangeSelect,'HIGH_RANGE_CLOCK')='LOW_RANGE_CLOCK')) *
                                                                        num:i(2000000) +
                                                                        num:i( number( node:fallback(../McuSIRCRangeSelect,'HIGH_RANGE_CLOCK')='HIGH_RANGE_CLOCK')) *
                                                                        num:i(8000000)
                                                                      )"/>
                                                    </a:da>
                                                    <a:a name="RANGE" type="XPath">
                                                        <a:tst expr="( not( node:empty( node:fallback(., 8000000) ) )
                                                                         and
                                                                         ( node:fallback(., 8000000) !=
                                                                           ( 
                                                                            num:i(number(node:fallback(../McuSIRCRangeSelect,'HIGH_RANGE_CLOCK')='LOW_RANGE_CLOCK')) *
                                                                            num:i(2000000) +
                                                                            num:i( number(
                                                                            node:fallback(../McuSIRCRangeSelect,'HIGH_RANGE_CLOCK')='HIGH_RANGE_CLOCK')) *
                                                                            num:i(8000000)
                                                                           )
                                                                         )
                                                                     )
                                                                     or
                                                                     ( node:fallback(., num:i(8000000)) &gt; num:i(8000000) )
                                                                     or
                                                                     ( node:fallback(., num:i(8000000)) &lt; num:i(2000000) ) "
                                                        true="Please press the button to calculate the value automatically."/>
                                                        <a:tst expr="((. != 2000000) and (. != 8000000))" true="Only the values 2000000 and 8000000 are allowed" />
                                                    </a:a>
                                                    <a:da name="INVALID" type="Range">
                                                        <a:tst expr="&lt;=8000000"/>
                                                        <a:tst expr="&gt;=2000000"/>
                                                    </a:da>
                                                </v:var>

                                                <v:var name="McuSIRCDiv2Frequency" type="FLOAT">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:
                                                                <html>
                                                                    This is the SIRC Divider 2 frequency for the specific instance of the
                                                                    McuClockReferencePoint container. It is expressed in Hz.<p/>
                                                                    This should be configured to 8 MHz or less. <p/>
                                                                    Note: Implementation Specific Parameter.<p/>
                                                                </html>
                                                          ]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="SIRC Div2 Frequency"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="UUID" value="ECUC:f6fb287d-7f25-48f9-9a15-4c7283f1f121"/>
                                                    <a:da name="DEFAULT" type="XPath">
                                                        <a:tst expr="(num:i(
                                                                            (node:fallback(../McuSIRCFrequency, 8000000)) 
                                                                             div num:i(node:fallback(../McuSIRCDiv2, num:i(2)))
                                                                          )*num:i(number(node:fallback(../McuSIRCDiv2, num:i(1)) != num:i(0)))
                                                                      +
                                                                      num:i(1)*num:i(number(node:fallback(../McuSIRCDiv2, num:i(1)) = num:i(0))))"/>
                                                    </a:da>
                                                    <a:da name="EDITABLE" type="XPath">
                                                        <a:tst expr="(node:fallback(../McuSIRCDiv2, num:i(1)) != num:i(0))"/>
                                                    </a:da>
                                                    <a:a name="RANGE" type="XPath">
                                                        <a:tst expr="( ( not( node:empty( node:fallback(., 8000000) ) )
                                                                         and
                                                                         ( node:fallback(., 8000000) != 
                                                                           num:i
                                                                           (
                                                                             (node:fallback(../McuSIRCFrequency, 8000000)) 
                                                                              div num:i(node:fallback(../McuSIRCDiv2, num:i(2)))
                                                                           )
                                                                         )
                                                                         and
                                                                         ( node:fallback(../McuSIRCDiv2, num:i(1)) != num:i(0))
                                                                       )
                                                                     )
                                                                     or
                                                                     ( node:fallback(., num:i(8000000)) &gt; num:i(8000000) )
                                                                     or
                                                                     ( node:fallback(., num:i(8000000)) &lt; num:i(1) ) "
                                                        true="Please press the button to calculate the value automatically."/>
                                                    </a:a>
                                                    <a:a name="INVALID" type="Range">
                                                        <a:tst expr="&lt;=8000000"/>
                                                        <a:tst expr="&gt;=1"/>
                                                    </a:a>
                                                </v:var>

                                                <v:var name="McuSIRCDiv1Frequency" type="FLOAT">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:
                                                                <html>
                                                                    This is the SIRC Divider 1 frequency for the specific instance of the
                                                                    McuClockReferencePoint container. It is expressed in Hz. <p/>
                                                                    This should be configured to 8 MHz or less in RUN/HSRUN mode and to 4 MHz or less in VLPR mode.
                                                                </html>
                                                          ]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="SIRC Div1 Frequency"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="UUID" value="ECUC:794aabbd-566f-44e3-98c9-9fda68dbccd4"/>
                                                    <a:da name="DEFAULT" type="XPath">
                                                        <a:tst expr="(num:i(
                                                                            (node:fallback(../McuSIRCFrequency, 8000000)) 
                                                                             div num:i(node:fallback(../McuSIRCDiv1, num:i(1)))
                                                                          )*num:i(number(node:fallback(../McuSIRCDiv1, num:i(1)) != num:i(0)))
                                                                      +
                                                                      num:i(1)*num:i(number(node:fallback(../McuSIRCDiv1, num:i(1)) = num:i(0))))"/>
                                                    </a:da>
                                                    <a:da name="EDITABLE" type="XPath">
                                                        <a:tst expr="(node:fallback(../McuSIRCDiv1, num:i(1)) != num:i(0))"/>
                                                    </a:da>
                                                    <a:a name="RANGE" type="XPath">
                                                        <a:tst expr="( ( not( node:empty( node:fallback(., 8000000) ) )
                                                                         and
                                                                         ( node:fallback(., 8000000) != 
                                                                           num:i
                                                                           (
                                                                             (node:fallback(../McuSIRCFrequency, 8000000)) 
                                                                              div num:i(node:fallback(../McuSIRCDiv1, num:i(1)))
                                                                           )
                                                                         )
                                                                         and
                                                                         ( node:fallback(../McuSIRCDiv1, num:i(1)) != num:i(0))
                                                                       )
                                                                     )
                                                                     or
                                                                     ( node:fallback(., num:i(8000000)) &gt; num:i(8000000) )
                                                                     or
                                                                     ( node:fallback(., num:i(8000000)) &lt; num:i(1) ) "
                                                        true="Please press the button to calculate the value automatically."/>
                                                    </a:a>
                                                    <a:da name="INVALID" type="Range">
                                                        <a:tst expr="&lt;=8000000"/>
                                                        <a:tst expr="&gt;=1"/>
                                                    </a:da>
                                                </v:var>

                                                <v:var name="McuSIRCEnable" type="BOOLEAN">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                SCG_SIRCCSR[SIRCEN] - Slow IRC Enable <p/>
                                                                0 - Slow IRC is disabled. <p/>
                                                                1 - Slow IRC is enabled.<p/>
                                                                Note: Implementation Specific Parameter.<p/>
                                                                The Node is not editable. This is needed for switching system clock.<p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="SIRC Enable"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="UUID" value="ECUC:e4198cd1-3c6b-44d8-86d4-65149081b533"/>
                                                    <a:da name="DEFAULT" value="true"/>
                                                    <a:a name="READONLY" value="true"/>
                                                </v:var>

                                                <v:var name="McuSIRCLowPowerEnable" type="BOOLEAN">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                SCG_SIRCCSR[SIRCLPEN] - Slow IRC Low Power Enable <p/>
                                                                0 - Slow IRC is disabled in VLP modes.<p/>
                                                                1 - Slow IRC is enabled in VLP modes.<p/>
                                                                Note: Implementation Specific Parameter.<p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="SIRC Low Power Enable"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="UUID" value="ECUC:e6a3d431-8997-4d0e-a38b-5d65dac3967f"/>
                                                    <a:da name="DEFAULT" value="false"/>
                                                </v:var>

                                                <v:var name="McuSIRCStopEnable" type="BOOLEAN">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                SCG_SIRCCSR[SIRCSTEN] - Slow IRC Enable <p/>
                                                                0 - Slow IRC is disabled.<p/>
                                                                1 - Slow IRC is enabled.<p/>
                                                                Note: Implementation Specific Parameter.<p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="SIRC Stop Enable"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="UUID" value="ECUC:06933c63-b2c6-490c-a546-7bb1f9dacd7f"/>
                                                    <a:da name="DEFAULT" value="false"/>
                                                </v:var>
                                                
                                                <v:var name="McuSIRCDiv2" type="INTEGER">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                    Configures SCG_SIRCDIV[SIRCDIV2].<p/>
                                                                    Slow IRC Clock Divider 2.<p/>
                                                                    Clock divider 2 for Slow IRC. Used by bus clock modules that need an asynchronous clock source. <p/>
                                                                    0 - Output disabled. <p/>
                                                                    Note: Implementation specific Parameter.
                                                            </html>
                                                            ]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="SIRC Divider 2"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="UUID" value="ECUC:9d336637-ce2a-4553-b6d7-5a8c23e0fd1c"/>
                                                    <a:da name="DEFAULT" value="2"/>
                                                    <a:da name="INVALID" type="Range">
                                                        <a:tst expr="&lt;=64"/>
                                                        <a:tst expr="&gt;=0"/>
                                                    </a:da>
                                                    <a:da name="INVALID" type="XPath">
                                                        <a:tst expr="((. != 0) and (. != 1) and (. != 2) and (. != 4) and (. != 8) and (. != 16) and (. != 32) and (. != 64))" true="Only the values 0, 1, 2, 4, 8, 16, 32 and 64 are allowed" />
                                                    </a:da>
                                                </v:var>
                                                
                                                <v:var name="McuSIRCDiv1" type="INTEGER">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                    Configures SCG_SIRCDIV[SIRCDIV1].<p/>
                                                                    Slow IRC Clock Divider 1.<p/>
                                                                    Clock divider 1 for Slow IRC. Used to generate the system clock source and by platform clock modules
                                                                    that need an asynchronous clock source. <p/>
                                                                    0 - Output disabled. <p/>
                                                                    Note: Implementation specific Parameter.
                                                            </html>
                                                            ]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="SIRC Divider 1"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="UUID" value="ECUC:e8791f92-81ae-4ac5-bdf6-41f3a1c26f3e"/>
                                                    <a:da name="DEFAULT" value="1"/>
                                                    <a:da name="INVALID" type="Range">
                                                        <a:tst expr="&lt;=64"/>
                                                        <a:tst expr="&gt;=0"/>
                                                    </a:da>
                                                    <a:da name="INVALID" type="XPath">
                                                        <a:tst expr="((. != 0) and (. != 1) and (. != 2) and (. != 4) and (. != 8) and (. != 16) and (. != 32) and (. != 64))" true="Only the values 0, 1, 2, 4, 8, 16, 32 and 64 are allowed" />
                                                    </a:da>
                                                </v:var>
                                                
                                                <v:var name="McuSIRCRangeSelect" type="ENUMERATION">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                SCG_SIRCCFG[RANGE] - Selects the Frequency Range <p/>
                                                                Slow IRC low range clock (2 MHz) <p/>
                                                                Slow IRC high range clock (8 MHz) <p/>
                                                                Note: The SIRC clock is chosen as source clock that must be sacrificed to be ON at all times. <p/>
                                                                Add addition, Software should not configure the SCG_SIRCCFG[RANGE] to any value other than HIGH_RANGE_CLOCK. <p/>
                                                                Note: Implementation Specific Parameter.
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="LABEL" value="SIRC Frequency Range"/>
                                                    <a:a name="UUID" value="ECUC:f796f5ab-7438-4571-b6b5-bebedc1fa5a9"/>
                                                    <a:da name="DEFAULT" value="HIGH_RANGE_CLOCK"/>
                                                    <a:da name="RANGE">
                                                        <a:v>LOW_RANGE_CLOCK</a:v>
                                                        <a:v>HIGH_RANGE_CLOCK</a:v>
                                                    </a:da>
                                                    <a:da name="INVALID" type="XPath">
                                                        <a:tst expr="(node:fallback(., 'HIGH_RANGE_CLOCK') != 'HIGH_RANGE_CLOCK')  "
                                                            true="HW only support to configure SCG_SIRCCFG[RANGE] with HIGH_RANGE_CLOCK"/>
                                                    </a:da>
                                                </v:var>

                                            </v:ctr>                                            

                                            <v:ctr name="McuFIRCClockConfig" type="IDENTIFIABLE">
                                                <a:a name="DESC">
                                                  <a:v>
                                                    <![CDATA[EN:
                                                      <html>
                                                          Configures Fast IRC (FIRC) registers.<p/>
                                                          Note: Implementation specific Container.
                                                      </html>
                                                    ]]>
                                                  </a:v>
                                                </a:a>
                                                <a:a name="TAB" value="McuFIRCClockConfig"/>
                                                <a:a name="UUID" value="ECUC:4e36c644-e344-461a-945f-a77f66842132"/>                                                                            
                                                <v:var name="McuFIRCUnderMcuControl" type="BOOLEAN">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                0 - Fast IRC is NOT under mcu control.<p/>
                                                                1 - Fast IRC is under mcu control.<p/>
                                                                If this is set to false, the MCU code will not configure the FIRC registers when Mcu_InitClock is called<p/>
                                                                Note: Implementation Specific Parameter.<p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="FIRC under MCU control"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="UUID" value="ECUC:21cde528-7d84-434e-aeb2-aac8c694005b"/>
                                                    <a:da name="DEFAULT" value="true"/>
                                                </v:var>

                                                <v:var name="McuFIRCFrequency" type="FLOAT">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:
                                                                <html>
                                                                    This is the FIRC frequency for the specific instance of the
                                                                    McuClockReferencePoint container. It is expressed in Hz.<p/>
                                                                    Note: Implementation specific Container.
                                                                </html>
                                                          ]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="FIRC Frequency"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="UUID" value="ECUC:c3026653-5011-4925-81ad-aa9313b48838"/>
                                                    <a:da name="DEFAULT" type="XPath">
                                                    <a:tst expr="num:i( num:i( number( node:fallback(../McuFIRCRangeSelect,'TRIMMED_TO_48MHZ')='TRIMMED_TO_48MHZ')) *
                                                                        num:i(48000000) +
                                                                        num:i( number( node:fallback(../McuFIRCRangeSelect,'TRIMMED_TO_48MHZ')='TRIMMED_TO_52MHZ')) *
                                                                        num:i(52000000) +
                                                                        num:i( number( node:fallback(../McuFIRCRangeSelect,'TRIMMED_TO_48MHZ')='TRIMMED_TO_56MHZ')) *
                                                                        num:i(56000000) +
                                                                        num:i( number( node:fallback(../McuFIRCRangeSelect,'TRIMMED_TO_48MHZ')='TRIMMED_TO_60MHZ')) *
                                                                        num:i(60000000)
                                                                      )"/>
                                                    </a:da>
                                                    <a:a name="RANGE" type="XPath">
                                                        <a:tst expr="( not( node:empty( node:fallback(., 48000000) ) )
                                                                         and
                                                                         ( node:fallback(., 48000000) !=
                                                                           ( 
                                                                            num:i( number( node:fallback(../McuFIRCRangeSelect,'TRIMMED_TO_48MHZ')='TRIMMED_TO_48MHZ')) *
                                                                            num:i(48000000) +
                                                                            num:i( number( node:fallback(../McuFIRCRangeSelect,'TRIMMED_TO_48MHZ')='TRIMMED_TO_52MHZ')) *
                                                                            num:i(52000000) +
                                                                            num:i( number( node:fallback(../McuFIRCRangeSelect,'TRIMMED_TO_48MHZ')='TRIMMED_TO_56MHZ')) *
                                                                            num:i(56000000) +
                                                                            num:i( number( node:fallback(../McuFIRCRangeSelect,'TRIMMED_TO_48MHZ')='TRIMMED_TO_60MHZ')) *
                                                                            num:i(60000000)
                                                                           )
                                                                         )
                                                                     )
                                                                     or
                                                                     ( node:fallback(., num:i(48000000)) &gt; num:i(60000000) )
                                                                     or
                                                                     ( node:fallback(., num:i(48000000)) &lt; num:i(48000000) ) "
                                                        true="Please press the button to calculate the value automatically."/>
                                                        <a:tst expr="((. != 48000000) and (. != 52000000) and (. != 56000000) and (. != 60000000))" true="Only the values 48000000, 52000000, 56000000 and 60000000 are allowed" />
                                                    </a:a>
                                                    <a:da name="INVALID" type="Range">
                                                        <a:tst expr="&lt;=60000000"/>
                                                        <a:tst expr="&gt;=48000000"/>
                                                    </a:da>
                                                </v:var>
                                                
                                                <v:var name="McuFIRCDiv2Frequency" type="FLOAT">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:
                                                                <html>
                                                                    This is the FIRC Divider 2 frequency for the specific instance of the
                                                                    McuClockReferencePoint container. It is expressed in Hz.<p/>
                                                                    This should be configured to 48 MHz or less in RUN/HSRUN mode.<p/>
                                                                    Note: Implementation specific Container.
                                                                </html>
                                                          ]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="FIRC Div2 Frequency"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="UUID" value="ECUC:d1672829-e341-4113-a346-27d82f9b4c9b"/>
                                                    <a:da name="DEFAULT" type="XPath">
                                                        <a:tst expr="(num:i(
                                                                            (node:fallback(../McuFIRCFrequency, 48000000)) 
                                                                             div num:i(node:fallback(../McuFIRCDiv2, num:i(2)))
                                                                          )*num:i(number(node:fallback(../McuFIRCDiv2, num:i(1)) != num:i(0)))
                                                                      +
                                                                      num:i(1)*num:i(number(node:fallback(../McuFIRCDiv2, num:i(1)) = num:i(0))))"/>
                                                    </a:da>
                                                    <a:da name="EDITABLE" type="XPath">
                                                        <a:tst expr="(node:fallback(../McuFIRCDiv2, num:i(1)) != num:i(0))"/>
                                                    </a:da>
                                                    <a:a name="RANGE" type="XPath">
                                                        <a:tst expr="( ( not( node:empty( node:fallback(., 48000000) ) )
                                                                         and
                                                                         ( node:fallback(., 48000000) != 
                                                                           num:i
                                                                           (
                                                                             (node:fallback(../McuFIRCFrequency, 48000000)) 
                                                                              div num:i(node:fallback(../McuFIRCDiv2, num:i(2)))
                                                                           )
                                                                         )
                                                                         and
                                                                         ( node:fallback(../McuFIRCDiv2, num:i(1)) != num:i(0))
                                                                       )
                                                                     )
                                                                     or
                                                                     ( node:fallback(., num:i(48000000)) &gt; num:i(48000000) )
                                                                     or
                                                                     ( node:fallback(., num:i(48000000)) &lt; num:i(1) ) "
                                                        true="Please press the button to calculate the value automatically."/>
                                                    </a:a>
                                                    <a:a name="INVALID" type="Range">
                                                        <a:tst expr="&lt;=48000000"/>
                                                        <a:tst expr="&gt;=1"/>
                                                    </a:a>
                                                </v:var>

                                                <v:var name="McuFIRCDiv1Frequency" type="FLOAT">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:
                                                                <html>
                                                                    This is the FIRC Divider 1 frequency for the specific instance of the
                                                                    McuClockReferencePoint container. It is expressed in Hz.<p/>
                                                                    This should be configured to 48 MHz or less in RUN/HSRUN mode. <p/>
                                                                    Note: Implementation specific Container.
                                                                </html>
                                                          ]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="FIRC Div1 Frequency"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="UUID" value="ECUC:158903ce-50de-4eb4-881e-509541ba3db7"/>
                                                    <a:da name="DEFAULT" type="XPath">
                                                        <a:tst expr="(num:i(
                                                                            (node:fallback(../McuFIRCFrequency, 48000000)) 
                                                                             div num:i(node:fallback(../McuFIRCDiv1, num:i(1)))
                                                                          )*num:i(number(node:fallback(../McuFIRCDiv1, num:i(1)) != num:i(0)))
                                                                      +
                                                                      num:i(1)*num:i(number(node:fallback(../McuFIRCDiv1, num:i(1)) = num:i(0))))"/>
                                                    </a:da>
                                                    <a:da name="EDITABLE" type="XPath">
                                                        <a:tst expr="(node:fallback(../McuFIRCDiv1, num:i(1)) != num:i(0))"/>
                                                    </a:da>
                                                    <a:a name="RANGE" type="XPath">
                                                        <a:tst expr="( ( not( node:empty( node:fallback(., 48000000) ) )
                                                                         and
                                                                         ( node:fallback(., 48000000) != 
                                                                           num:i
                                                                           (
                                                                             (node:fallback(../McuFIRCFrequency, 48000000)) 
                                                                              div num:i(node:fallback(../McuFIRCDiv1, num:i(1)))
                                                                           )
                                                                         )
                                                                         and
                                                                         ( node:fallback(../McuFIRCDiv1, num:i(1)) != num:i(0))
                                                                       )
                                                                     )
                                                                     or
                                                                     ( node:fallback(., num:i(48000000)) &gt; num:i(48000000) )
                                                                     or
                                                                     ( node:fallback(., num:i(48000000)) &lt; num:i(1) ) "
                                                        true="Please press the button to calculate the value automatically."/>
                                                    </a:a>
                                                    <a:a name="INVALID" type="Range">
                                                        <a:tst expr="&lt;=48000000"/>
                                                        <a:tst expr="&gt;=1"/>
                                                    </a:a>
                                                </v:var>

                                                <v:var name="McuFIRCEnable" type="BOOLEAN">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                SCG_FIRCCSR[FIRCEN] - Fast IRC Enable <p/>
                                                                0 - Fast IRC is disabled. <p/>
                                                                1 - Fast IRC is enabled.<p/>
                                                                Note: Implementation specific Container.                                                               
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="FIRC Enable"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="UUID" value="ECUC:be1ee9a7-128c-4b32-9b99-a029d31c51ee"/>
                                                    <a:da name="DEFAULT" value="true"/>
                                                </v:var>

                                                <v:var name="McuFIRCRegulatorEnable" type="BOOLEAN">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                SCG_FIRCCSR[FIRCREGOFF] - Fast IRC Regulator Enable <p/>
                                                                0 - Fast IRC Regulator is disabled.<p/>
                                                                1 - Fast IRC Regulator is enabled.<p/>
                                                                Note: Implementation Specific Parameter.<p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="FIRC Regulator Enable"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="UUID" value="ECUC:d5b0195e-5a7a-421e-903e-e4fafc4cca27"/>
                                                    <a:da name="DEFAULT" value="false"/>
                                                </v:var>

                                                <v:var name="McuFIRCDiv2" type="INTEGER">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                    Configures SCG_FIRCDIV[FIRCDIV2] <p/>
                                                                    Fast IRC Clock Divider 2.<p/>
                                                                    Clock divider 2 for the Fast IRC. Used by bus clock modules that need an asynchronous clock source. <p/>
                                                                    0 - Output disabled. <p/>
                                                                    Note: Implementation specific Parameter.
                                                            </html>
                                                            ]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="FIRC Divider 2"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="UUID" value="ECUC:9252979f-f0ff-4b53-864d-b34e3b1a4bf9"/>
                                                    <a:da name="DEFAULT" value="2"/>
                                                    <a:da name="INVALID" type="Range">
                                                        <a:tst expr="&lt;=64"/>
                                                        <a:tst expr="&gt;=0"/>
                                                    </a:da>
                                                    <a:da name="INVALID" type="XPath">
                                                        <a:tst expr="((. != 0) and (. != 1) and (. != 2) and (. != 4) and (. != 8) and (. != 16) and (. != 32) and (. != 64))" true="Only the values 0, 1, 2, 4, 8, 16, 32 and 64 are allowed" />
                                                    </a:da>
                                                </v:var>
                                                
                                                <v:var name="McuFIRCDiv1" type="INTEGER">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                    Configures SCG_FIRCDIV[FIRCDIV1] <p/>
                                                                    Fast IRC Clock Divider 1.<p/>
                                                                    Clock divider 1 for Fast IRC. Used to generate the system clock source and by platform clock modules
                                                                    that need an asynchronous clock source. <p/>
                                                                    0 - Output disabled. <p/>
                                                                    Note: Implementation specific Parameter.
                                                            </html>
                                                            ]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="FIRC Divider 1"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="UUID" value="ECUC:dc5f2873-665c-436d-8b2b-4692381ed29e"/>
                                                    <a:da name="DEFAULT" value="1"/>
                                                    <a:da name="INVALID" type="Range">
                                                        <a:tst expr="&lt;=64"/>
                                                        <a:tst expr="&gt;=0"/>
                                                    </a:da>
                                                    <a:da name="INVALID" type="XPath">
                                                        <a:tst expr="((. != 0) and (. != 1) and (. != 2) and (. != 4) and (. != 8) and (. != 16) and (. != 32) and (. != 64))" true="Only the values 0, 1, 2, 4, 8, 16, 32 and 64 are allowed" />
                                                    </a:da>
                                                </v:var>
                                                
                                                <v:var name="McuFIRCRangeSelect" type="ENUMERATION">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                SCG_FIRCCFG[RANGE] - Selects the Frequency Range. <p/>
                                                                 - Fast IRC is trimmed to 48 MHz. <p/>
                                                                 - Fast IRC is trimmed to 52 MHz. <p/>
                                                                 - Fast IRC is trimmed to 56 MHz. <p/>
                                                                 - Fast IRC is trimmed to 60 MHz. <p/>
                                                                Note: Software should not configure the SCG_FIRCCFG[RANGE] to any value other than Fast IRC is trimmed to 48 MHz. <p/>
                                                                Note: Implementation Specific Parameter.
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="LABEL" value="FIRC Frequency Range"/>
                                                    <a:a name="UUID" value="ECUC:b95a6c1a-7a3c-4b19-ba28-cf181127aaef"/>
                                                    <a:da name="DEFAULT" value="TRIMMED_TO_48MHZ"/>
                                                    <a:da name="RANGE">
                                                        <a:v>TRIMMED_TO_48MHZ</a:v>
                                                        <a:v>TRIMMED_TO_52MHZ</a:v>
                                                        <a:v>TRIMMED_TO_56MHZ</a:v>
                                                        <a:v>TRIMMED_TO_60MHZ</a:v>
                                                    </a:da>
                                                    <a:da name="INVALID" type="XPath">
                                                        <a:tst expr="(node:fallback(., 'TRIMMED_TO_48MHZ') != 'TRIMMED_TO_48MHZ')  "
                                                            true="HW only support to configure SCG_FIRCCFG[RANGE] with TRIMMED_TO_48MHZ"/>
                                                    </a:da>
                                                </v:var>

                                            </v:ctr>                                            

                                            <v:ctr name="McuSystemPll" type="IDENTIFIABLE">
                                                <a:a name="DESC">
                                                    <a:v>
                                                        <![CDATA[EN:<html>
                                                            This container provides the specific configuration for the System PLL.
                                                            Note: Implementation Specific Container.
                                                        </html>]]>
                                                    </a:v>
                                                </a:a>
                                                <a:a name="TAB" value="McuSystemPll"/>
                                                <a:a name="UUID" value="ECUC:1c99c0df-f50f-4c56-9ddc-6695551fee64"/>
                                                <v:var name="McuSystemPllUnderMcuControl" type="BOOLEAN">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                Set this to TRUE if System PLL is under mcu control<p/>
                                                                If it is FALSE then the mcu driver will not write the corresponding registers.<p/>
                                                                The user must still set the values - they are used by the clock refference points <p/>
                                                                Note: Implementation Specific Parameter.<p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="EDITABLE" type="XPath">
                                                        <a:tst expr="ecu:get('MCU.HsrunSystemClkSource.Exist') = 'true'"/>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="System PLL under MCU control"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="UUID" value="ECUC:12d46c75-af01-4b2a-a333-e6870b6a73fd"/>
                                                    <a:da name="DEFAULT" value="false"/>
                                                </v:var>
                                                
                                                <v:var name="McuSPLLFrequency" type="FLOAT">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:
                                                                <html>
                                                                    This is the System PLL frequency for the specific instance of the
                                                                    McuClockReferencePoint container. It is expressed in Hz.<p/>
                                                                    Note: Pll_freq = (McuSPLLInputFrequency * McuSPLLMultiplier)/2 <p/>
                                                                    Note: Implementation Specific Parameter.
                                                                </html>
                                                          ]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="EDITABLE" type="XPath">
                                                        <a:tst expr="ecu:get('MCU.HsrunSystemClkSource.Exist') = 'true'"/>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="System PLL Frequency"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="UUID" value="ECUC:bdcd06c9-1b5f-439c-b924-8b199e5b1898"/>
                                                    <a:da name="DEFAULT" type="XPath">
                                                        <a:tst expr="num:i(num:i(node:fallback(../McuSPLLInputFrequency, num:i(8000000))) * 
                                                                     num:i(node:fallback(../McuSPLLMultiplier,num:i(24))) div num:i(2))"/>
                                                    </a:da>
                                                    <a:a name="RANGE" type="XPath">
                                                        <a:tst expr="( ( not( node:empty( node:fallback(., 96000000) ) )
                                                                         and
                                                                         ( node:fallback(., 96000000) != 
                                                                           num:i(num:i(node:fallback(../McuSPLLInputFrequency, num:i(8000000))) * 
                                                                                 num:i(node:fallback(../McuSPLLMultiplier,num:i(1))) div 
                                                                                 num:i(2))
                                                                         )
                                                                       )
                                                                     )
                                                                     or
                                                                     ( node:fallback(., num:i(96000000)) &gt; num:i(160000000) )
                                                                     or
                                                                     ( node:fallback(., num:i(96000000)) &lt; num:i(90000000) ) "
                                                        true="Please press the button to calculate the value automatically."/>
                                                    </a:a>
                                                    <a:da name="INVALID" type="Range">
                                                        <a:tst expr="&lt;=160000000"/>
                                                        <a:tst expr="&gt;=90000000"/>
                                                    </a:da>
                                                </v:var>
                                                
                                                <v:var name="McuSPLLDiv2Frequency" type="FLOAT">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:
                                                                <html>
                                                                    This is the System PLL Divider 2 frequency for the specific instance of the
                                                                    McuClockReferencePoint container. It is expressed in Hz.<p/>
                                                                    This should be configured to 40 MHz or less in RUN mode and 56 MHz or less in HSRUN mode.<p/>
                                                                    Note: Implementation Specific Parameter.<p/>
                                                                </html>
                                                          ]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="EDITABLE" type="XPath">
                                                        <a:tst expr="((ecu:get('MCU.HsrunSystemClkSource.Exist') = 'true')
                                                                    and 
                                                                    (node:fallback(../McuSPLLDiv2, num:i(1)) != num:i(0)))"/>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="System PLL Div2 Frequency"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="UUID" value="ECUC:9b4f04c4-bed8-4e9a-aaea-6d0e0f9530a3"/>
                                                    <a:da name="DEFAULT" type="XPath">
                                                        <a:tst expr="(num:i(
                                                                            (node:fallback(../McuSPLLFrequency, 96000000)) 
                                                                             div num:i(node:fallback(../McuSPLLDiv2, num:i(2)))
                                                                          )*num:i(number(node:fallback(../McuSPLLDiv2, num:i(1)) != num:i(0)))
                                                                      +
                                                                      num:i(9000000)*num:i(number(node:fallback(../McuSPLLDiv2, num:i(1)) = num:i(0))))"/>
                                                    </a:da>
                                                    <!-- <a:da name="EDITABLE" type="XPath">
                                                        <a:tst expr="(node:fallback(../McuSPLLDiv2, num:i(1)) != num:i(0))"/>
                                                    </a:da> -->
                                                    <a:a name="RANGE" type="XPath">
                                                        <a:tst expr="( ( not( node:empty( node:fallback(., 48000000) ) )
                                                                         and
                                                                         ( node:fallback(., 48000000) != 
                                                                           num:i
                                                                           (
                                                                             (node:fallback(../McuSPLLFrequency, 96000000)) 
                                                                              div num:i(node:fallback(../McuSPLLDiv2, num:i(2)))
                                                                           )
                                                                         )
                                                                         and
                                                                         ( node:fallback(../McuSPLLDiv2, num:i(1)) != num:i(0))
                                                                       )
                                                                     )
                                                                     or
                                                                     ( node:fallback(., num:i(48000000)) &gt; num:i(56000000) )
                                                                     or
                                                                     ( node:fallback(., num:i(48000000)) &lt; num:i(1000000) ) "
                                                        true="Please press the button to calculate the value automatically."/>
                                                    </a:a>
                                                    <a:a name="INVALID" type="Range">
                                                        <a:tst expr="&lt;=56000000"/>
                                                        <a:tst expr="&gt;=9000000"/>
                                                    </a:a>
                                                </v:var>

                                                <v:var name="McuSPLLDiv1Frequency" type="FLOAT">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:
                                                                <html>
                                                                    This is the System PLL Divider 1 frequency for the specific instance of the
                                                                    McuClockReferencePoint container. It is expressed in Hz. <p/>
                                                                    This should be configured to 80MHz or less in RUN mode and to 112 MHz or less in HSRUN mode.<p/>
                                                                    Note: Implementation Specific Parameter.<p/>
                                                                </html>
                                                          ]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="EDITABLE" type="XPath">
                                                        <a:tst expr="((ecu:get('MCU.HsrunSystemClkSource.Exist') = 'true')
                                                                    and 
                                                                    (node:fallback(../McuSPLLDiv1, num:i(1)) != num:i(0)))"/>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="System PLL Div1 Frequency"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="UUID" value="ECUC:c987da34-a8f9-4ad3-b421-0c07323dc22c"/>
                                                    <a:da name="DEFAULT" type="XPath">
                                                        <a:tst expr="(num:i(
                                                                            (node:fallback(../McuSPLLFrequency, 96000000)) 
                                                                             div num:i(node:fallback(../McuSPLLDiv1, num:i(1)))
                                                                          )*num:i(number(node:fallback(../McuSPLLDiv1, num:i(1)) != num:i(0)))
                                                                      +
                                                                      num:i(9000000)*num:i(number(node:fallback(../McuSPLLDiv1, num:i(1)) = num:i(0))))"/>
                                                    </a:da>
                                                    
                                                    <!-- <a:da name="EDITABLE" type="XPath">
                                                        <a:tst expr="(node:fallback(../McuSPLLDiv1, num:i(1)) != num:i(0))"/>
                                                    </a:da> -->
                                                    <a:a name="RANGE" type="XPath">
                                                        <a:tst expr="( ( not( node:empty( node:fallback(., 48000000) ) )
                                                                         and
                                                                         ( node:fallback(., 48000000) != 
                                                                           num:i
                                                                           (
                                                                             (node:fallback(../McuSPLLFrequency, 96000000)) 
                                                                              div num:i(node:fallback(../McuSPLLDiv1, num:i(1)))
                                                                           )
                                                                         )
                                                                         and
                                                                         ( node:fallback(../McuSPLLDiv1, num:i(1)) != num:i(0))
                                                                       )
                                                                     )
                                                                     or
                                                                     ( node:fallback(., num:i(48000000)) &gt; num:i(112000000) )
                                                                     or
                                                                     ( node:fallback(., num:i(48000000)) &lt; num:i(9000000) ) "
                                                        true="Please press the button to calculate the value automatically."/>
                                                    </a:a>
                                                    <a:a name="INVALID" type="Range">
                                                        <a:tst expr="&lt;=112000000"/>
                                                        <a:tst expr="&gt;=9000000"/>
                                                    </a:a>
                                                </v:var>

                                                <v:var name="McuSPLLEnable" type="BOOLEAN">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                SCG_SPLLCSR[SPLLEN] - System PLL Enable <p/>
                                                                0 - System PLL is disabled. <p/>
                                                                1 - System PLL is enabled.<p/>
                                                                Note: Implementation Specific Parameter.<p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="EDITABLE" type="XPath">
                                                        <a:tst expr="ecu:get('MCU.HsrunSystemClkSource.Exist') = 'true'"/>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="System PLL Enable"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="UUID" value="ECUC:8413e329-5d60-4041-b9f8-e875bed95e93"/>
                                                    <a:da name="DEFAULT" value="false"/>
                                                </v:var>

                                                <v:var name="McuSPLLClockMonitorResetEnable" type="BOOLEAN">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                SCG_SPLLCSR[SPLLCMRE] - System PLL Clock Monitor Reset Enable <p/>
                                                                0 - Clock Monitor generates interrupt when error detected.<p/>
                                                                1 - Clock Monitor generates reset when error detected.<p/>
                                                                Note: Implementation Specific Parameter.<p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="EDITABLE" type="XPath">
                                                        <a:tst expr="ecu:get('MCU.HsrunSystemClkSource.Exist') = 'true'"/>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="System PLL clock monitor reset enable"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="UUID" value="ECUC:31220a58-35bf-458d-9409-6620bfd19450"/>
                                                    <a:da name="DEFAULT" value="false"/>
                                                </v:var>

                                                <v:var name="McuSPLLClockMonitorEnable" type="BOOLEAN">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                SCG_SPLLCSR[SPLLCM] - System PLL Clock Monitor <p/>
                                                                Enables the clock monitor, if the clock source is disabled in a low power mode then the clock monitor is
                                                                also disabled in the low power mode. When the clock monitor is disabled in a low power mode, it remains
                                                                disabled until the clock valid flag is set following exit from the low power mode. <p/>
                                                                0 - RTC OSC Clock Monitor is disabled.<p/>
                                                                1 - RTC OSC Clock Monitor is enabled.<p/>
                                                                Note: Implementation Specific Parameter.<p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="EDITABLE" type="XPath">
                                                        <a:tst expr="ecu:get('MCU.HsrunSystemClkSource.Exist') = 'true'"/>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="System PLL clock monitor enable"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="UUID" value="ECUC:7371baca-b13e-4955-a24d-78b3a860661e"/>
                                                    <a:da name="DEFAULT" value="false"/>
                                                </v:var>
                                                
                                                <v:var name="McuSPLLDiv2" type="INTEGER">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                    Configures SCG_SPLLDIV[SPLLDIV2].<p/>
                                                                    System PLL Clock Divider 2.<p/>
                                                                    Clock divider 2 for System PLL. Used by bus clock modules that need an asynchronous clock source. <p/>
                                                                    0 - Output disabled. <p/>
                                                                    Note: Implementation specific Parameter.
                                                            </html>
                                                            ]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="EDITABLE" type="XPath">
                                                        <a:tst expr="ecu:get('MCU.HsrunSystemClkSource.Exist') = 'true'"/>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="System PLL Divider 2"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="UUID" value="ECUC:d0342e86-63d8-4053-bd84-564be99ce3a6"/>
                                                    <a:da name="DEFAULT" value="2"/>
                                                    <a:da name="INVALID" type="Range">
                                                        <a:tst expr="&lt;=64"/>
                                                        <a:tst expr="&gt;=0"/>
                                                    </a:da>
                                                    <a:da name="INVALID" type="XPath">
                                                        <a:tst expr="((. != 0) and (. != 1) and (. != 2) and (. != 4) and (. != 8) and (. != 16) and (. != 32) and (. != 64))" true="Only the values 0, 1, 2, 4, 8, 16, 32 and 64 are allowed" />
                                                    </a:da>
                                                </v:var>
                                                
                                                <v:var name="McuSPLLDiv1" type="INTEGER">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                    Configures SCG_SPLLDIV[SPLLDIV1]<p/>
                                                                    System PLL Clock Divider 1.<p/>
                                                                    Clock divider 1 for System PLL. Used to generate the system clock source and by platform clock modules
                                                                    that need an asynchronous clock source. <p/>
                                                                    0 - Output disabled. <p/>
                                                                    Note: Implementation specific Parameter.
                                                            </html>
                                                            ]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="EDITABLE" type="XPath">
                                                        <a:tst expr="ecu:get('MCU.HsrunSystemClkSource.Exist') = 'true'"/>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="System PLL Divider 1"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="UUID" value="ECUC:eb129277-76f9-46a7-9eaa-071b3a23189d"/>
                                                    <a:da name="DEFAULT" value="1"/>
                                                    <a:da name="INVALID" type="Range">
                                                        <a:tst expr="&lt;=64"/>
                                                        <a:tst expr="&gt;=0"/>
                                                    </a:da>
                                                    <a:da name="INVALID" type="XPath">
                                                        <a:tst expr="((. != 0) and (. != 1) and (. != 2) and (. != 4) and (. != 8) and (. != 16) and (. != 32) and (. != 64))" true="Only the values 0, 1, 2, 4, 8, 16, 32 and 64 are allowed" />
                                                    </a:da>
                                                </v:var>

                                                <v:var name="McuSPLLInputClkPreDivider" type="INTEGER">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                PLL Reference Clock Divider.<p/>
                                                                Set the SPLL: SCG_SPLLCFG[PREDIV] field register.<p/>
                                                                Selects the amount to divide down the reference clock for the System PLL. The resulting frequency must be in the range of 8 MHz to 50 MHz. <p/>
                                                                Note: Implementation Specific Parameter.<p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="EDITABLE" type="XPath">
                                                        <a:tst expr="ecu:get('MCU.HsrunSystemClkSource.Exist') = 'true'"/>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="LABEL" value="System PLL Reference Clock Divider"/>
                                                    <a:a name="UUID" value="ECUC:2adc11d2-b1a7-4073-9ba1-6d5f0c3bcd67"/>
                                                    <a:da name="DEFAULT" value="1"/>
                                                    <a:da name="INVALID" type="Range">
                                                        <a:tst expr="&lt;=8"/>
                                                        <a:tst expr="&gt;=1"/>
                                                    </a:da>
                                                </v:var>

                                                <v:var name="McuSPLLInputFrequency" type="FLOAT">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                Resulted frequency after prediving.<p/>
                                                                F = SPLL_ReferenceClock / McuSPLLInputClkPreDivider.<p/>
                                                                Note: Implementation Specific Parameter.
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="EDITABLE" type="XPath">
                                                        <a:tst expr="ecu:get('MCU.HsrunSystemClkSource.Exist') = 'true'"/>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="System PLL Input Frequency (Calculated)"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="UUID" value="ECUC:81c88572-79f1-4a6e-aa75-bad9824ac400"/>
                                                    <a:da name="DEFAULT" type="XPath">
                                                        <a:tst expr="num:i( num:i( node:fallback(../../McuSystemOSCClockConfig/McuSOSCFrequency, num:i(8000000)))
                                                                          ) div num:i(node:fallback(../McuSPLLInputClkPreDivider,num:i(1)))"/>
                                                    </a:da>
                                                    <a:a name="RANGE" type="XPath">
                                                        <a:tst expr="(( ( not( node:empty( node:fallback(., 40000000) ) )
                                                                         and
                                                                         ( node:fallback(., 40000000) !=
                                                                           ( num:i( num:i( node:fallback(../../McuSystemOSCClockConfig/McuSOSCFrequency, num:i(8000000)))
                                                                                  ) div num:i(node:fallback(../McuSPLLInputClkPreDivider,num:i(1)))
                                                                           )
                                                                         )
                                                                       )
                                                                     )
                                                                     or
                                                                     ( node:fallback(., num:i(40000000)) &gt; num:i(40000000) )
                                                                     or
                                                                     ( node:fallback(., num:i(40000000)) &lt; num:i(8000000) ))
                                                                     and (ecu:get('MCU.CMU.UNITS') = '0')"
                                                               true="Please press the button to calculate the value automatically."/>
                                                    </a:a>
                                                    <a:da name="INVALID" type="Range">
                                                        <a:tst expr="&lt;=40000000"/>
                                                        <a:tst expr="&gt;=8000000"/>
                                                    </a:da>
                                                </v:var>

                                                <v:var name="McuSPLLMultiplier" type="INTEGER">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                System PLL Multiplier. <p/>
                                                                Set the SCG_SPLLCFG[MULT] field register. <p/>
                                                                Valid range is in [16..47].
                                                                Note: Implementation Specific Parameter.
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="EDITABLE" type="XPath">
                                                        <a:tst expr="ecu:get('MCU.HsrunSystemClkSource.Exist') = 'true'"/>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="LABEL" value="System PLL Multiplier"/>
                                                    <a:a name="UUID" value="ECUC:63912b4d-5ca9-4215-aebb-b6dcc88da28d"/>
                                                    <a:da name="DEFAULT" value="24"/>
                                                    <a:da name="INVALID" type="Range">
                                                        <a:tst expr="&lt;=47"/>
                                                        <a:tst expr="&gt;=16"/>
                                                    </a:da>
                                                </v:var>

                                            </v:ctr>

                                            <v:ctr name="McuSIMClockConfig" type="IDENTIFIABLE">
                                                <a:a name="DESC">
                                                  <a:v>
                                                    <![CDATA[EN:
                                                      <html>
                                                          Configures SIM_CHIPCTL[TRACECLK_SEL], SIM_CHIPCTL[CLKOUTSEL] bits and SIM_PLATGC and SIM_CLKDIV4  registers.<p/>
                                                          Note: Implementation specific Container.
                                                      </html>
                                                    ]]>
                                                  </a:v>
                                                </a:a>
                                                <a:a name="TAB" value="McuSIMClockConfig"/>
                                                <a:a name="UUID" value="ECUC:bffd35f6-13bc-4909-994d-246f4f3089bb"/>
                                                <v:var name="McuDebugTraceDividerEnable" type="BOOLEAN">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                SIM_CLKDIV4[TRACEDIVEN] - Debug Trace Divider Control <p/>
                                                                0 - Debug trace divider disabled.<p/>
                                                                1 - Debug trace divider enabled.<p/>
                                                                Note: Implementation Specific Parameter.<p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="Debug Trace Divider Control"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="UUID" value="ECUC:84fb360d-f958-4fcd-9e1e-bf896f817176"/>
                                                    <a:da name="DEFAULT" value="true"/>
                                                </v:var>

                                                <v:var name="McuTraceClockDivider" type="INTEGER">
                                                  <a:a name="DESC">
                                                    <a:v>
                                                      <![CDATA[EN:
                                                        <html>
                                                          <p> Configures the SIM_CLKDIV4[TRACEDIV] bitfield
                                                            Trace clock divider divisor - This field sets the divide value for the fractional clock divider used as a source for trace clock. The source clock for the trace clock is set by the SIM_CHIPCTRL[TRACECLK_SEL]. Divider output clock = Divider input clock * [(TRACEFRAC+1)/(TRACEDIV+1)].
                                                            Note: implementation specific parameter.
                                                          </p>
                                                        </html>
                                                      ]]>
                                                    </a:v>
                                                  </a:a>
                                                  <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                    <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                  </a:a>
                                                  <a:a name="ORIGIN" value="NXP"/>
                                                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                  <a:a name="SCOPE" value="LOCAL"/>
                                                  <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                  <a:a name="UUID" value="ECUC:2aa3a82d-be06-4a76-a50c-9638d580e966"/>
                                                  <a:a name="LABEL" value="Trace Clock Divider"/>
                                                  <a:da name="DEFAULT" value="1"/>
                                                  <a:da name="INVALID" type="Range">
                                                    <a:tst expr="&lt;=8"/>
                                                    <a:tst expr="&gt;=1"/>
                                                  </a:da>
                                                </v:var>

                                                <v:var name="McuTraceClockFraction" type="INTEGER">
                                                  <a:a name="DESC">
                                                    <a:v>
                                                      <![CDATA[EN:
                                                        <html>
                                                          <p> Configures the SIM_CLKDIV4[TRACEFRAC] bitfield
                                                            Trace clock divider fraction - This field sets the divide value for the fractional clock divider used as a source for trace clock. The source clock for the trace clock is set by the SIM_CHIPCTRL[TRACECLK_SEL]. Divider output clock = Divider input clock * [(TRACEFRAC+1)/(TRACEDIV+1)].
                                                            Note: implementation specific parameter.
                                                          </p>
                                                        </html>
                                                      ]]>
                                                    </a:v>
                                                  </a:a>
                                                  <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                    <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                  </a:a>
                                                  <a:a name="ORIGIN" value="NXP"/>
                                                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                  <a:a name="SCOPE" value="LOCAL"/>
                                                  <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                  <a:a name="UUID" value="ECUC:df92ea75-3361-403d-88fd-2dcb6645146d"/>
                                                  <a:a name="LABEL" value="Trace Clock Fraction"/>
                                                  <a:da name="DEFAULT" value="0"/>
                                                  <a:da name="INVALID" type="Range">
                                                    <a:tst expr="&lt;=1"/>
                                                    <a:tst expr="&gt;=0"/>
                                                  </a:da>
                                                </v:var>

                                                <v:var name="McuTraceClockSelect" type="ENUMERATION">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                SIM_CHIPCTL[TRACECLK_SEL] - Debug trace clock select <p/>
                                                                Selects core clock or platform clock as the trace clock source. <p/>
                                                                Note: Implementation Specific Parameter.
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="LABEL" value="Trace clock select"/>
                                                    <a:a name="UUID" value="ECUC:61ec7bfc-1249-463f-adfd-5ccc9e30cd3a"/>
                                                    <a:da name="DEFAULT" value="CORE_CLK"/>
                                                    <a:da name="RANGE">
                                                        <a:v>CORE_CLK</a:v>
                                                        <a:v>PLATFORM_CLK</a:v>
                                                    </a:da>
                                                </v:var>

                                                <v:var name="McuClockOutEnable" type="BOOLEAN">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                SIM_CHIPCTL[CLKOUTEN] - CLKOUT enable <p/>
                                                                unchecked - Clockout disabled.<p/>
                                                                checked - Clockout enabled.<p/>
                                                                Note: Implementation Specific Parameter.<p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="LABEL" value="Enable CLKOUT"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="UUID" value="ECUC:ee7dc1ef-4752-4bb0-9c13-4c7a07d11b42"/>
                                                    <a:da name="DEFAULT" value="false"/>
                                                </v:var>

                                                <v:var name="McuClockOutDivider" type="INTEGER">
                                                  <a:a name="DESC">
                                                    <a:v>
                                                      <![CDATA[EN:
                                                        <html>
                                                          <p> Configures the SIM_CHIPCTL[CLKOUTDIV] bitfield
                                                            CLKOUT Divide Ratio.
                                                            Note: implementation specific parameter.
                                                          </p>
                                                        </html>
                                                      ]]>
                                                    </a:v>
                                                  </a:a>
                                                  <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                    <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                  </a:a>
                                                  <a:a name="ORIGIN" value="NXP"/>
                                                  <a:a name="SCOPE" value="LOCAL"/>
                                                  <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                  <a:a name="UUID" value="ECUC:9e70a8da-6276-4df6-9d6a-a112cad40240"/>
                                                  <a:a name="LABEL" value="CLKOUT Divider"/>
                                                  <a:da name="DEFAULT" value="1"/>
                                                  <a:da name="INVALID" type="Range">
                                                    <a:tst expr="&lt;=8"/>
                                                    <a:tst expr="&gt;=1"/>
                                                  </a:da>
                                                </v:var>

                                                <v:var name="McuClockOutSelect" type="ENUMERATION">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                SIM_CHIPCTL[CLKOUTSEL] - CLKOUT select <p/>
                                                                Selects the clock to output on the CLKOUT pin. <p/>
                                                                0  SCG_CLKOUT <p/>
                                                                2  SOSC_DIV2 <p/>
                                                                4  SIRC_DIV2 <p/>
                                                                5  QSPI_SFIF_CLK_HYP Divide by 2 clock (configured through SCLKCONFIG[5]) for HyperRAM going to sfif clock to QSPI(Specific for S32K148) <p/>
                                                                6  FIRC_DIV2 <p/>
                                                                7  HCLK <p/>
                                                                8  SPLL_DIV2 <p/>
                                                                9  BUS_CLK <p/>
                                                                A  LPO_CLK_128K <p/>
                                                                B  QSPI_IPG_CLK (Specific for S32K148) <p/>
                                                                C  LPO_CLK as selected by SIM_LPOCLKS[LPOCLKSEL]  <p/>
                                                                D  QSPI_IPG_CLK_SFIF (Specific for S32K148) <p/>
                                                                E  RTC_CLK as selected by SIM CLK 32 KHz Select <p/>
                                                                F  QSPI_IPG_CLK_2XSFIF (Specific for S32K148) <p/>
                                                                Note: Implementation Specific Parameter.
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="LABEL" value="SIM CLKOUT select"/>
                                                    <a:a name="UUID" value="ECUC:017dcdba-7976-4bae-b915-590d75b87f17"/>
                                                    <a:da name="DEFAULT" type="XPath" expr="ecu:get('MCU.CLKOUT.Default')"/>
                                                    <a:da name="RANGE" type="XPath" expr="ecu:list('MCU.CLKOUT.List')"/>
                                                </v:var>

                                                <v:var name="McuEIMClockGatingEnable" type="BOOLEAN">
                                                    <a:a name="DESC">
