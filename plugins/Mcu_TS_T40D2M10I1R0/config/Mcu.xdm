                                                <a:a name="UUID" value="ECUC:3cb29794-c3ce-4389-a134-b544dd20a19f"/>
                                                <a:da name="DEFAULT" value="true"/>
                                            </v:var>
                                            
                                            <v:var name="McuScgClkOutSelect" type="ENUMERATION">
                                                <a:a name="DESC">
                                                    <a:v>
                                                        <![CDATA[EN:<html>
                                                            SCG_CLKOUTCNFG[CLKOUTSEL] - SCG Clkout Select.
                                                            This register controls which SCG clock source is selected to be ported out to the CLKOUT pin. <p/>
                                                            0 - SCG SLOW Clock (FLASH_CLK Clock). <p/>
                                                            1 - System OSC <p/>
                                                            2 - Slow IRC <p/>
                                                            3 - Fast IRC <p/>
                                                            6 - System PLL <p/>
                                                            Note: Implementation Specific Parameter.<p/>
                                                        </html>]]>
                                                    </a:v>
                                                </a:a>
                                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                    <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                </a:a>
                                                <a:a name="ORIGIN" value="NXP"/>
                                                <a:a name="LABEL" value="SCG ClkOut Select"/>
                                                <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                <a:a name="SCOPE" value="LOCAL"/>
                                                <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                <a:a name="UUID" value="ECUC:b20d3aea-fae1-4d5e-a972-55880e7a4c4e"/>
                                                <a:da name="DEFAULT" value="SCG_SLOW_CLK"/>
                                                <a:da name="RANGE">
                                                    <a:v>SCG_SLOW_CLK</a:v>
                                                    <a:v>SOSC</a:v>
                                                    <a:v>SIRC</a:v>
                                                    <a:v>FIRC</a:v>
                                                    <a:v>SPLL</a:v>
                                                </a:da>
                                            </v:var>
                                                                                                                                        
                                            <v:ctr name="McuRunClockConfig" type="IDENTIFIABLE">
                                                <a:a name="DESC">
                                                  <a:v>
                                                    <![CDATA[EN:
                                                      <html>
                                                          This container configures the system clock source and the system clock dividers 
                                                          for the core, platform, external and bus clock domains when in Run mode only.<p/>
                                                          Note: Implementation specific Container.
                                                      </html>
                                                    ]]>
                                                  </a:v>
                                                </a:a>
                                                <a:a name="UUID" value="ECUC:881f49a0-374e-4d6a-8327-239b534c0d3d"/>

                                                <v:var name="McuPreDivSystemClockFrequency" type="FLOAT">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                Run Core clock - Pre Divide System Clock Frequency. <p/>
                                                                This is the frequency for the specific instance of the "McuClockReferencePoint" container. <p/>
                                                                Value calculated for user info. It is given in Hz. <p/>
                                                                Note: Implementation Specific Parameter. <p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="LABEL" value="Run Pre Div System Clock Frequency (Hz)"/>
                                                    <a:a name="UUID" value="ECUC:77715d1c-031d-46bc-ac89-fbbbc9a4a0d7"/>
                                                    <a:da name="DEFAULT" type="XPath">
                                                        <a:tst expr="num:i( num:i( number( node:fallback(../McuSystemClockSwitch,'FIRC')='SOSC')) *
                                                                            num:i( node:fallback(../../McuSystemOSCClockConfig/McuSOSCFrequency, num:i(1000000))) +
                                                                            num:i( number( node:fallback(../McuSystemClockSwitch,'FIRC')='SIRC')) *
                                                                            num:i( node:fallback(../../McuSIRCClockConfig/McuSIRCFrequency, num:i(8000000))) +
                                                                            num:i( number( node:fallback(../McuSystemClockSwitch,'FIRC')='FIRC')) *
                                                                            num:i( node:fallback(../../McuFIRCClockConfig/McuFIRCFrequency, num:i(48000000))) +
                                                                            num:i( number( node:fallback(../McuSystemClockSwitch,'FIRC')='SPLL')) *
                                                                            num:i( node:fallback(../../McuSystemPll/McuSPLLFrequency, num:i(40000000)))
                                                                          )"/>
                                                    </a:da>
                                                    <a:a name="RANGE" type="XPath">
                                                        <a:tst expr="( ( not( node:empty( node:fallback(., 80000000) ) )
                                                                         and
                                                                         ( node:fallback(., 80000000) !=
                                                                           ( num:i( num:i( number( node:fallback(../McuSystemClockSwitch,'FIRC')='SOSC')) *
                                                                                    num:i( node:fallback(../../McuSystemOSCClockConfig/McuSOSCFrequency, num:i(1000000))) +
                                                                                    num:i( number( node:fallback(../McuSystemClockSwitch,'FIRC')='SIRC')) *
                                                                                    num:i( node:fallback(../../McuSIRCClockConfig/McuSIRCFrequency, num:i(8000000))) +
                                                                                    num:i( number( node:fallback(../McuSystemClockSwitch,'FIRC')='FIRC')) *
                                                                                    num:i( node:fallback(../../McuFIRCClockConfig/McuFIRCFrequency, num:i(48000000))) +
                                                                                    num:i( number( node:fallback(../McuSystemClockSwitch,'FIRC')='SPLL')) *
                                                                                    num:i( node:fallback(../../McuSystemPll/McuSPLLFrequency, num:i(40000000)))
                                                                                  )
                                                                           )
                                                                         )
                                                                       )
                                                                     )
                                                                     or
                                                                     ( node:fallback(., num:i(80000000)) &gt; num:i(160000000) )
                                                                     or
                                                                     ( node:fallback(., num:i(80000000)) &lt; num:i(1000) ) "
                                                        true="Please press the button to calculate the value automatically."/>
                                                    </a:a>
                                                    <a:a name="INVALID" type="Range">
                                                        <a:tst expr="&lt;=160000000"/>
                                                        <a:tst expr="&gt;=1000"/>
                                                    </a:a>
                                                </v:var>

                                                <v:var name="McuCoreClockFrequency" type="FLOAT">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                Run Core clock - Clocks the ARM core, divided by DIVCORE bits inside SCG <p/>
                                                                This is the frequency for the specific instance of the "McuClockReferencePoint" container.
                                                                Value calculated for user info. It is given in Hz. <p/>
                                                                Note: Implementation Specific Parameter. <p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="LABEL" value="Run Core Clock Frequency (Hz)"/>
                                                    <a:a name="UUID" value="ECUC:8ef0be74-6333-484e-95f5-971ded44bfdc"/>
                                                    <a:da name="DEFAULT" type="XPath">
                                                        <a:tst expr="num:i( num:i( number( node:fallback(../McuSystemClockSwitch,'FIRC')='SOSC')) *
                                                                            num:i( node:fallback(../../McuSystemOSCClockConfig/McuSOSCFrequency, num:i(1000000))) +
                                                                            num:i( number( node:fallback(../McuSystemClockSwitch,'FIRC')='SIRC')) *
                                                                            num:i( node:fallback(../../McuSIRCClockConfig/McuSIRCFrequency, num:i(8000000))) +
                                                                            num:i( number( node:fallback(../McuSystemClockSwitch,'FIRC')='FIRC')) *
                                                                            num:i( node:fallback(../../McuFIRCClockConfig/McuFIRCFrequency, num:i(48000000))) +
                                                                            num:i( number( node:fallback(../McuSystemClockSwitch,'FIRC')='SPLL')) *
                                                                            num:i( node:fallback(../../McuSystemPll/McuSPLLFrequency, num:i(40000000)))
                                                                          ) div num:i(node:fallback(../McuCoreClockDivider,num:i(1)))"/>
                                                    </a:da>
                                                    <a:a name="RANGE" type="XPath">
                                                        <a:tst expr="( ( not( node:empty( node:fallback(., 80000000) ) )
                                                                         and
                                                                         ( node:fallback(., 80000000) !=
                                                                           ( num:i( num:i( number( node:fallback(../McuSystemClockSwitch,'FIRC')='SOSC')) *
                                                                                    num:i( node:fallback(../../McuSystemOSCClockConfig/McuSOSCFrequency, num:i(1000000))) +
                                                                                    num:i( number( node:fallback(../McuSystemClockSwitch,'FIRC')='SIRC')) *
                                                                                    num:i( node:fallback(../../McuSIRCClockConfig/McuSIRCFrequency, num:i(8000000))) +
                                                                                    num:i( number( node:fallback(../McuSystemClockSwitch,'FIRC')='FIRC')) *
                                                                                    num:i( node:fallback(../../McuFIRCClockConfig/McuFIRCFrequency, num:i(48000000))) +
                                                                                    num:i( number( node:fallback(../McuSystemClockSwitch,'FIRC')='SPLL')) *
                                                                                    num:i( node:fallback(../../McuSystemPll/McuSPLLFrequency, num:i(40000000)))
                                                                                  ) div num:i(node:fallback(../McuCoreClockDivider,num:i(1)))
                                                                           )
                                                                         )
                                                                       )
                                                                     )
                                                                     or
                                                                     ( node:fallback(., num:i(80000000)) &gt; num:i(80000000) )
                                                                     or
                                                                     ( node:fallback(., num:i(80000000)) &lt; num:i(1000) ) "
                                                        true="Please press the button to calculate the value automatically."/>
                                                    </a:a>
                                                    <a:a name="INVALID" type="Range">
                                                        <a:tst expr="&lt;=80000000"/>
                                                        <a:tst expr="&gt;=1000"/>
                                                    </a:a>
                                                </v:var>

                                                <v:var name="McuSystemClockFrequency" type="FLOAT">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                Run System clock - Clocks the Crossbar, NVIC, Flash controller, FTM and PDB,etc. <p/>
                                                                RUN_SYS_CLK can run up to CORE_CLK. <p/>
                                                                This is the frequency for the specific instance of the "McuClockReferencePoint" container.
                                                                Value calculated for user info. It is given in Hz. <p/>
                                                                Note: Implementation Specific Parameter.
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="LABEL" value="Run System Clock Frequency (Hz)"/>
                                                    <a:a name="UUID" value="ECUC:31264c4f-1705-49e5-9c5f-ff8d1fa255c9"/>
                                                    <a:da name="DEFAULT" type="XPath">
                                                        <a:tst expr="num:i(node:fallback(../McuCoreClockFrequency, num:i(80000000)))"/>
                                                    </a:da>
                                                    <a:a name="RANGE" type="XPath">
                                                        <a:tst expr="( ( not( node:empty( node:fallback(., 80000000) ) )
                                                                         and
                                                                         ( node:fallback(., 80000000) != 
                                                                              num:i(node:fallback(../McuCoreClockFrequency, num:i(80000000)))
                                                                         )
                                                                       )
                                                                     )
                                                                     or
                                                                     ( node:fallback(., num:i(80000000)) &gt; num:i(80000000) )
                                                                     or
                                                                     ( node:fallback(., num:i(80000000)) &lt; num:i(1000) ) "
                                                        true="Please press the button to calculate the value automatically."/>
                                                    </a:a>
                                                    <a:a name="INVALID" type="Range">
                                                        <a:tst expr="&lt;=80000000"/>
                                                        <a:tst expr="&gt;=1000"/>
                                                    </a:a>
                                                </v:var>

                                                <v:var name="McuBusClockFrequency" type="FLOAT">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                Run Bus clock - BUS_CLK Clocks the Peripherals, divided by DIVBUS bits inside SCG <p/>
                                                                This is the frequency for the specific instance of the "McuClockReferencePoint" container.
                                                                Value calculated for user info. It is given in Hz. <p/>
                                                                Note: Implementation Specific Parameter.
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="LABEL" value="Run Bus Clock Frequency (Hz)"/>
                                                    <a:a name="UUID" value="ECUC:e1834826-9f3a-4d3c-9571-afa28f021d5f"/>
                                                    <a:da name="DEFAULT" type="XPath">
                                                        <a:tst expr="num:i(
                                                                            num:i(node:fallback(../McuCoreClockFrequency, num:i(48000000))) div
                                                                            num:i(node:fallback(../McuBusClockDivider,num:i(1)))
                                                                          )"/>
                                                    </a:da>
                                                    <a:a name="RANGE" type="XPath">
                                                        <a:tst expr="( ( not( node:empty( node:fallback(., 48000000) ) )
                                                                         and
                                                                         ( node:fallback(., 48000000) != 
                                                                           num:i( num:i(node:fallback(../McuCoreClockFrequency, num:i(48000000))) div
                                                                                  num:i(node:fallback(../McuBusClockDivider,num:i(1)))
                                                                                )
                                                                         )
                                                                       )
                                                                     )
                                                                     or
                                                                     ( node:fallback(., num:i(48000000)) &gt; num:i(48000000) )
                                                                     or
                                                                     ( node:fallback(., num:i(48000000)) &lt; num:i(1000) ) "
                                                        true="Please press the button to calculate the value automatically."/>
                                                    </a:a>
                                                    <a:a name="INVALID" type="Range">
                                                        <a:tst expr="&lt;=48000000"/>
                                                        <a:tst expr="&gt;=1000"/>
                                                    </a:a>
                                                </v:var>

                                                <v:var name="McuFlashClockFrequency" type="FLOAT">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                Run Flash clock - Clocks the flash module, divided by DIVSLOW bits inside SCG <p/>
                                                                This is the frequency for the specific instance of the "McuClockReferencePoint" container.
                                                                Value calculated for user info. It is given in Hz. <p/>
                                                                Note: Implementation Specific Parameter.
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="LABEL" value="Run Flash Clock Frequency (Hz)"/>
                                                    <a:a name="UUID" value="ECUC:d04ab0ff-8009-4102-b82f-30c6362d6e5a"/>
                                                    <a:da name="DEFAULT" type="XPath">
                                                        <a:tst expr="num:i(
                                                                            num:i(node:fallback(../McuCoreClockFrequency, num:i(26670000))) div
                                                                            num:i(node:fallback(../McuSlowClockDivider,num:i(1)))
                                                                          )"/>
                                                    </a:da>
                                                    <a:a name="RANGE" type="XPath">
                                                        <a:tst expr="( ( not( node:empty( node:fallback(., 26670000) ) )
                                                                         and
                                                                         ( node:fallback(., 26670000) != 
                                                                           num:i(
                                                                                  num:i(node:fallback(../McuCoreClockFrequency, num:i(40000000))) div
                                                                                  num:i(node:fallback(../McuSlowClockDivider,num:i(1)))
                                                                                )
                                                                         )
                                                                       )
                                                                     )
                                                                     or
                                                                     ( node:fallback(., num:i(26670000)) &gt; num:i(26670000) )
                                                                     or
                                                                     ( node:fallback(., num:i(26670000)) &lt; num:i(1000) ) "
                                                        true="Please press the button to calculate the value automatically."/>
                                                    </a:a>
                                                    <a:da name="INVALID" type="XPath">
                                                        <a:tst expr="(
                                                                        num:i(
                                                                            num:i(node:fallback(../McuCoreClockFrequency, num:i(1000))) 
                                                                            div 
                                                                            num:i(node:fallback(., num:i(1000)))
                                                                        )
                                                                        &gt; num:i(8)
                                                                     )"
                                                               true="The core clock to the flash clock ratio is limited to a max vaule of 8."/>
                                                    </a:da>
                                                    <a:a name="INVALID" type="Range">
                                                        <a:tst expr="&lt;=26670000"/>
                                                        <a:tst expr="&gt;=1000"/>
                                                    </a:a>
                                                </v:var>

                                                <v:var name="McuSystemClockSwitch" type="ENUMERATION">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                Run System Clock Select. Configure the SCG_RCCR[SCS] register field.
                                                                The system clock is either:<p/>
                                                                - System OSC (SCG_RCCR[SCS]=1)<p/>
                                                                - Slow IRC (SCG_RCCR[SCS]=2)<p/>
                                                                - Fast IRC (SCG_RCCR[SCS]=3)<p/>
                                                                - System PLL (SCG_RCCR[SCS]=6)<p/>
                                                                Value extracted from Resource: MCU.RunSystemClkSource.List<p/>
                                                                Note: Implementation Specific Parameter.
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="UUID" value="ECUC:84ed3484-3322-42ca-9afc-17833af8edfe"/>
                                                    <a:a name="LABEL" value="Run System Clock Select"/>
                                                    <a:da name="DEFAULT" type="XPath" expr="ecu:get('MCU.RunSystemClkSource.Default')"/>
                                                    <a:da name="RANGE" type="XPath" expr="ecu:list('MCU.RunSystemClkSource.List')"/>
                                                    <a:da name="INVALID" type="XPath">
                                                        <a:tst expr="(
                                                                        (
                                                                            node:fallback(../../McuFIRCClockConfig/McuFIRCUnderMcuControl, 'true') = 'true' and
                                                                            node:fallback(../../McuFIRCClockConfig/McuFIRCEnable, 'true') = 'false'
                                                                        ) and (node:fallback(., 'FIRC') = 'FIRC')
                                                                     ) or
                                                                     (
                                                                        (
                                                                            node:fallback(../../McuSIRCClockConfig/McuSIRCUnderMcuControl, 'true') = 'true' and
                                                                            node:fallback(../../McuSIRCClockConfig/McuSIRCEnable, 'true') = 'false'
                                                                        ) and (node:fallback(., 'FIRC') = 'SIRC')
                                                                     ) or
                                                                     (
                                                                        (
                                                                            node:fallback(../../McuSystemOSCClockConfig/McuSOSCUnderMcuControl, 'true') = 'true' and
                                                                            node:fallback(../../McuSystemOSCClockConfig/McuSOSCEnable, 'true') = 'false'
                                                                        ) and (node:fallback(., 'FIRC') = 'SOSC')
                                                                     ) or
                                                                     (
                                                                        (
                                                                            node:fallback(../../McuSystemPll/McuSystemPllUnderMcuControl, 'true') = 'true' and
                                                                            node:fallback(../../McuSystemPll/McuSPLLEnable, 'true') = 'false'
                                                                        ) and (node:fallback(., 'FIRC') = 'SPLL')
                                                                     )
                                                                     "
                                                            true="The source clock must be enabled. If not under MCU control the source clock should be enabled prior to setting this configuration. "/>
                                                    </a:da>
                                                </v:var>

                                                <v:var name="McuCoreClockDivider" type="INTEGER">
                                                  <a:a name="DESC">
                                                    <a:v>
                                                      <![CDATA[EN:
                                                        <html>
                                                          <p> Configures the SCG_RCCR[DIVCORE] bitfield
                                                            This parameter represents the core clock divider.
                                                            Note: implementation specific parameter.
                                                          </p>
                                                        </html>
                                                      ]]>
                                                    </a:v>
                                                  </a:a>
                                                  <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                    <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                  </a:a>
                                                  <a:a name="ORIGIN" value="NXP"/>
                                                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                  <a:a name="SCOPE" value="LOCAL"/>
                                                  <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                  <a:a name="UUID" value="ECUC:67446bc3-b58d-419d-9acd-4b783b47eb01"/>
                                                  <a:da name="DEFAULT" value="1"/>
                                                  <a:a name="LABEL" value="Run Core Clock Divider"/>
                                                  <a:da name="INVALID" type="XPath">
                                                      <a:tst expr="( ( node:fallback(../McuSystemClockSwitch, 'FIRC') = 'SPLL' ) and
                                                                     ( (num:i(node:fallback(., num:i(1)))) &gt; num:i(4) )
                                                                   )"
                                                             true="If SPLL is selected as system clock source, the maximum DIVCORE value is Divide-by-4."/>
                                                  </a:da>
                                                  <a:da name="INVALID" type="Range">
                                                    <a:tst expr="&lt;=16"/>
                                                    <a:tst expr="&gt;=1"/>
                                                  </a:da>
                                                </v:var>

                                                <v:var name="McuBusClockDivider" type="INTEGER">
                                                  <a:a name="DESC">
                                                    <a:v>
                                                      <![CDATA[EN:
                                                        <html>
                                                          <p> Configures the SCG_RCCR[DIVBUS] bitfield
                                                            This parameter represents the bus clock divider.
                                                            Note: implementation specific parameter.
                                                          </p>
                                                        </html>
                                                      ]]>
                                                    </a:v>
                                                  </a:a>
                                                  <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                    <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                  </a:a>
                                                  <a:a name="ORIGIN" value="NXP"/>
                                                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                  <a:a name="SCOPE" value="LOCAL"/>
                                                  <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                  <a:a name="UUID" value="ECUC:01397154-aa83-4f0d-89d6-e8e71dca0df8"/>
                                                  <a:a name="LABEL" value="Run Bus Clock Divider"/>
                                                  <a:da name="DEFAULT" value="1"/>
                                                  <a:da name="INVALID" type="Range">
                                                    <a:tst expr="&lt;=16"/>
                                                    <a:tst expr="&gt;=1"/>
                                                  </a:da>
                                                </v:var>

                                                <v:var name="McuSlowClockDivider" type="INTEGER">
                                                  <a:a name="DESC">
                                                    <a:v>
                                                      <![CDATA[EN:
                                                        <html>
                                                          <p> Configures the SCG_RCCR[DIVSLOW] bitfield
                                                            This parameter represents the flash clock divider.
                                                            Note: implementation specific parameter.
                                                          </p>
                                                        </html>
                                                      ]]>
                                                    </a:v>
                                                  </a:a>
                                                  <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                    <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                  </a:a>
                                                  <a:a name="ORIGIN" value="NXP"/>
                                                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                  <a:a name="SCOPE" value="LOCAL"/>
                                                  <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                  <a:a name="UUID" value="ECUC:3e97b7b6-c020-42de-a1de-606d2ee36741"/>
                                                  <a:a name="LABEL" value="Run Slow Clock Divider"/>
                                                  <a:da name="DEFAULT" value="2"/>
                                                  <a:da name="INVALID" type="Range">
                                                    <a:tst expr="&lt;=8"/>
                                                    <a:tst expr="&gt;=1"/>
                                                  </a:da>
                                                </v:var>
                                                
                                                <v:var name="McuScgClkOutFrequency" type="FLOAT">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                This is frequency of SCG clockout. It is given in Hz. <p/>
                                                                Note: Implementation Specific Parameter.
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="LABEL" value="RCCR SCG ClockOut Frequency (Hz)"/>
                                                    <a:a name="UUID" value="ECUC:9bd9dcda-6984-4799-9262-6c11d336cf68"/>
                                                    <a:da name="DEFAULT" type="XPath">
                                                        <a:tst expr="num:i( num:i( number( node:fallback(../../McuScgClkOutSelect,'SCG_SLOW_CLK')='SCG_SLOW_CLK')) *
                                                                                    num:i( node:fallback(../McuFlashClockFrequency, num:i(1000000))) +
                                                                                    num:i( number( node:fallback(../../McuScgClkOutSelect,'SCG_SLOW_CLK')='SIRC')) *
                                                                                    num:i( node:fallback(../../McuSIRCClockConfig/McuSIRCFrequency, num:i(8000000))) +
                                                                                    num:i( number( node:fallback(../../McuScgClkOutSelect,'SCG_SLOW_CLK')='FIRC')) *
                                                                                    num:i( node:fallback(../../McuFIRCClockConfig/McuFIRCFrequency, num:i(48000000))) +
                                                                                    num:i( number( node:fallback(../../McuScgClkOutSelect,'SCG_SLOW_CLK')='SOSC')) *
                                                                                    num:i( node:fallback(../../McuSystemOSCClockConfig/McuSOSCFrequency, num:i(40000000))) +
                                                                                    num:i( number( node:fallback(../../McuScgClkOutSelect,'SCG_SLOW_CLK')='SPLL')) *
                                                                                    num:i( node:fallback(../../McuSystemPll/McuSPLLFrequency, num:i(40000000)))
                                                                                  )"/>
                                                    </a:da>
                                                    <a:a name="RANGE" type="XPath">
                                                        <a:tst expr="( ( not( node:empty( node:fallback(., 100000000) ) )
                                                                         and
                                                                         ( node:fallback(., 100000000) !=
                                                                           ( num:i( num:i( number( node:fallback(../../McuScgClkOutSelect,'SCG_SLOW_CLK')='SCG_SLOW_CLK')) *
                                                                                    num:i( node:fallback(../McuFlashClockFrequency, num:i(1000000))) +
                                                                                    num:i( number( node:fallback(../../McuScgClkOutSelect,'SCG_SLOW_CLK')='SIRC')) *
                                                                                    num:i( node:fallback(../../McuSIRCClockConfig/McuSIRCFrequency, num:i(8000000))) +
                                                                                    num:i( number( node:fallback(../../McuScgClkOutSelect,'SCG_SLOW_CLK')='FIRC')) *
                                                                                    num:i( node:fallback(../../McuFIRCClockConfig/McuFIRCFrequency, num:i(48000000))) +
                                                                                    num:i( number( node:fallback(../../McuScgClkOutSelect,'SCG_SLOW_CLK')='SOSC')) *
                                                                                    num:i( node:fallback(../../McuSystemOSCClockConfig/McuSOSCFrequency, num:i(40000000))) +
                                                                                    num:i( number( node:fallback(../../McuScgClkOutSelect,'SCG_SLOW_CLK')='SPLL')) *
                                                                                    num:i( node:fallback(../../McuSystemPll/McuSPLLFrequency, num:i(40000000)))
                                                                                  )
                                                                           )
                                                                         )
                                                                       )
                                                                     )
                                                                     or
                                                                     ( node:fallback(., num:i(100000000)) &gt; num:i(180000000) )
                                                                     or
                                                                     ( node:fallback(., num:i(100000000)) &lt; num:i(1000) ) "
                                                        true="Please press the button to calculate the value automatically."/>
                                                    </a:a>
                                                    <a:a name="INVALID" type="Range">
                                                        <a:tst expr="&lt;=180000000"/>
                                                        <a:tst expr="&gt;=1000"/>
                                                    </a:a>
                                                </v:var>

                                            </v:ctr>

                                            <v:ctr name="McuVlprClockConfig" type="IDENTIFIABLE">
                                                <a:a name="DESC">
                                                  <a:v>
                                                    <![CDATA[EN:
                                                      <html>
                                                          Selects the clock source generating the system clock in VLPR mode.<p/>
                                                          The clock dividers cannot be changed while in VLPR mode. 
                                                          They must be programmed prior to entering VLPR mode to guarantee <p/>
                                                          - the core/system and bus clocks are less than or equal to 4 MHz <p/>
                                                          - the flash memory clock is less than or equal to 1 MHz. <p/>
                                                          Note: Implementation specific Container.
                                                      </html>
                                                    ]]>
                                                  </a:v>
                                                </a:a>
                                                <a:a name="UUID" value="ECUC:f913f407-eec1-4148-8a27-28b901989d0d"/>

                                                <v:var name="McuPreDivSystemClockFrequency" type="FLOAT">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                VLPR Core clock - Pre Divide System Clock Frequency. <p/>
                                                                This is the frequency for the specific instance of the "McuClockReferencePoint" container. <p/>
                                                                Value calculated for user info. It is given in Hz. <p/>
                                                                Note: Implementation Specific Parameter. <p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="LABEL" value="VLPR Pre Div System Clock Frequency (Hz)"/>
                                                    <a:a name="UUID" value="ECUC:6f2abe83-6510-4364-b55a-79bce1d59088"/>
                                                    <a:da name="DEFAULT" type="XPath">
                                                        <a:tst expr="num:i( num:i( number( node:fallback(../McuSystemClockSwitch,'SIRC')='SIRC')) *
                                                                            num:i( node:fallback(../../McuSIRCClockConfig/McuSIRCFrequency, num:i(8000000)))
                                                                          )"/>
                                                    </a:da>
                                                    <a:a name="RANGE" type="XPath">
                                                        <a:tst expr="( ( not( node:empty( node:fallback(., 8000000) ) )
                                                                         and
                                                                         ( node:fallback(., 8000000) !=
                                                                           ( num:i( num:i( number( node:fallback(../McuSystemClockSwitch,'SIRC')='SIRC')) *
                                                                                    num:i( node:fallback(../../McuSIRCClockConfig/McuSIRCFrequency, num:i(8000000)))
                                                                                  )
                                                                           )
                                                                         )
                                                                       )
                                                                     )
                                                                     or
                                                                     ( node:fallback(., num:i(8000000)) &gt; num:i(8000000) )
                                                                     or
                                                                     ( node:fallback(., num:i(8000000)) &lt; num:i(1000) ) "
                                                        true="Please press the button to calculate the value automatically."/>
                                                    </a:a>
                                                    <a:a name="INVALID" type="Range">
                                                        <a:tst expr="&lt;=8000000"/>
                                                        <a:tst expr="&gt;=1000"/>
                                                    </a:a>
                                                </v:var>

                                                <v:var name="McuCoreClockFrequency" type="FLOAT">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                VLPR Core clock - Clocks the ARM core, divided by DIVCORE bits inside SCG <p/>
                                                                This is the frequency for the specific instance of the "McuClockReferencePoint" container.
                                                                Value calculated for user info. It is given in Hz. <p/>
                                                                Note: Implementation Specific Parameter. <p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="LABEL" value="VLPR Core Clock Frequency (Hz)"/>
                                                    <a:a name="UUID" value="ECUC:821e8f15-a576-4719-8725-98d66118fcb2"/>
                                                    <a:da name="DEFAULT" type="XPath">
                                                        <a:tst expr="num:i( num:i( number( node:fallback(../McuSystemClockSwitch,'SIRC')='SIRC')) *
                                                                            num:i( node:fallback(../../McuSIRCClockConfig/McuSIRCFrequency, num:i(8000000)))
                                                                          ) div num:i(node:fallback(../McuCoreClockDivider,num:i(2)))"/>
                                                    </a:da>
                                                    <a:a name="RANGE" type="XPath">
                                                        <a:tst expr="( ( not( node:empty( node:fallback(., 4000000) ) )
                                                                         and
                                                                         ( node:fallback(., 4000000) !=
                                                                           ( num:i( num:i( number( node:fallback(../McuSystemClockSwitch,'SIRC')='SIRC')) *
                                                                                    num:i( node:fallback(../../McuSIRCClockConfig/McuSIRCFrequency, num:i(8000000)))
                                                                                  ) div num:i(node:fallback(../McuCoreClockDivider,num:i(2)))
                                                                           )
                                                                         )
                                                                       )
                                                                     )
                                                                     or
                                                                     ( node:fallback(., num:i(4000000)) &gt; num:i(4000000) )
                                                                     or
                                                                     ( node:fallback(., num:i(4000000)) &lt; num:i(1000) ) "
                                                        true="Please press the button to calculate the value automatically."/>
                                                    </a:a>
                                                    <a:da name="INVALID" type="Range">
                                                        <a:tst expr="&lt;=4000000"/>
                                                        <a:tst expr="&gt;=1000"/>
                                                    </a:da>
                                                </v:var>

                                                <v:var name="McuSystemClockFrequency" type="FLOAT">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                VLPR System clock - Clocks the Crossbar, NVIC, Flash controller, FTM and PDB,etc. <p/>
                                                                VLPR_SYS_CLK can run up to CORE_CLK. <p/>
                                                                This is the frequency for the specific instance of the "McuClockReferencePoint" container.
                                                                Value calculated for user info. It is given in Hz. <p/>
                                                                Note: Implementation Specific Parameter.
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="LABEL" value="Run System Clock Frequency (Hz)"/>
                                                    <a:a name="UUID" value="ECUC:d8527f5a-2214-402d-a4e3-1b47ea533934"/>
                                                    <a:da name="DEFAULT" type="XPath">
                                                        <a:tst expr="num:i(node:fallback(../McuCoreClockFrequency, num:i(4000000)))"/>
                                                    </a:da>
                                                    <a:a name="RANGE" type="XPath">
                                                        <a:tst expr="( ( not( node:empty( node:fallback(., 4000000) ) )
                                                                         and
                                                                         ( node:fallback(., 4000000) != 
                                                                              num:i(node:fallback(../McuCoreClockFrequency, num:i(4000000)))
                                                                         )
                                                                       )
                                                                     )
                                                                     or
                                                                     ( node:fallback(., num:i(4000000)) &gt; num:i(4000000) )
                                                                     or
                                                                     ( node:fallback(., num:i(4000000)) &lt; num:i(1000) ) "
                                                        true="Please press the button to calculate the value automatically."/>
                                                    </a:a>
                                                    <a:da name="INVALID" type="Range">
                                                        <a:tst expr="&lt;=4000000"/>
                                                        <a:tst expr="&gt;=1000"/>
                                                    </a:da>
                                                </v:var>

                                                <v:var name="McuBusClockFrequency" type="FLOAT">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                VLPR Bus clock - BUS_CLK Clocks the Peripherals, divided by DIVBUS bits inside SCG <p/>
                                                                This is the frequency for the specific instance of the "McuClockReferencePoint" container.
                                                                Value calculated for user info. It is given in Hz. <p/>
                                                                Note: Implementation Specific Parameter.
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="LABEL" value="VLPR Bus Clock Frequency (Hz)"/>
                                                    <a:a name="UUID" value="ECUC:0ef36b35-31fd-42f6-955c-f415ac74fc18"/>
                                                    <a:da name="DEFAULT" type="XPath">
                                                        <a:tst expr="num:i(
                                                                            num:i(node:fallback(../McuCoreClockFrequency, num:i(4000000))) div
                                                                            num:i(node:fallback(../McuBusClockDivider,num:i(1)))
                                                                          )"/>
                                                    </a:da>
                                                    <a:a name="RANGE" type="XPath">
                                                        <a:tst expr="( ( not( node:empty( node:fallback(., 4000000) ) )
                                                                         and
                                                                         ( node:fallback(., 4000000) != 
                                                                           num:i( num:i(node:fallback(../McuCoreClockFrequency, num:i(40000000))) div
                                                                                  num:i(node:fallback(../McuBusClockDivider,num:i(1)))
                                                                                )
                                                                         )
                                                                       )
                                                                     )
                                                                     or
                                                                     ( node:fallback(., num:i(4000000)) &gt; num:i(4000000) )
                                                                     or
                                                                     ( node:fallback(., num:i(4000000)) &lt; num:i(1000) ) "
                                                        true="Please press the button to calculate the value automatically."/>
                                                    </a:a>
                                                    <a:da name="INVALID" type="Range">
                                                        <a:tst expr="&lt;=4000000"/>
                                                        <a:tst expr="&gt;=1000"/>
                                                    </a:da>
                                                </v:var>

                                                <v:var name="McuFlashClockFrequency" type="FLOAT">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                VLPR Flash clock - Clocks the flash module, divided by DIVSLOW bits inside SCG <p/>
                                                                This is the frequency for the specific instance of the "McuClockReferencePoint" container.
                                                                Value calculated for user info. It is given in Hz. <p/>
                                                                Note: Implementation Specific Parameter.
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="LABEL" value="VLPR Flash Clock Frequency (Hz)"/>
                                                    <a:a name="UUID" value="ECUC:ef08dffc-9791-431a-af69-3f6ff7c3d7cc"/>
                                                    <a:da name="DEFAULT" type="XPath">
                                                        <a:tst expr="num:i(
                                                                            num:i(node:fallback(../McuCoreClockFrequency, num:i(4000000))) div
                                                                            num:i(node:fallback(../McuSlowClockDivider,num:i(4)))
                                                                          )"/>
                                                    </a:da>
                                                    <a:a name="RANGE" type="XPath">
                                                        <a:tst expr="( ( not( node:empty( node:fallback(., 1000000) ) )
                                                                         and
                                                                         ( node:fallback(., 1000000) != 
                                                                           num:i(
                                                                                  num:i(node:fallback(../McuCoreClockFrequency, num:i(40000000))) div
                                                                                  num:i(node:fallback(../McuSlowClockDivider,num:i(4)))
                                                                                )
                                                                         )
                                                                       )
                                                                     )
                                                                     or
                                                                     ( node:fallback(., num:i(1000000)) &gt; num:i(1000000) )
                                                                     or
                                                                     ( node:fallback(., num:i(1000000)) &lt; num:i(1000) ) "
                                                        true="Please press the button to calculate the value automatically."/>
                                                    </a:a>
                                                    <a:da name="INVALID" type="XPath">
                                                        <a:tst expr="(
                                                                        num:i(
                                                                            num:i(node:fallback(../McuCoreClockFrequency, num:i(1000))) 
                                                                            div 
                                                                            num:i(node:fallback(., num:i(1000)))
                                                                        )
                                                                        &gt; num:i(8)
                                                                     )"
                                                               true="The core clock to the flash clock ratio is limited to a max vaule of 8."/>
                                                    </a:da>
                                                    <a:da name="INVALID" type="Range">
                                                        <a:tst expr="&lt;=1000000"/>
                                                        <a:tst expr="&gt;=1000"/>
                                                    </a:da>
                                                </v:var>

                                                <v:var name="McuSystemClockSwitch" type="ENUMERATION">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                VLPR System Clock Select. Configure the SCG_VCCR[SCS] register field.
                                                                The system clock is either:<p/>
                                                                - Slow IRC (SCG_VCCR[SCS]=2)<p/>
                                                                Value extracted from Resource: MCU.VlprSystemClkSource.List<p/>
                                                                Note: Implementation Specific Parameter.
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="UUID" value="ECUC:c9049d60-0337-45eb-9520-6c78f8cc042e"/>
                                                    <a:a name="LABEL" value="VLPR System Clock Select"/>
                                                    <a:da name="DEFAULT" type="XPath" expr="ecu:get('MCU.VlprSystemClkSource.Default')"/>
                                                    <a:da name="RANGE" type="XPath" expr="ecu:list('MCU.VlprSystemClkSource.List')"/>
                                                    <a:da name="INVALID" type="XPath">
                                                        <a:tst expr="(
                                                                        (
                                                                            node:fallback(../../McuSIRCClockConfig/McuSIRCUnderMcuControl, 'true') = 'true' and
                                                                            node:fallback(../../McuSIRCClockConfig/McuSIRCEnable, 'true') = 'false'
                                                                        ) and (node:fallback(., 'FIRC') = 'SIRC')
                                                                     )
                                                                     "
                                                            true="The source clock must be enabled. If not under MCU control the source clock should be enabled prior to setting this configuration. "/>
                                                    </a:da>
                                                </v:var>

                                                <v:var name="McuCoreClockDivider" type="INTEGER">
                                                  <a:a name="DESC">
                                                    <a:v>
                                                      <![CDATA[EN:
                                                        <html>
                                                          <p> Configures the SCG_VCCR[DIVCORE] bitfield
                                                            This parameter represents the core clock divider.
                                                            Note: implementation specific parameter.
                                                          </p>
                                                        </html>
                                                      ]]>
                                                    </a:v>
                                                  </a:a>
                                                  <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                    <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                  </a:a>
                                                  <a:a name="ORIGIN" value="NXP"/>
                                                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                  <a:a name="SCOPE" value="LOCAL"/>
                                                  <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                  <a:a name="UUID" value="ECUC:051e5d7e-7ed1-42f0-8e79-bcd851ae0b7f"/>
                                                  <a:da name="DEFAULT" value="2"/>
                                                  <a:a name="LABEL" value="VLPR Core Clock Divider"/>
                                                  <a:da name="INVALID" type="Range">
                                                    <a:tst expr="&lt;=16"/>
                                                    <a:tst expr="&gt;=1"/>
                                                  </a:da>
                                                </v:var>

                                                <v:var name="McuBusClockDivider" type="INTEGER">
                                                  <a:a name="DESC">
                                                    <a:v>
                                                      <![CDATA[EN:
                                                        <html>
                                                          <p> Configures the SCG_VCCR[DIVBUS] bitfield
                                                            This parameter represents the bus clock divider.
                                                            Note: implementation specific parameter.
                                                          </p>
                                                        </html>
                                                      ]]>
                                                    </a:v>
                                                  </a:a>
                                                  <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                    <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                  </a:a>
                                                  <a:a name="ORIGIN" value="NXP"/>
                                                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                  <a:a name="SCOPE" value="LOCAL"/>
                                                  <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                  <a:a name="UUID" value="ECUC:ed7ddae9-ffc9-4045-b33c-2fe5de5982fc"/>
                                                  <a:a name="LABEL" value="VLPR Bus Clock Divider"/>
                                                  <a:da name="DEFAULT" value="1"/>
                                                  <a:da name="INVALID" type="Range">
                                                    <a:tst expr="&lt;=16"/>
                                                    <a:tst expr="&gt;=1"/>
                                                  </a:da>
                                                </v:var>

                                                <v:var name="McuSlowClockDivider" type="INTEGER">
                                                  <a:a name="DESC">
                                                    <a:v>
                                                      <![CDATA[EN:
                                                        <html>
                                                          <p> Configures the SCG_VCCR[DIVSLOW] bitfield
                                                            This parameter represents the flash clock divider.
                                                            Note: implementation specific parameter.
                                                          </p>
                                                        </html>
                                                      ]]>
                                                    </a:v>
                                                  </a:a>
                                                  <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                    <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                  </a:a>
                                                  <a:a name="ORIGIN" value="NXP"/>
                                                  <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                  <a:a name="SCOPE" value="LOCAL"/>
                                                  <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                  <a:a name="UUID" value="ECUC:5884d12a-62ff-40af-945e-40c380b39680"/>
                                                  <a:a name="LABEL" value="VLPR Slow Clock Divider"/>
                                                  <a:da name="DEFAULT" value="4"/>
                                                  <a:da name="INVALID" type="Range">
                                                    <a:tst expr="&lt;=8"/>
                                                    <a:tst expr="&gt;=1"/>
                                                  </a:da>
                                                </v:var>
                                                
                                                <v:var name="McuScgClkOutFrequency" type="FLOAT">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                This is frequency of SCG clockout. It is given in Hz. <p/>
                                                                Note: Implementation Specific Parameter.
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="LABEL" value="VLPR SCG ClockOut Frequency (Hz)"/>
                                                    <a:a name="UUID" value="ECUC:4c7eb0ee-79b7-456f-abce-284982a183ee"/>
                                                    <a:da name="DEFAULT" type="XPath">
                                                        <a:tst expr="num:i( num:i( number( node:fallback(../../McuScgClkOutSelect,'SCG_SLOW_CLK')='SCG_SLOW_CLK')) *
                                                                                    num:i( node:fallback(../McuFlashClockFrequency, num:i(1000000))) +
                                                                                    num:i( number( node:fallback(../../McuScgClkOutSelect,'SCG_SLOW_CLK')='SIRC')) *
                                                                                    num:i( node:fallback(../../McuSIRCClockConfig/McuSIRCFrequency, num:i(8000000))) +
                                                                                    num:i( number( node:fallback(../../McuScgClkOutSelect,'SCG_SLOW_CLK')='FIRC')) *
                                                                                    num:i( node:fallback(../../McuFIRCClockConfig/McuFIRCFrequency, num:i(48000000))) +
                                                                                    num:i( number( node:fallback(../../McuScgClkOutSelect,'SCG_SLOW_CLK')='SOSC')) *
                                                                                    num:i( node:fallback(../../McuSystemOSCClockConfig/McuSOSCFrequency, num:i(40000000))) +
                                                                                    num:i( number( node:fallback(../../McuScgClkOutSelect,'SCG_SLOW_CLK')='SPLL')) *
                                                                                    num:i( node:fallback(../../McuSystemPll/McuSPLLFrequency, num:i(40000000)))
                                                                                  )"/>
                                                    </a:da>
                                                    <a:a name="RANGE" type="XPath">
                                                        <a:tst expr="( ( not( node:empty( node:fallback(., 100000000) ) )
                                                                         and
                                                                         ( node:fallback(., 100000000) !=
                                                                           ( num:i( num:i( number( node:fallback(../../McuScgClkOutSelect,'SCG_SLOW_CLK')='SCG_SLOW_CLK')) *
                                                                                    num:i( node:fallback(../McuFlashClockFrequency, num:i(1000000))) +
                                                                                    num:i( number( node:fallback(../../McuScgClkOutSelect,'SCG_SLOW_CLK')='SIRC')) *
                                                                                    num:i( node:fallback(../../McuSIRCClockConfig/McuSIRCFrequency, num:i(8000000))) +
                                                                                    num:i( number( node:fallback(../../McuScgClkOutSelect,'SCG_SLOW_CLK')='FIRC')) *
                                                                                    num:i( node:fallback(../../McuFIRCClockConfig/McuFIRCFrequency, num:i(48000000))) +
                                                                                    num:i( number( node:fallback(../../McuScgClkOutSelect,'SCG_SLOW_CLK')='SOSC')) *
