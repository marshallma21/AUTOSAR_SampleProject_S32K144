                                                                            node:fallback(../../McuFIRCClockConfig/McuFIRCUnderMcuControl, 'true') = 'true' and
                                                                            node:fallback(../../McuFIRCClockConfig/McuFIRCEnable, 'true') = 'false'
                                                                        ) and (node:fallback(., 'FIRC') = 'FIRC')
                                                                     ) or
                                                                     (
                                                                        (
                                                                            node:fallback(../../McuSIRCClockConfig/McuSIRCUnderMcuControl, 'true') = 'true' and
                                                                            node:fallback(../../McuSIRCClockConfig/McuSIRCEnable, 'true') = 'false'
                                                                        ) and (node:fallback(., 'FIRC') = 'SIRC')
                                                                     ) or
