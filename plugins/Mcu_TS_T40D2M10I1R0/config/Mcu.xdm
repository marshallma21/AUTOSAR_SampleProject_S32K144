                                                <v:var name="McuSystemClockSwitch" type="ENUMERATION">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                HSRUN System Clock Select. Configure the SCG_RCCR[SCS] register field.
                                                                The system clock is either:<p/>
                                                                - System OSC (SCG_RCCR[SCS]=1)<p/>
                                                                - Slow IRC (SCG_RCCR[SCS]=2)<p/>
                                                                - Fast IRC (SCG_RCCR[SCS]=3)<p/>
                                                                - System PLL (SCG_RCCR[SCS]=6)<p/>
                                                                Value extracted from Resource: MCU.HsrunSystemClkSource.List<p/>
                                                                Note: Implementation Specific Parameter.
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
