                                            </v:ctr>                                            

                                            <v:ctr name="McuClkMonitor_0" type="IDENTIFIABLE">
                                                <a:a name="DESC">
                                                    <a:v>
                                                        <![CDATA[EN:<html>
                                                            This container contains the specific configuration (parameters) of the Monitor_0.
                                                            Clock Monitor Unit for Motor Clock.
                                                            This parameter is enabled only if "McuClockSrcFailureNotification" is enabled.<p/>
                                                            Note: Implementation Specific Parameter.
                                                        </html>]]>
                                                    </a:v>
                                                </a:a>
                                                <a:a name="TAB" value="McuClkMonitor_0"/>
                                                <a:a name="UUID" value="ECUC:de776258-ea37-41be-84fa-0ccec30d8eab"/>

                                                <v:var name="McuClkMonitorEn" type="BOOLEAN">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                Enables/Disables the clock monitor (CMU_GCR[FCE]).<p/>
                                                                Note: Implementation Specific Parameter.This bit is available in S32K11x variants only<p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:da name="INVALID" type="XPath">
                                                        <a:tst expr="(((node:fallback(../../../../../McuDebugConfiguration/McuEnablePeripheralCMU,false)) = 'false')
																	  or ((node:fallback(../../McuPeripheralClockConfig/*[McuPerName='CMU0']/McuPeripheralClockEnable,false)) = 'false'))
                                                                      and ((node:fallback(../McuClkMonitorEn, true)) = 'true')"
                                                                true="Clock Monitoring Unit exist  only on S32K11X. Please disable node Frequency Check Enable.If you want enable node Frequency Check Enable Please enable node Mcu Enable Peripheral CMU and node McuPeripheralClockEnable CMU0."
                                                        />
                                                    </a:da>
                                                    <a:da name="EDITABLE" type="XPath">
                                                        <a:tst  expr="(ecu:get('MCU.CMU.UNITS') != '0')"/>
                                                    </a:da>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="Frequency Check Enable"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="UUID" value="ECUC:6f41f714-38a0-4a05-aa4f-91668fff481b"/>
                                                    <a:da name="DEFAULT" value="false"/>
                                                </v:var>

                                                <v:var name="McuFrequencyHigherInterrupt" type="BOOLEAN">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                Enables/Disables FHH asynchronous interrupt at the module boundary (CMU0_IER[FHHAIE]).<p/>
                                                                Note: Implementation Specific Parameter.This bit is available in S32K11x variants only<p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="Frequency Higher Interrupt"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                    <a:a name="UUID" value="ECUC:6f41f714-38a0-4a05-aa4f-91668fff4296"/>
                                                    <a:da name="DEFAULT" value="false"/>
                                                    <a:a name="EDITABLE" type="XPath">
                                                        <a:tst expr="node:fallback(../McuClkMonitorEn, 'true') = 'true'"/>
                                                    </a:a>
                                                </v:var>
                                                
                                                <v:var name="McuFrequencyLowerInterrupt" type="BOOLEAN">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                Enables/Disables FLL asynchronous interrupt at the module boundary (CMU0_IER[FLLAIE]).<p/>
                                                                Note: Implementation Specific Parameter.This bit is available in S32K11x variants only<p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="Frequency Lower Interrupt"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
