                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="LABEL" value="EIM Clock Enable"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="UUID" value="ECUC:e84365e6-94f7-47fa-af36-87a0b5e5e42b"/>
                                                    <a:da name="DEFAULT" value="true"/>
                                                </v:var>

                                                <v:var name="McuERMClockGatingEnable" type="BOOLEAN">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                SIM_PLATCGC[CGCERM] - ERM Clock Gating Control <p/>
                                                                Controls the clock gating to the ERM. <p/>
                                                                0 - Clock disabled. <p/>
                                                                1 - Clock enabled.<p/>
                                                                Note: Implementation Specific Parameter.<p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="LABEL" value="ERM Clock Enable"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="UUID" value="ECUC:e84365e6-94f7-47fa-af36-87a0b5e5e21b"/>
                                                    <a:da name="DEFAULT" value="true"/>
                                                </v:var>

                                                <v:var name="McuDMAClockGatingEnable" type="BOOLEAN">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                SIM_PLATCGC[CGCDMA] - DMA Clock Gating Control <p/>
                                                                Controls the clock gating to the DMA module. <p/>
                                                                0 - Clock disabled. <p/>
                                                                1 - Clock enabled.<p/>
                                                                Note: Implementation Specific Parameter.<p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="DMA Clock Enable"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="UUID" value="ECUC:1182b3c6-8b44-442f-bf66-3f2a69f3c47b"/>
                                                    <a:da name="DEFAULT" value="true"/>
                                                </v:var>
                                                
                                                <v:var name="McuMPUClockGatingEnable" type="BOOLEAN">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                SIM_PLATCGC[CGCMPU] - MPU Clock Gating Control <p/>
                                                                Controls the clock gating to the MPU module. <p/>
                                                                0 - Clock disabled. <p/>
                                                                1 - Clock enabled.<p/>
                                                                Note: Implementation Specific Parameter.<p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="MPU Clock Enable"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="UUID" value="ECUC:6821d0e0-df80-4529-a33c-9db6dbbe52de"/>
                                                    <a:da name="DEFAULT" value="true"/>
                                                </v:var>
                                                
                                                <v:var name="McuMSCMClockGatingEnable" type="BOOLEAN">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                SIM_PLATCGC[CGCMSCM] - MSCM Clock Gating Control <p/>
                                                                Controls the clock gating to the MSCM module. <p/>
                                                                0 - Clock disabled. <p/>
                                                                1 - Clock enabled.<p/>
                                                                Note: Implementation Specific Parameter.<p/>
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <a:a name="ORIGIN" value="NXP"/>
                                                    <a:a name="LABEL" value="MSCM Clock Enable"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="SCOPE" value="LOCAL"/>
                                                    <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                    <a:a name="UUID" value="ECUC:5c7f33b9-9860-4f7c-b23b-7d473bae83d6"/>
                                                    <a:da name="DEFAULT" value="true"/>
                                                </v:var>

                                                <v:var name="McuGPIOClockGatingEnable" type="BOOLEAN">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                SIM_PLATCGC[CGCGPIO] - GPIO Clock Gating Control <p/>
                                                                Controls the clock gating to the GPIO module. <p/>
                                                                0 - Clock disabled. <p/>
                                                                1 - Clock enabled.<p/>
                                                                Note: Implementation Specific Parameter.This bit is available in S32K11x variants only<p/>
                                                            </html>]]>
                                                        </a:v>
