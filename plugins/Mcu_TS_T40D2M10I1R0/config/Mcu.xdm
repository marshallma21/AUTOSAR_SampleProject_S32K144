                                                    <a:a name="LABEL" value="GPIO Clock Enable"/>
                                                    <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                    <a:a name="UUID" value="ECUC:0bc662b0-da55-4131-be8a-f91a3a33a5a1"/>
