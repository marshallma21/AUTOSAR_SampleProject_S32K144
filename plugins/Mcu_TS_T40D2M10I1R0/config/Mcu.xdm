                                            <v:lst name="McuPeripheralClockConfig" type="MAP">
                                                <a:da name="MIN" type="XPath" expr="num:i(count(ecu:list('MCU.PerSource.List')))"/>
                                                <a:da name="MAX" type="XPath" expr="num:i(count(ecu:list('MCU.PerSource.List')))"/>

                                                <v:ctr name="McuPeripheralClockConfig" type="IDENTIFIABLE">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                This contains the combination for current peripheral in Run and LowPower Mode.<p/>
                                                                Note: Implementation Specific Container.
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="TAB" value="McuPeripheralClockConfig"/>
                                                    <a:a name="UUID" value="ECUC:26415612-a119-41ad-973e-e52bc9a719ab"/>
                                                    <v:var name="McuPerName" type="ENUMERATION">
                                                        <a:a name="DESC">
                                                            <a:v>
                                                                <![CDATA[EN:<html>
                                                                    This is the name of the peripheral.<p/>
                                                                    Note: Implementation Specific Parameter.
                                                                </html>]]>
                                                            </a:v>
                                                        </a:a>
                                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                            <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                        </a:a>
                                                        <a:a name="ORIGIN" value="NXP"/>
                                                        <a:a name="LABEL" value="Mcu Peripheral Name"/>
                                                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                        <a:a name="SCOPE" value="LOCAL"/>
                                                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                        <a:a name="UUID" value="ECUC:8cec4151-bfc1-4812-8ed0-1f73d84c7a7a"/>
                                                        <a:da name="DEFAULT" type="XPath" expr="ecu:list('MCU.PerSource.List')[num:i(node:fallback(node:current()/../@index, num:i(0)) + 1)]"/>
                                                        <a:da name="RANGE" type="XPath" expr="ecu:list('MCU.PerSource.List')"/>
                                                        <a:da name="INVALID" type="XPath">
                                                            <a:tst expr="text:uniq(node:fallback(../../*/McuPerName, ecu:list('MCU.PerSource.List')), node:fallback(., ecu:get('MCU.PerSource.Default')))" 
                                                                false="Duplicated value, McuPerName must be unique across McuPeripheral."/>
                                                        </a:da>
                                                    </v:var>

                                                    <v:var name="McuPeripheralClockEnable" type="BOOLEAN">
                                                            <a:a name="DESC">
                                                                <a:v>
                                                                    <![CDATA[EN:<html>
                                                                        Sets PCC_[peripheral][CGC] bit.
                                                                        This read/write bit enables the clock for the peripheral.<p/>
                                                                            0 - Clock disabled. <p/>
                                                                            1 - Clock enabled. <p/>
                                                                        Note: Implementation Specific Parameter.
                                                                    </html>]]>
                                                                </a:v>
                                                            </a:a>
                                                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                                <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                                <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                            </a:a>
                                                            <a:a name="LABEL" value="Enable peripheral"/>
                                                            <a:a name="ORIGIN" value="NXP"/>
                                                            <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                            <a:a name="SCOPE" value="LOCAL"/>
                                                            <a:a name="POSTBUILDVARIANTVALUE" value="true"/> 
                                                            <a:a name="UUID" value="ECUC:513e4144-5661-48c5-b9ce-b539108bdd5f"/>
                                                            <a:da name="DEFAULT" value="true"/>
                                                    </v:var>
                                                
                                                    <v:var name="McuPeripheralClockSelect" type="ENUMERATION">
                                                        <a:a name="DESC">
                                                            <a:v>
                                                                <![CDATA[EN:<html>
                                                                    Configures PCC_[peripheral][PCS]. <p/>
                                                                    This is used for peripherals that support various clock selections.<p/>
                                                                    If the peripheral does not support various clock selections the field won't be editable.<p/>
                                                                    This field can only be written when the CGC bit is 0 (clock disabled). Likewise, if the INUSE flag is set, this field is locked. <p/>
                                                                    0 - Clock is off (or external clock as selected by FTMnCLKSEL for FTM modules). <p/>
                                                                    1 - SOSCDIV2_CLK (SOCDIV1_CLK for FTM modules) <p/>
                                                                    2 - SIRCDIV2_CLK (SIRCDIV1_CLK for FTM modules) <p/>
                                                                    3 - FIRCDIV2_CLK (FIRCDIV1_CLK for FTM modules) <p/>
                                                                    6 - SPLLDIV2_CLK (SPLLDIV1_CLK for FTM modules) <p/>
                                                                    Note: Please make sure that the divider of clock source is not 0 .<p/>
                                                                    Note: Implementation Specific Parameter.<p/>
                                                                    If the field is not editable, it means the bit field is read only and the value will not be used.
                                                                </html>]]>
                                                            </a:v>
                                                        </a:a>
                                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                            <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                        </a:a>
                                                        <a:a name="ORIGIN" value="NXP"/>
                                                        <a:a name="LABEL" value="Peripheral clock selection"/>
                                                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                        <a:a name="SCOPE" value="LOCAL"/>
                                                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                        <a:a name="UUID" value="ECUC:87380375-99cc-4ebd-976c-6e7133a096c4"/>
                                                        <a:da name="DEFAULT" value="EXT_CLK_OR_CLK_OFF"/>
                                                        <a:da name="RANGE">
                                                            <a:v>EXT_CLK_OR_CLK_OFF</a:v>
                                                            <a:v>SOSC</a:v>
                                                            <a:v>SIRC</a:v>
                                                            <a:v>FIRC</a:v>
                                                            <a:v>SPLL</a:v>
                                                        </a:da>
                                                        <a:a name="EDITABLE" type="XPath">
                                                            <a:tst expr="contains(ecu:list('MCU.PerSourceWithPcsFromPlat.List'),../McuPerName)
                                                                             or contains(ecu:list('MCU.PerSourceWithPcsFromSlow.List'),../McuPerName)
                                                                        "/>                                                            
                                                        </a:a>
                                                        <a:da name="INVALID" type="XPath">
                                                            <a:tst expr="(
                                                                            (
                                                                                node:fallback(../../../McuFIRCClockConfig/McuFIRCUnderMcuControl, 'true') = 'true' and
                                                                                node:fallback(../../../McuFIRCClockConfig/McuFIRCEnable, 'true') = 'false'
                                                                            ) and (node:fallback(., 'FIRC') = 'FIRC')
                                                                         ) or
                                                                         (
                                                                            (
                                                                                node:fallback(../../../McuSIRCClockConfig/McuSIRCUnderMcuControl, 'true') = 'true' and
                                                                                node:fallback(../../../McuSIRCClockConfig/McuSIRCEnable, 'true') = 'false'
                                                                            ) and (node:fallback(., 'FIRC') = 'SIRC')
                                                                         ) or
                                                                         (
                                                                            (
                                                                                node:fallback(../../../McuSystemOSCClockConfig/McuSOSCUnderMcuControl, 'true') = 'true' and
                                                                                node:fallback(../../../McuSystemOSCClockConfig/McuSOSCEnable, 'true') = 'false'
                                                                            ) and (node:fallback(., 'FIRC') = 'SOSC')
                                                                         ) or
                                                                         (
                                                                            (
                                                                                node:fallback(../../../McuSystemPll/McuSystemPllUnderMcuControl, 'true') = 'true' and
                                                                                node:fallback(../../McuSystemPll/McuSPLLEnable, 'true') = 'false'
                                                                            ) and (node:fallback(., 'FIRC') = 'SPLL')
                                                                         )
                                                                         "
                                                                true="The source clock must be enabled. If not under MCU control the source clock should be enabled prior to setting this configuration. "/>
                                                        </a:da>
                                                    </v:var>
                                                    
                                                    <v:var name="McuPeripheralClockDivider" type="INTEGER">
                                                        <a:a name="DESC">
                                                            <a:v>
                                                                 <![CDATA[EN:<html>
                                                                    Configures PCC_[peripheral][PCD]. <p/>
                                                                    This is used for peripherals that require a clock divider. At SOC integration, each peripheral is assigned either a divider or not. <p/>
                                                                    This field can only be written when the CGC bit is 0 (clock disabled). Likewise, if the INUSE flag is set, this field is locked. <p/>
                                                                    Allowed values are from 1 to 8. <p/>
                                                                    Note: Implementation Specific Parameter.<p/>
                                                                    If the field is not editable, it means the bit field is read only and the value will not be used.
                                                                </html>]]>
                                                            </a:v>
                                                        </a:a>
                                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                            <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                        </a:a>
                                                        <a:a name="ORIGIN" value="NXP"/>
                                                        <a:a name="LABEL" value="Peripheral Clock Divider"/>
                                                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                        <a:a name="SCOPE" value="LOCAL"/>
                                                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                        <a:a name="UUID" value="ECUC:2ceb6839-8200-42dd-a670-85da4e2c6079"/>
                                                        <a:da name="DEFAULT" value="1"/>
                                                        <a:da name="INVALID" type="Range">
                                                            <a:tst expr="&lt;=8"/>
                                                            <a:tst expr="&gt;=1"/>
                                                        </a:da>
                                                        <a:a name="EDITABLE" type="XPath">
                                                            <a:tst expr="contains(ecu:list('MCU.PerSourceWithPcd.List'),../McuPerName)"/>
                                                        </a:a>
                                                    </v:var>
                                                    
                                                    <v:var name="McuPeripheralFractionalDivider" type="INTEGER">
                                                        <a:a name="DESC">
                                                            <a:v>
                                                                <![CDATA[EN:<html>
                                                                    Configures PCC_[peripheral][FRAC]. <p/>
                                                                    This sets the fraction multiply value for the fractional clock divider used as a clock source. Divider output clock = Divider input clock x [(FRAC+1)/(DIV+1)]. <p/>
                                                                    This field can only be written when the CGC bit is 0 (clock disabled). Likewise, if the INUSE flag is set, this field is locked. <p/>
                                                                    0 - Fractional value is 0. <p/>
                                                                    1 - Fractional value is 1. <p/>
                                                                    Note: Implementation Specific Parameter.<p/>
                                                                    If the field is not editable, it means the bit field is read only and the value will not be used.
                                                                </html>]]>
                                                            </a:v>
                                                        </a:a>
                                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                            <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                        </a:a>
                                                        <a:a name="ORIGIN" value="NXP"/>
                                                        <a:a name="LABEL" value="Peripheral Fractional Divider"/>
                                                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                        <a:a name="SCOPE" value="LOCAL"/>
                                                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                        <a:a name="UUID" value="ECUC:d052c977-4508-47d6-867b-67e996113265"/>
                                                        <a:da name="DEFAULT" value="0"/>
                                                        <a:da name="INVALID" type="Range">
                                                            <a:tst expr="&lt;=1"/>
                                                            <a:tst expr="&gt;=0"/>
                                                        </a:da>
                                                        <a:a name="EDITABLE" type="XPath">
                                                            <a:tst expr="contains(ecu:list('MCU.PerSourceWithPcd.List'),../McuPerName)"/>
                                                        </a:a>
                                                    </v:var>
                                                
                                                    <v:var name="McuPeripheralClockFrequency" type="FLOAT">
                                                        <a:a name="DESC">
                                                            <a:v>
                                                                <![CDATA[EN:<html>
                                                                    Divider output clock = Divider input clock x [(FRAC+1)/(DIV+1)]. <p/>
                                                                    This is only calculated if the clock source is selectable and if the peripheral is enabled. <p/>
                                                                    This is the frequency for the specific instance of the "McuClockReferencePoint" container.
                                                                    Value calculated for user info. It is given in Hz. <p/>
                                                                    Note: Implementation Specific Parameter. <p/>
                                                                </html>]]>
                                                            </a:v>
                                                        </a:a>
                                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                            <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                        </a:a>
                                                        <a:a name="ORIGIN" value="NXP"/>
                                                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                        <a:a name="SCOPE" value="LOCAL"/>
                                                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                        <a:a name="LABEL" value="Peripheral Clock Frequency (Hz)"/>
                                                        <a:a name="UUID" value="ECUC:dea525c6-9d20-43ff-a605-c1bedb4c8616"/>
                                                        <a:da name="DEFAULT" type="XPath">
                                                            <a:tst expr="num:i( 
                                                                                num:i( number( node:fallback(../McuPeripheralClockSelect,'EXT_CLK_OR_CLK_OFF')='EXT_CLK_OR_CLK_OFF')) * num:i(0) +
                                                                                num:i( number( node:fallback(../McuPeripheralClockSelect,'EXT_CLK_OR_CLK_OFF')='SOSC')) *
                                                                                (
                                                                                    num:i( number( contains(ecu:list('MCU.PerSourceWithPcsFromPlat.List'),node:fallback(../McuPerName,'DMA0')))) *
                                                                                    num:i( node:fallback(../../../McuSystemOSCClockConfig/McuSOSCDiv1Frequency, num:i(32000000))) +
                                                                                    num:i( number( contains(ecu:list('MCU.PerSourceWithPcsFromSlow.List'),node:fallback(../McuPerName,'DMA0')))) *
                                                                                    num:i( node:fallback(../../../McuSystemOSCClockConfig/McuSOSCDiv2Frequency, num:i(32000000)))
                                                                                )+
                                                                                num:i( number( node:fallback(../McuPeripheralClockSelect,'EXT_CLK_OR_CLK_OFF')='SIRC')) *
                                                                                (
                                                                                    num:i( number( contains(ecu:list('MCU.PerSourceWithPcsFromPlat.List'),node:fallback(../McuPerName,'DMA0')))) *
                                                                                    num:i( node:fallback(../../../McuSIRCClockConfig/McuSIRCDiv1Frequency, num:i(48000000))) +
                                                                                    num:i( number( contains(ecu:list('MCU.PerSourceWithPcsFromSlow.List'),node:fallback(../McuPerName,'DMA0')))) *
                                                                                    num:i( node:fallback(../../../McuSIRCClockConfig/McuSIRCDiv2Frequency, num:i(48000000)))
                                                                                )+
                                                                                num:i( number( node:fallback(../McuPeripheralClockSelect,'EXT_CLK_OR_CLK_OFF')='FIRC')) *
                                                                                (
                                                                                    num:i( number( contains(ecu:list('MCU.PerSourceWithPcsFromPlat.List'),node:fallback(../McuPerName,'DMA0')))) *
                                                                                    num:i( node:fallback(../../../McuFIRCClockConfig/McuFIRCDiv1Frequency, num:i(48000000))) +
                                                                                    num:i( number( contains(ecu:list('MCU.PerSourceWithPcsFromSlow.List'),node:fallback(../McuPerName,'DMA0')))) *
                                                                                    num:i( node:fallback(../../../McuFIRCClockConfig/McuFIRCDiv2Frequency, num:i(48000000)))
                                                                                )+
                                                                                num:i( number( node:fallback(../McuPeripheralClockSelect,'EXT_CLK_OR_CLK_OFF')='SPLL')) *
                                                                                (
                                                                                    num:i( number( contains(ecu:list('MCU.PerSourceWithPcsFromPlat.List'),node:fallback(../McuPerName,'DMA0')))) *
                                                                                    num:i( node:fallback(../../../McuSystemPll/McuSPLLDiv1Frequency, num:i(40000000))) +
                                                                                    num:i( number( contains(ecu:list('MCU.PerSourceWithPcsFromSlow.List'),node:fallback(../McuPerName,'DMA0')))) *
                                                                                    num:i( node:fallback(../../../McuSystemPll/McuSPLLDiv2Frequency, num:i(40000000)))
                                                                                )
                                                                              ) * 
                                                                              (
                                                                                num:i(number(contains(ecu:list('MCU.PerSourceWithPcd.List'),node:fallback(../McuPerName,'DMA0'))='true')) *
                                                                                ( 
                                                                                  num:i(number(node:fallback(../McuPeripheralFractionalDivider, 'false')) + num:i(1)) div
                                                                                  num:i(node:fallback(../McuPeripheralClockDivider,num:i(1)))
                                                                                ) + num:i(not(number(contains(ecu:list('MCU.PerSourceWithPcd.List'),node:fallback(../McuPerName,'DMA0'))='true')))
                                                                              )"/>
                                                        </a:da>
                                                        
                                                        <a:a name="RANGE" type="XPath">
                                                            <a:tst expr="( ( not( node:empty( node:fallback(., 80000000) ) ) 
                                                                             and (node:fallback(../McuPeripheralClockEnable, 'false') = 'true') 
                                                                             and (contains(ecu:list('MCU.PerSourceWithPcsFromPlat.List'),../McuPerName) or contains(ecu:list('MCU.PerSourceWithPcsFromSlow.List'),../McuPerName))
                                                                             and
                                                                             ( node:fallback(., 80000000) !=
                                                                               ( num:i(
                                                                                        num:i( number( node:fallback(../McuPeripheralClockSelect,'EXT_CLK_OR_CLK_OFF')='EXT_CLK_OR_CLK_OFF')) * num:i(0) +
                                                                                        num:i( number( node:fallback(../McuPeripheralClockSelect,'EXT_CLK_OR_CLK_OFF')='SOSC')) *
                                                                                        (
                                                                                            num:i( number( contains(ecu:list('MCU.PerSourceWithPcsFromPlat.List'),node:fallback(../McuPerName,'DMA0')))) *
                                                                                            num:i( node:fallback(../../../McuSystemOSCClockConfig/McuSOSCDiv1Frequency, num:i(32000000))) +
                                                                                            num:i( number( contains(ecu:list('MCU.PerSourceWithPcsFromSlow.List'),node:fallback(../McuPerName,'DMA0')))) *
                                                                                            num:i( node:fallback(../../../McuSystemOSCClockConfig/McuSOSCDiv2Frequency, num:i(32000000)))
                                                                                        )+
                                                                                        num:i( number( node:fallback(../McuPeripheralClockSelect,'EXT_CLK_OR_CLK_OFF')='SIRC')) *
                                                                                        (
                                                                                            num:i( number( contains(ecu:list('MCU.PerSourceWithPcsFromPlat.List'),node:fallback(../McuPerName,'DMA0')))) *
                                                                                            num:i( node:fallback(../../../McuSIRCClockConfig/McuSIRCDiv1Frequency, num:i(48000000))) +
                                                                                            num:i( number( contains(ecu:list('MCU.PerSourceWithPcsFromSlow.List'),node:fallback(../McuPerName,'DMA0')))) *
                                                                                            num:i( node:fallback(../../../McuSIRCClockConfig/McuSIRCDiv2Frequency, num:i(48000000)))
                                                                                        )+
                                                                                        num:i( number( node:fallback(../McuPeripheralClockSelect,'EXT_CLK_OR_CLK_OFF')='FIRC')) *
                                                                                        (
                                                                                            num:i( number( contains(ecu:list('MCU.PerSourceWithPcsFromPlat.List'),node:fallback(../McuPerName,'DMA0')))) *
                                                                                            num:i( node:fallback(../../../McuFIRCClockConfig/McuFIRCDiv1Frequency, num:i(48000000))) +
                                                                                            num:i( number( contains(ecu:list('MCU.PerSourceWithPcsFromSlow.List'),node:fallback(../McuPerName,'DMA0')))) *
                                                                                            num:i( node:fallback(../../../McuFIRCClockConfig/McuFIRCDiv2Frequency, num:i(48000000)))
                                                                                        )+
                                                                                        num:i( number( node:fallback(../McuPeripheralClockSelect,'EXT_CLK_OR_CLK_OFF')='SPLL')) *
                                                                                        (
                                                                                            num:i( number( contains(ecu:list('MCU.PerSourceWithPcsFromPlat.List'),node:fallback(../McuPerName,'DMA0')))) *
                                                                                            num:i( node:fallback(../../../McuSystemPll/McuSPLLDiv1Frequency, num:i(40000000))) +
                                                                                            num:i( number( contains(ecu:list('MCU.PerSourceWithPcsFromSlow.List'),node:fallback(../McuPerName,'DMA0')))) *
                                                                                            num:i( node:fallback(../../../McuSystemPll/McuSPLLDiv2Frequency, num:i(40000000)))
                                                                                        )
                                                                                       ) *
                                                                                      (
                                                                                        num:i(number(contains(ecu:list('MCU.PerSourceWithPcd.List'),node:fallback(../McuPerName,'DMA0'))='true')) *
                                                                                          ( 
                                                                                            num:i(number(node:fallback(../McuPeripheralFractionalDivider, 'false')) + num:i(1)) div
                                                                                            num:i(node:fallback(../McuPeripheralClockDivider,num:i(1)))
                                                                                          ) + num:i(not(number(contains(ecu:list('MCU.PerSourceWithPcd.List'),node:fallback(../McuPerName,'DMA0'))='true')))
                                                                                      )
                                                                               )
                                                                             )
                                                                           )
                                                                         )
                                                                         or
                                                                         ( node:fallback(., num:i(80000000)) &gt; num:i(80000000) )
                                                                         or
                                                                         ( node:fallback(., num:i(80000000)) &lt; num:i(0) ) "
                                                            true="Please press the button to calculate the value automatically."/>
                                                        </a:a>
                                                        
                                                        <a:da name="INVALID" type="Range">
                                                            <a:tst expr="&lt;=80000000"/>
                                                            <a:tst expr="&gt;=0"/>
                                                        </a:da>
                                                        <a:a name="EDITABLE" type="XPath">
                                                            <a:tst expr="(contains(ecu:list('MCU.PerSourceWithPcsFromPlat.List'),../McuPerName) 
                                                                          or contains(ecu:list('MCU.PerSourceWithPcsFromSlow.List'),../McuPerName)
                                                                         )
                                                                         and (node:fallback(../McuPeripheralClockEnable, 'false') = 'true')
                                                                        "/>
                                                        </a:a>
                                                    </v:var>
                                                
                                                </v:ctr>                                   
                                            
                                            </v:lst>
                                            
                                            <!-- /** @implements McuClockReferencePoint_Object */ -->
                                            <v:lst name="McuClockReferencePoint" type="MAP">
                                                <a:da name="MIN" value="1"/>
                                                <v:ctr name="McuClockReferencePoint" type="IDENTIFIABLE">
                                                    <a:a name="DESC">
                                                        <a:v>
                                                            <![CDATA[EN:<html>
                                                                This container defines a reference point in the Mcu Clock tree. It defines the frequency which then can be used by other modules as an input value. Lower multiplictiy is 1, as even in the simpliest case (only one frequency is used), there is one frequency to be defined.
                                                            </html>]]>
                                                        </a:v>
                                                    </a:a>
                                                    <a:a name="UUID" value="ECUC:0d1e1584-2c4a-4c3f-bbde-5bbe7d4c2b0e"/>
                                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                        <icc:v mclass="PreCompile">VariantPreCompile</icc:v>
                                                        <icc:v mclass="PreCompile">VariantPostBuild</icc:v>
                                                    </a:a>
                                                    <!-- /** @implements McuClockReferencePointFrequency_Object */ -->
                                                    <v:var name="McuClockReferencePointFrequency" type="FLOAT">
                                                        <a:a name="DESC">
                                                            <a:v>
                                                                <![CDATA[EN:<html>
                                                                    This is the frequency for the specific instance of the McuClockReferencePoint container.
                                                                    It shall be given in Hz.
                                                                    Calculated value.
                                                                </html>]]>
                                                            </a:v>
                                                        </a:a>
                                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                                            <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                                        </a:a>
                                                        <a:a name="LABEL" value="Mcu Clock Reference Point Frequency"/>
                                                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                                        <a:a name="SCOPE" value="ECU"/>
                                                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                                        <a:a name="UUID" value="ECUC:a00b43ce-b661-40ed-9d1b-9e7b9e20e049"/>
                                                        <a:da name="DEFAULT" type="XPath">
                                                            <a:tst expr="floor
                                                                            (   (   (num:i(number( node:fallback(../McuClockFrequencySelect, 'RUN_SYS_CLK') = 'FIRC_CLK'))) * 
                                                                                    (num:i(node:fallback(../../../McuFIRCClockConfig/McuFIRCFrequency, 48000000)))
                                                                                ) +
                                                                                (   (num:i(number( node:fallback(../McuClockFrequencySelect, 'RUN_SYS_CLK') = 'FIRCDIV2_CLK'))) * 
                                                                                    (num:i(node:fallback(../../../McuFIRCClockConfig/McuFIRCDiv2Frequency, 48000000)))
                                                                                ) +
                                                                                (   (num:i(number( node:fallback(../McuClockFrequencySelect, 'RUN_SYS_CLK') = 'FIRCDIV1_CLK'))) * 
                                                                                    (num:i(node:fallback(../../../McuFIRCClockConfig/McuFIRCDiv1Frequency, 48000000)))
                                                                                ) +
                                                                                (   (num:i(number( node:fallback(../McuClockFrequencySelect, 'RUN_SYS_CLK') = 'SIRC_CLK'))) * 
                                                                                    (num:i(num:i(node:fallback(../../../McuSIRCClockConfig/McuSIRCFrequency, 8000000))))
                                                                                ) +
                                                                                (   (num:i(number( node:fallback(../McuClockFrequencySelect, 'RUN_SYS_CLK') = 'SIRCDIV2_CLK'))) * 
                                                                                    (num:i(node:fallback(../../../McuSIRCClockConfig/McuSIRCDiv2Frequency, 8000000)))
                                                                                ) +
                                                                                (   (num:i(number( node:fallback(../McuClockFrequencySelect, 'RUN_SYS_CLK') = 'SIRCDIV1_CLK'))) * 
                                                                                    (num:i(node:fallback(../../../McuSIRCClockConfig/McuSIRCDiv1Frequency, 8000000)))
                                                                                ) +
                                                                                (   (num:i(number( node:fallback(../McuClockFrequencySelect, 'RUN_SYS_CLK') = 'SOSC_CLK'))) * 
                                                                                    (num:i(node:fallback(../../../McuSystemOSCClockConfig/McuSOSCFrequency, 32000000)))
                                                                                ) +
                                                                                (   (num:i(number( node:fallback(../McuClockFrequencySelect, 'RUN_SYS_CLK') = 'SOSCDIV2_CLK'))) * 
                                                                                    (num:i(node:fallback(../../../McuSystemOSCClockConfig/McuSOSCDiv2Frequency, 32000000)))
                                                                                ) +
                                                                                (   (num:i(number( node:fallback(../McuClockFrequencySelect, 'RUN_SYS_CLK') = 'SOSCDIV1_CLK'))) * 
                                                                                    (num:i(node:fallback(../../../McuSystemOSCClockConfig/McuSOSCDiv1Frequency, 32000000)))
                                                                                ) +
                                                                                (   (num:i(number( node:fallback(../McuClockFrequencySelect, 'RUN_SYS_CLK') = 'LPO_CLK_128K'))) * 
                                                                                    (num:i(128000))
                                                                                ) +
                                                                                (   (num:i(number( node:fallback(../McuClockFrequencySelect, 'RUN_SYS_CLK') = 'LPO_CLK_32K'))) * 
                                                                                    (num:i(32000))
                                                                                ) +
                                                                                (   (num:i(number( node:fallback(../McuClockFrequencySelect, 'RUN_SYS_CLK') = 'LPO_CLK_1K'))) * 
                                                                                    (num:i(1000))
                                                                                ) +
                                                                                (   (num:i(number( node:fallback(../McuClockFrequencySelect, 'RUN_SYS_CLK') = 'SPLL_CLK'))) * 
                                                                                    (num:i(node:fallback(../../../McuSystemPll/McuSPLLFrequency, 48000000)))
                                                                                ) +
                                                                                (   (num:i(number( node:fallback(../McuClockFrequencySelect, 'RUN_SYS_CLK') = 'SPLLDIV2_CLK'))) * 
                                                                                    (num:i(node:fallback(../../../McuSystemPll/McuSPLLDiv2Frequency, 48000000)))
                                                                                ) +
                                                                                (   (num:i(number( node:fallback(../McuClockFrequencySelect, 'RUN_SYS_CLK') = 'SPLLDIV1_CLK'))) * 
                                                                                    (num:i(node:fallback(../../../McuSystemPll/McuSPLLDiv1Frequency, 48000000)))
                                                                                ) +
                                                                                (   (num:i(number( node:fallback(../McuClockFrequencySelect, 'RUN_SYS_CLK') = 'RUN_PRI_DIV_SYS_CLK'))) * 
                                                                                    (num:i(node:fallback(../../../McuRunClockConfig/McuPreDivSystemClockFrequency, 48000000)))
                                                                                ) +
                                                                                (   (num:i(number( node:fallback(../McuClockFrequencySelect, 'RUN_SYS_CLK') = 'RUN_CORE_CLK'))) * 
                                                                                    (num:i(node:fallback(../../../McuRunClockConfig/McuCoreClockFrequency, 48000000)))
                                                                                ) +
                                                                                (   (num:i(number( node:fallback(../McuClockFrequencySelect, 'RUN_SYS_CLK') = 'RUN_SYS_CLK'))) * 
                                                                                    (num:i(node:fallback(../../../McuRunClockConfig/McuSystemClockFrequency, 48000000)))
                                                                                ) +
                                                                                (   (num:i(number( node:fallback(../McuClockFrequencySelect, 'RUN_SYS_CLK') = 'RUN_BUS_CLK'))) * 
                                                                                    (num:i(node:fallback(../../../McuRunClockConfig/McuBusClockFrequency, 48000000)))
                                                                                ) +
                                                                                (   (num:i(number( node:fallback(../McuClockFrequencySelect, 'RUN_SYS_CLK') = 'RUN_FLASH_CLK'))) * 
                                                                                    (num:i(node:fallback(../../../McuRunClockConfig/McuFlashClockFrequency, 48000000)))
                                                                                ) +
                                                                                (   (num:i(number( node:fallback(../McuClockFrequencySelect, 'RUN_SYS_CLK') = 'HSRUN_PRI_DIV_SYS_CLK'))) * 
                                                                                    (num:i(node:fallback(../../../McuHsrunClockConfig/McuPreDivSystemClockFrequency, 48000000)))
                                                                                ) +
                                                                                (   (num:i(number( node:fallback(../McuClockFrequencySelect, 'RUN_SYS_CLK') = 'HSRUN_CORE_CLK'))) * 
                                                                                    (num:i(node:fallback(../../../McuHsrunClockConfig/McuCoreClockFrequency, 48000000)))
                                                                                ) +
                                                                                (   (num:i(number( node:fallback(../McuClockFrequencySelect, 'RUN_SYS_CLK') = 'HSRUN_SYS_CLK'))) * 
                                                                                    (num:i(node:fallback(../../../McuHsrunClockConfig/McuSystemClockFrequency, 48000000)))
                                                                                ) +
                                                                                (   (num:i(number( node:fallback(../McuClockFrequencySelect, 'RUN_SYS_CLK') = 'HSRUN_BUS_CLK'))) * 
                                                                                    (num:i(node:fallback(../../../McuHsrunClockConfig/McuBusClockFrequency, 48000000)))
                                                                                ) +
                                                                                (   (num:i(number( node:fallback(../McuClockFrequencySelect, 'RUN_SYS_CLK') = 'HSRUN_FLASH_CLK'))) * 
                                                                                    (num:i(node:fallback(../../../McuHsrunClockConfig/McuFlashClockFrequency, 48000000)))
                                                                                ) +
                                                                                (   (num:i(number( node:fallback(../McuClockFrequencySelect, 'RUN_SYS_CLK') = 'VLPR_PRI_DIV_SYS_CLK'))) * 
                                                                                    (num:i(node:fallback(../../../McuVlprClockConfig/McuPreDivSystemClockFrequency, 48000000)))
                                                                                ) +
                                                                                (   (num:i(number( node:fallback(../McuClockFrequencySelect, 'RUN_SYS_CLK') = 'VLPR_CORE_CLK'))) * 
                                                                                    (num:i(node:fallback(../../../McuVlprClockConfig/McuCoreClockFrequency, 48000000)))
                                                                                ) +
                                                                                (   (num:i(number( node:fallback(../McuClockFrequencySelect, 'RUN_SYS_CLK') = 'VLPR_SYS_CLK'))) * 
                                                                                    (num:i(node:fallback(../../../McuVlprClockConfig/McuSystemClockFrequency, 48000000)))
