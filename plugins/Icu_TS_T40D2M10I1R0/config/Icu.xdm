-->
<d:ctr type="AUTOSAR" factory="autosar"
    xmlns:ad="http://www.tresos.de/_projects/DataModel2/08/admindata.xsd"
    xmlns:icc="http://www.tresos.de/_projects/DataModel2/08/implconfigclass.xsd"
    xmlns:mt="http://www.tresos.de/_projects/DataModel2/11/multitest.xsd" >
    <d:lst type="TOP-LEVEL-PACKAGES">
        <d:ctr name="TS_T40D2M10I1R0" type="AR-PACKAGE">
            <a:a name="UUID" value="ECUC:eda8bf5e-1cf8-43ef-8244-a62dad176cc1"/>
            <d:lst type="ELEMENTS">
                <d:chc name="Icu" type="AR-ELEMENT" value="MODULE-DEF">
                    <v:ctr type="MODULE-DEF">
                        <a:a name="RELEASE" value="asc:4.2"/>
                        <a:a name="ADMIN-DATA" type="ADMIN-DATA">
                            <ad:ADMIN-DATA>
                                <ad:DOC-REVISIONS>
                                    <ad:DOC-REVISION>
                                        <ad:REVISION-LABEL>4.2.0</ad:REVISION-LABEL>
                                        <ad:ISSUED-BY>AUTOSAR</ad:ISSUED-BY>
                                        <ad:DATE>2011-11-02</ad:DATE>
                                    </ad:DOC-REVISION>
                                </ad:DOC-REVISIONS>
                            </ad:ADMIN-DATA>
                        </a:a>
                        <a:a name="DESC"
                             value="EN: Configuration of the Icu (Input Capture Unit) module."/>
                        <a:a name="UUID" value="ECUC:c6f698ea-86b1-4f5e-b3a1-9d77ee51563e"/>
                        <a:a name="POSTBUILDVARIANTSUPPORT" value="true"/>

                        <!-- /** @implements IMPLEMENTATION_CONFIG_VARIANT_Object */ -->
                        <v:var name="IMPLEMENTATION_CONFIG_VARIANT" type="ENUMERATION">
                            <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                <icc:v class="PreCompile">VariantPostBuild</icc:v>
                                <icc:v class="PreCompile">VariantPreCompile</icc:v>
                            </a:a>
                            <a:a name="LABEL" value="Config Variant"/>
                            <a:a name="UUID" value="ECUC:e591e02b-0bd4-425a-b3d5-789eeef95252"/>
                            <a:da name="DEFAULT" value="VariantPostBuild"/>
                            <a:da name="RANGE">
                                <a:v>VariantPostBuild</a:v>
                                <a:v>VariantPreCompile</a:v>
                            </a:da>
                        </v:var>

                        <!-- @implements IcuConfigSet_Object -->
                        <v:ctr name="IcuConfigSet" type="IDENTIFIABLE">
                            <a:a name="DESC">
                                <a:v><![CDATA[EN: <html>This container is the base for a multiple configuration set
                                    </html>]]>
                                </a:v>
                            </a:a>
                            <a:a name="UUID" value="ECUC:7540fb6e-3bcb-4add-8a8e-51a01efc9249"/>

                            <!-- @implements IcuMaxChannel_Object -->
                            <v:var name="IcuMaxChannel" type="INTEGER">
                                <a:a name="CALCULATION-FORMULA" value="number of configured icu channels"/>
                                <a:a name="CALCULATION-LANGUAGE" value="INFORMAL"/>
                                <a:a name="DERIVED" value="TRUE"/>
                                <a:a name="DESC">
                                    <a:v><![CDATA[EN: <html>
                                        <p>The value for the IcuMaxChannel must match with the number of IcuChannel configured
                                        For calculating the correct value use the CALC button. </p>
                                        <b>Note</b>: Total number of configured channels should be same across all IcuConfigSets.
                                        </html>]]>
                                    </a:v>
                                </a:a>
                                <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                    <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                    <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                </a:a>
                                <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                <a:a name="SCOPE" value="LOCAL"/>
                                <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                <a:a name="UUID" value="ECUC:6aa337dc-7191-403e-ada1-e857c0a7c087"/>
                                <a:da name="DEFAULT" type="XPath">
                                    <a:tst expr="num:i(num:i(count(node:fallback(../IcuChannel/*,text:split('1 2 3')))))"/>
                                </a:da>
                                <a:da name="RANGE" type="XPath">
                                    <a:tst expr="node:fallback(.,1) =num:i(num:i(count(node:fallback(../IcuChannel/*,text:split('1 2 3')))))" false="The value for the IcuMaxChannel must match with the number of IcuChannel configured. For calculating the correct value use the CALC button"/>
                                </a:da>
                                <a:da name="INVALID" type="Range">
                                    <a:tst expr="&gt;0"/>
                                    <a:tst expr="&lt;=126"/>
                                </a:da>
                            </v:var>

                            <v:lst name="IcuChannel" type="MAP">
                                <a:da name="MIN" value="1"/>

                                <!-- @implements IcuChannel_Object -->
                                <v:ctr name="IcuChannel" type="IDENTIFIABLE">
                                    <a:a name="DESC" value="EN: Configuration of an individual ICU channel."/>
                                    <a:a name="UUID" value="ECUC:51cf0cff-36e4-4ab3-9561-9737e7113566"/>
                                    <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                        <icc:v mclass="PreCompile">VariantPostBuild</icc:v>
                                        <icc:v mclass="PreCompile">VariantPreCompile</icc:v>
                                    </a:a>

                                    <!-- @implements IcuChannelId_Object -->
                                    <v:var name="IcuChannelId" type="INTEGER">
                                        <a:a name="DESC">
                                            <a:v><![CDATA[EN: <html>
                                                    <p>Channel Id of the ICU channel.
                                                    This value will be assigned to the symbolic name derived of the IcuChannel container short name.</p>
                                                    </html>]]>
                                            </a:v>
                                        </a:a>
                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                            <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                            <icc:v vclass="PreCompile">VariantPostBuild</icc:v>
                                        </a:a>
                                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                        <a:a name="SCOPE" value="ECU"/>
                                        <a:a name="SYMBOLICNAMEVALUE" value="true"/>
                                        <a:a name="UUID" value="ECUC:32e318ee-f58e-4592-abfc-aef66e7a78e4"/>
                                        <a:da name="DEFAULT" type="XPath" expr="node:fallback(node:current()/../@index, num:i(1))"/>
                                        <a:da name="RANGE" type="XPath">
                                            <a:tst expr="(node:fallback(.,1) &gt;= 0) and (node:fallback(.,1) &lt; num:i(count(node:fallback(node:current()/../../*, num:i(180)))) )" false="Value out of range: must be in range 0 to N-1 (N is number of configured channels). Use the Calc button to calculate correct default value."/>
                                            <a:tst expr="text:uniq(node:fallback(../../*/IcuChannelId, text:split('1 2 3')), node:fallback(.,1))" false="Duplicated value, IcuChannelId must be unique across all channels. Use the Calc button to calculate correct default value."/>
                                        </a:da>
                                        <a:da name="INVALID" type="Range">
                                            <a:tst expr="&gt;=0"/>
                                            <a:tst expr="&lt;126"/>
                                        </a:da>
                                    </v:var>

                                    <v:var name="IcuHwIP" type="ENUMERATION">
                                        <a:a name="DESC">
                                            <a:v>
                                                <![CDATA[EN: <html>Selects the physical HwChannelId Channel.</html>]]>
                                            </a:v>
                                        </a:a>
                                        <a:a name="ORIGIN" value="NXP"/>
                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                            <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                        </a:a>
                                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                        <a:a name="UUID" value="ECUC:944a8e08-0503-444f-bca8-a9e7cfeed904"/>
                                        <a:da name="DEFAULT" value="FTM"/>
                                        <a:da name="RANGE">
                                            <a:v>FTM</a:v>
                                            <a:v>PORT</a:v>
                                            <a:v>LPIT</a:v>
                                            <a:v>LPTMR</a:v>
                                        </a:da>
                                        <a:da name="INVALID" type="XPath">
                                                <a:tst expr="((node:value(.) = 'FTM') and not(node:exists(../IcuFtmChannelRef)))" true="please enable IcuFtmChannelRef"/>
                                                <a:tst expr="((node:value(.) = 'PORT') and not(node:exists(../IcuPortChannelRef)))" true="please enable IcuPortChannelRef"/>
                                                <a:tst expr="((node:value(.) = 'LPIT') and not(node:exists(../IcuLpitChannelRef)))" true="please enable IcuLpitChannelRef"/>
                                                <a:tst expr="((node:value(.) = 'LPTMR') and not(node:exists(../IcuLptmrChannelRef)))" true="please enable IcuLptmrChannelRef"/>
                                        </a:da>
                                    </v:var>

                                    <v:ref name="IcuFtmChannelRef" type="REFERENCE">
                                        <a:a name="DESC">
                                        <a:v><![CDATA[EN:
                                            <html>
                                            Select the FTM channel on which the functionality of the current ICU channel will be implemented
                                            </html> ]]>
                                        </a:v>
                                        </a:a>
                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                            <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                            <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                        </a:a>
                                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                        <a:a name="ORIGIN" value="NXP"/>
                                        <a:a name="UUID" value="ECUC:9420eca1-7b64-4e68-bf47-933564b3440c"/>
                                        <a:a name="OPTIONAL" value="true" />
                                        <a:da name="EDITABLE" type="XPath" expr="contains(../IcuHwIP,'FTM') " />
                                        <a:da name="INVALID" type="XPath">
                                            <a:tst expr="
                                                node:value(../IcuHwIP) = 'FTM' and not(node:refvalid(.))"
                                                true="Invalid value of the FTM channel. Please select a valid FTM channel from list of already configured FTM channels. If list is empty,
                                                please configure an FTM channel first by browsing in the tabs IcuConfigSet/IcuFtm/IcuFtmChannels"
                                            />
                                                <a:tst expr="node:value(../IcuHwIP) = 'FTM' and not(text:uniq(../../*/IcuFtmChannelRef, .)) and (string(node:value(.)) != '')" true="Duplicate physical channel"/>
                                        </a:da>
                                        <a:da name="REF"
                                        value="ASPathDataOfSchema:/TS_T40D2M10I1R0/Icu/IcuConfigSet/IcuFtm/IcuFtmChannels"/>
                                    </v:ref>

                                    <v:ref name="IcuPortChannelRef" type="REFERENCE">
                                        <a:a name="DESC">
                                        <a:v><![CDATA[EN:
                                            <html>
                                            Select the PORT channel on which the functionality of the current ICU channel will be implemented
                                            </html> ]]>
                                        </a:v>
                                        </a:a>
                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                            <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                            <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                        </a:a>
                                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                        <a:a name="ORIGIN" value="NXP"/>
                                        <a:a name="UUID" value="ECUC:162ab5f7-5fc6-458b-a9b3-c953512d7447"/>
                                        <a:a name="OPTIONAL" value="true"/>
                                        <a:da name="EDITABLE" type="XPath" expr="contains(../IcuHwIP,'PORT') " />
                                        <a:da name="INVALID" type="XPath">
                                            <a:tst expr="
                                                node:value(../IcuHwIP) = 'PORT' and not(node:refvalid(.))"
                                                true="Invalid value of the PORT channel. Please select a valid PORT channel from list of already configured PORT channels. If list is empty,
                                                please configure an PORT channel first by browsing in the tabs IcuConfigSet/IcuPort/IcuPortChannels"
                                            />
                                                <a:tst expr="node:value(../IcuHwIP) = 'PORT' and  not(text:uniq(../../*/IcuPortChannelRef, .)) and (string(node:value(.)) != '')" true="Duplicate physical channel"/>
                                        </a:da>
                                        <a:da name="REF"
                                        value="ASPathDataOfSchema:/TS_T40D2M10I1R0/Icu/IcuConfigSet/IcuPort/IcuPortChannels"/>
                                    </v:ref>

                                    <v:ref name="IcuLpitChannelRef" type="REFERENCE">
                                        <a:a name="DESC">
                                        <a:v><![CDATA[EN:
                                            <html>
                                            Select the LPIT channel on which the functionality of the current ICU channel will be implemented
                                            </html> ]]>
                                        </a:v>
                                        </a:a>
                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                            <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                            <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                        </a:a>
                                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                        <a:a name="ORIGIN" value="NXP"/>
                                        <a:a name="UUID" value="ECUC:1825ae09-7015-48e2-bc15-5dd74b4958ed"/>
                                        <a:a name="OPTIONAL" value="true"/>
                                        <a:da name="EDITABLE" type="XPath" expr="contains(../IcuHwIP,'LPIT') " />
                                        <a:da name="INVALID" type="XPath">
                                            <a:tst expr="
                                                node:value(../IcuHwIP) = 'LPIT' and not(node:refvalid(.))"
                                                true="Invalid value of the LPIT channel. Please select a valid LPIT channel from list of already configured LPIT channels. If list is empty,
                                                please configure an LPIT channel first by browsing in the tabs IcuConfigSet/IcuLpit/IcuLpitChannels"
                                            />
                                                <a:tst expr="node:value(../IcuHwIP) = 'LPIT' and not(text:uniq(../../*/IcuLpitChannelRef, .)) and (string(node:value(.)) != '')" true="Duplicate physical channel"/>
                                        </a:da>
                                        <a:da name="REF"
                                        value="ASPathDataOfSchema:/TS_T40D2M10I1R0/Icu/IcuConfigSet/IcuLpit/IcuLpitChannels"/>
                                    </v:ref>

                                    <v:ref name="IcuLptmrChannelRef" type="REFERENCE">
                                        <a:a name="DESC">
                                        <a:v><![CDATA[EN:
                                            <html>
                                            Select the LPTMR channel on which the functionality of the current ICU channel will be implemented
                                            </html> ]]>
                                        </a:v>
                                        </a:a>
                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                            <icc:v class="PreCompile">VariantPreCompile</icc:v>
                                            <icc:v class="PostBuild">VariantPostBuild</icc:v>
                                        </a:a>
                                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                        <a:a name="ORIGIN" value="NXP"/>
                                        <a:a name="UUID" value="ECUC:1c0e52ae-0807-4e9a-8857-3206b2e795a0"/>
                                        <a:a name="OPTIONAL" value="true"/>
                                        <a:da name="EDITABLE" type="XPath" expr="contains(../IcuHwIP,'LPTMR') " />
                                        <a:da name="INVALID" type="XPath">
                                            <a:tst expr="
                                                node:value(../IcuHwIP) = 'LPTMR' and not(node:refvalid(.))"
                                                true="Invalid value of the LPTMR channel. Please select a valid LPTMR channel from list of already configured LPTMR channels. If list is empty,
                                                please configure an LPTMR channel first by browsing in the tabs IcuConfigSet/IcuLptmr/IcuLptmrChannels"
                                            />
                                                <a:tst expr="node:value(../IcuHwIP) = 'LPTMR' and not(text:uniq(../../*/IcuLptmrChannelRef, .)) and (string(node:value(.)) != '')" true="Duplicate physical channel"/>
                                        </a:da>
                                        <a:da name="REF"
                                        value="ASPathDataOfSchema:/TS_T40D2M10I1R0/Icu/IcuConfigSet/IcuLptmr/IcuLptmrChannels"/>
                                    </v:ref>

                                    <v:var name="IcuDMAChannelEnable" type="BOOLEAN">
                                       <a:a name="DESC">
                                            <a:v>
                                                <![CDATA[EN: <html>
                                                <b>IcuDMAChannelEnable</b> indicates if the corresponding channel will use DMA for measurement
                                                </html>]]>
                                            </a:v>
                                        </a:a>
                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                            <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                        </a:a>
                                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                        <a:a name="ORIGIN" value="NXP"/>
                                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                        <a:a name="UUID" value="ECUC:5f6b6f0d-9dd2-4238-90fd-fe196b0231db"/>
                                        <a:da name="DEFAULT" value="false"/>
                                        <a:da name="EDITABLE" type="XPath">
                                            <a:tst expr="(contains(../IcuHwIP,'FTM')) and (../IcuMeasurementMode= 'ICU_MODE_TIMESTAMP')" true="true" false="false"/>
                                        </a:da>
                                    </v:var>

                                    <v:ref name="IcuDMAChannelRef" type="CHOICE-REFERENCE">
                                        <a:a name="LABEL" value="IcuDMAChannelReference"/>
                                        <a:a name="DESC">
                                            <a:v>
                                                <![CDATA[EN:
                                                <html>
                                                <b>Icu DMA Channel Reference</b><p>
                                                Reference to the DMA Channel configure for the Request</p>
                                                </html>
                                                ]]>
                                            </a:v>
                                        </a:a>
                                        <a:a name="IMPLEMENTATIONCONFIGCLASS"
                                             type="IMPLEMENTATIONCONFIGCLASS">
                                            <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                        </a:a>
                                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                        <a:da name="EDITABLE" type="XPath">
                                            <a:tst expr="(../IcuDMAChannelEnable = 'true')" true="true" false="false"/>
                                        </a:da>
                                        <a:da name="INVALID" type="XPath">
                                            <a:tst expr="text:uniq(../../*/IcuDMAChannelRef, .)" false="Duplicate physical channel"/>
                                            <a:tst expr="(../IcuDMAChannelEnable = 'true') and not(node:refvalid(.))" true="Invalid or empty reference."/>
                                        </a:da>
                                        <a:a name="ORIGIN" value="NXP"/>
                                        <a:a name="UUID" value="ECUC:511ba006-b102-4e66-9dd1-73bb51bc88ab"/>
                                        <a:da name="REF">
                                            <a:v>ASPathDataOfSchema:/TS_T40D2M10I1R0/Mcl/MclConfigSet/DMAChannel</a:v>
                                        </a:da>
                                    </v:ref>

                                    <!-- @implements IcuDefaultStartEdge_Object -->
                                    <v:var name="IcuDefaultStartEdge" type="ENUMERATION">
                                        <a:a name="DESC">
                                            <a:v><![CDATA[EN: <html>
                                                <p>Configures the default-activation-edge which shall be used for this channel</p>
                                                <p>if there was no activation-edge configured by the call of service Icu_SetActivationCondition().</p>
                                                <p>In case the Measurement Mode is &quot;IcuSignalMeasurement&quot; and the properties &quot;DutyCycle&quot; or &quot;Period&quot; are set, the edge configured here is used as Default Period Start Edge.</p>
                                                <p>Implementation Type: Icu_ActivationType</p>
                                                </html>]]>
                                            </a:v>
                                        </a:a>
                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                            <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                        </a:a>
                                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                        <a:a name="SCOPE" value="LOCAL"/>
                                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                        <a:a name="UUID" value="ECUC:819929bd-c038-4d81-bfea-28333cf31fd5"/>
                                        <a:da name="DEFAULT" value="ICU_RISING_EDGE"/>
                                        <a:da name="RANGE">
                                            <a:v>ICU_BOTH_EDGES</a:v>
                                            <a:v>ICU_FALLING_EDGE</a:v>
                                            <a:v>ICU_RISING_EDGE</a:v>
                                        </a:da>
                                        <a:da name="INVALID" type="XPath">
                                            <a:tst expr="((../IcuMeasurementMode = 'ICU_MODE_SIGNAL_MEASUREMENT') and (../IcuSignalMeasurement/IcuSignalMeasurementProperty='ICU_HIGH_TIME') and (.!='ICU_RISING_EDGE') or
                                                          (../IcuMeasurementMode = 'ICU_MODE_SIGNAL_MEASUREMENT') and (../IcuSignalMeasurement/IcuSignalMeasurementProperty='ICU_LOW_TIME') and (.!='ICU_FALLING_EDGE'))"
                                                          true = " Error in configuring the default start edge - for measuring the high time should be ICU_RISING_EDGE and for measuring low time should be ICU_FALLING_EDGE"/>
                                         </a:da>
                                    </v:var>

                                    <!-- @implements IcuMeasurementMode_Object -->
                                    <v:var name="IcuMeasurementMode" type="ENUMERATION">
                                        <a:a name="DESC">
                                            <a:v><![CDATA[EN: <html>
                                                    <p>Configures the measurement mode of this channel.</p>
                                                    <p>User should enable optional parameters with respect to the selected IcuMeasurementMode.</p>
                                                    <p>Implementation Type: Icu_MeasurementModeType</p>
                                                    </html>]]>
                                            </a:v>
                                        </a:a>
                                        <a:a name="IMPLEMENTATIONCONFIGCLASS" type="IMPLEMENTATIONCONFIGCLASS">
                                            <icc:v vclass="PreCompile">VariantPreCompile</icc:v>
                                            <icc:v vclass="PostBuild">VariantPostBuild</icc:v>
                                        </a:a>
                                        <a:a name="POSTBUILDVARIANTVALUE" value="true"/>
                                        <a:a name="ORIGIN" value="AUTOSAR_ECUC"/>
                                        <a:a name="SCOPE" value="LOCAL"/>
                                        <a:a name="SYMBOLICNAMEVALUE" value="false"/>
                                        <a:a name="UUID" value="ECUC:d15ff176-dbe1-42be-99fd-13382c7daf7e"/>
                                        <a:da name="DEFAULT" value="ICU_MODE_SIGNAL_EDGE_DETECT"/>
                                        <a:da name="RANGE">
                                            <a:v>ICU_MODE_EDGE_COUNTER</a:v>
                                            <a:v>ICU_MODE_SIGNAL_EDGE_DETECT</a:v>
                                            <a:v>ICU_MODE_SIGNAL_MEASUREMENT</a:v>
                                            <a:v>ICU_MODE_TIMESTAMP</a:v>
                                        </a:da>
                                        <a:da name="INVALID" type="XPath">
