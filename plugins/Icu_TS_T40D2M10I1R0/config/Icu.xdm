                                        <a:a name="SYMBOLICNAMEVALUE" value="true"/>
                                        <a:a name="UUID" value="ECUC:37972c2b-b403-4eef-bea9-87d0e04cab57"/>
                                        <a:da name="DEFAULT" type="XPath" expr="(ecu:list('Icu.IcuConfigSet.IcuChannel.IcuHwChannel'))[position()-1=node:fallback(node:current()/../@index, num:i(0)+1)]">
                                        </a:da>
                                        <a:da name="RANGE" type="XPath" expr="ecu:list('Icu.IcuConfigSet.IcuChannel.IcuHwChannel')"/>
                                        <a:da name="INVALID" type="XPath">
